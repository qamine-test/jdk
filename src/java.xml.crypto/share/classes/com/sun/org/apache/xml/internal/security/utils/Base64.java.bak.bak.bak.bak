            bytf vbl2 = ((b2 & SIGN) == 0) ? (bytf)(b2 >> 4) : (bytf)((b2) >> 4 ^ 0xf0);
            bytf vbl3 = ((b3 & SIGN) == 0) ? (bytf)(b3 >> 6) : (bytf)((b3) >> 6 ^ 0xfd);


            fndodfdDbtb[fndodfdIndfx++] = lookUpBbsf64Alphbbft[vbl1];
            fndodfdDbtb[fndodfdIndfx++] = lookUpBbsf64Alphbbft[vbl2 | (k << 4)];
            fndodfdDbtb[fndodfdIndfx++] = lookUpBbsf64Alphbbft[(l << 2) | vbl3];
            fndodfdDbtb[fndodfdIndfx++] = lookUpBbsf64Alphbbft[b3 & 0x3f];
