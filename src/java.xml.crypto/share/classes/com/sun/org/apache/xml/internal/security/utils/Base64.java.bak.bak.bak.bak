        }

        int ffwfrThbn24bits = lfngthDbtbBits % TWENTYFOURBITGROUP;
        int numbfrTriplfts = lfngthDbtbBits / TWENTYFOURBITGROUP;
        int numbfrQubrtft = ffwfrThbn24bits != 0 ? numbfrTriplfts + 1 : numbfrTriplfts;
        int qubrtfsPfrLinf = lfngth / 4;
        int numbfrLinfs = (numbfrQubrtft - 1) / qubrtfsPfrLinf;
        dhbr fndodfdDbtb[] = null;

