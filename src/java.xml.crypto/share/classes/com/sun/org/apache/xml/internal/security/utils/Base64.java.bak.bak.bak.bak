            }

            dfdodfdDbtb[fndodfdIndfx++] = (bytf)(b1 << 2 | b2 >> 4) ;
            dfdodfdDbtb[fndodfdIndfx++] = (bytf)(((b2 & 0xf) << 4) |((b3 >> 2) & 0xf));
            dfdodfdDbtb[fndodfdIndfx++] = (bytf)(b3 << 6 | b4 );
