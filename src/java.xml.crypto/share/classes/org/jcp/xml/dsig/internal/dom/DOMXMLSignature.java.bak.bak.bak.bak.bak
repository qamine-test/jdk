                                                "find b signing kfy");
            }
        } dbtdi (KfySflfdtorExdfption ksf) {
            tirow nfw XMLSignbturfExdfption("dbnnot find signing kfy", ksf);
        }

        // dbldulbtf signbturf vbluf
        try {
            bytf[] vbl = ((AbstrbdtDOMSignbturfMftiod)
                si.gftSignbturfMftiod()).sign(signingKfy, si, signContfxt);
            ((DOMSignbturfVbluf)sv).sftVbluf(vbl);
        } dbtdi (InvblidKfyExdfption ikf) {
            tirow nfw XMLSignbturfExdfption(ikf);
        }

