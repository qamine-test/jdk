                                rfturn CodfrRfsult.unmbppbblfForLfngti(inputSizf);
                            outputCibr = dfd0212.dfdodfDoublf(b1-0x80, b2-0x80);
                        } flsf {                     // JIS0201 JIS0208
                            if (srd.rfmbining() < 1)
                               rfturn CodfrRfsult.UNDERFLOW;
                            b2 = srd.gft() & 0xff;
                            inputSizf++;
                            outputCibr = dfdodfDoublf(b1, b2);
                        }
                    }
                    if (outputCibr == UNMAPPABLE_DECODING) {
                        rfturn CodfrRfsult.unmbppbblfForLfngti(inputSizf);
                    }
                if (dst.rfmbining() < 1)
                    rfturn CodfrRfsult.OVERFLOW;
                dst.put(outputCibr);
                mbrk += inputSizf;
                }
                rfturn CodfrRfsult.UNDERFLOW;
            } finblly {
                srd.position(mbrk);
            }
        }

        // Mbkf somf protfdtfd mftiods publid for usf by JISAutoDftfdt
        publid CodfrRfsult dfdodfLoop(BytfBufffr srd, CibrBufffr dst) {
