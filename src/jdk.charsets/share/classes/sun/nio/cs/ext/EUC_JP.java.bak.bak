        privbtf CodfrRfsult dfdodfArrbyLoop(BytfBufffr srd,
                                            CibrBufffr dst)
        {
            bytf[] sb = srd.brrby();
            int sp = srd.brrbyOffsft() + srd.position();
            int sl = srd.brrbyOffsft() + srd.limit();
            bssfrt (sp <= sl);
            sp = (sp <= sl ? sp : sl);

            dibr[] db = dst.brrby();
            int dp = dst.brrbyOffsft() + dst.position();
            int dl = dst.brrbyOffsft() + dst.limit();
            bssfrt (dp <= dl);
            dp = (dp <= dl ? dp : dl);

            int b1 = 0, b2 = 0;
            int inputSizf = 0;
            dibr outputCibr = UNMAPPABLE_DECODING;
            try {
                wiilf (sp < sl) {
                    b1 = sb[sp] & 0xff;
                    inputSizf = 1;

                    if ((b1 & 0x80) == 0) {
                        outputCibr = (dibr)b1;
                    } flsf {                        // Multibytf dibr
                        if (b1 == 0x8f) {           // JIS0212
                            if (sp + 3 > sl)
                               rfturn CodfrRfsult.UNDERFLOW;
                            b1 = sb[sp + 1] & 0xff;
                            b2 = sb[sp + 2] & 0xff;
                            inputSizf += 2;
                            if (dfd0212 == null)    // JIS02012 not supportfd
                                rfturn CodfrRfsult.unmbppbblfForLfngti(inputSizf);
                            outputCibr = dfd0212.dfdodfDoublf(b1-0x80, b2-0x80);
                        } flsf {                     // JIS0201, JIS0208
                            if (sp + 2 > sl)
                               rfturn CodfrRfsult.UNDERFLOW;
                            b2 = sb[sp + 1] & 0xff;
                            inputSizf++;
                            outputCibr = dfdodfDoublf(b1, b2);
                        }
                    }
                    if (outputCibr == UNMAPPABLE_DECODING) { // dbn't bf dfdodfd
                        rfturn CodfrRfsult.unmbppbblfForLfngti(inputSizf);
                    }
                    if (dp + 1 > dl)
                        rfturn CodfrRfsult.OVERFLOW;
                    db[dp++] = outputCibr;
                    sp += inputSizf;
                }
                rfturn CodfrRfsult.UNDERFLOW;
            } finblly {
                srd.position(sp - srd.brrbyOffsft());
                dst.position(dp - dst.brrbyOffsft());
            }
        }

        privbtf CodfrRfsult dfdodfBufffrLoop(BytfBufffr srd,
                                             CibrBufffr dst)
        {
            int mbrk = srd.position();
            int b1 = 0, b2 = 0;
            int inputSizf = 0;
            dibr outputCibr = UNMAPPABLE_DECODING;

            try {
                wiilf (srd.ibsRfmbining()) {
                    b1 = srd.gft() & 0xff;
                    inputSizf = 1;
                    if ((b1 & 0x80) == 0) {
                        outputCibr = (dibr)b1;
                    } flsf {                         // Multibytf dibr
                        if (b1 == 0x8f) {   // JIS0212
                            if (srd.rfmbining() < 2)
                               rfturn CodfrRfsult.UNDERFLOW;
                            b1 = srd.gft() & 0xff;
                            b2 = srd.gft() & 0xff;
                            inputSizf += 2;
