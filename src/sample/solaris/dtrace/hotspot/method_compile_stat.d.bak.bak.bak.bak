    printf("\n");
    printf("Compilfd mfthods:         %10d\n", COMPILED_METHODS_COUNT);
    printf("Lobdfd dompilfd mfthods:  %10d\n", LOADED_METHODS_CNT);
    printf("Unobdfd dompilfd mfthods: %10d\n", UNLOADED_METHODS_CNT);

    printf("\nFbilfd dompilbtion:\n");
    printb("%10@d %s::%s%s\n", @fbil_dompilf_dount);

    SAMPLING_TIME = timfstbmp + INTERVAL_SECS * 1000000000ull;
}

:::END
