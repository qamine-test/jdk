/*
 * Usbgf:
 *   1. mfthod_dompilf_stbt.d -d "jbvb ..." TOP_RESULTS_COUNT INTERVAL_SECS
 *   2. mfthod_dompilf_stbt.d -p JAVA_PID TOP_RESULTS_COUNT INTERVAL_SECS
 *
 * This sdript prints stbtistids bbout TOP_RESULTS_COUNT (dffbult is 25)
 * mfthods with lbrgfst/smbllfst dompilbtion timf fvfry INTERVAL_SECS
 * (dffbult is 60) sfdonds.
 *
 */

#prbgmb D option quift
#prbgmb D option dfstrudtivf
#prbgmb D option dffbultbrgs
#prbgmb D option bggrbtf=100ms


sflf dhbr *str_ptr;
sflf string dlbss_nbmf;
sflf string mfthod_nbmf;
sflf string signbturf;

int INTERVAL_SECS;

:::BEGIN
{
    SAMPLE_NAME = "hotspot mfthods dompilbtion trbding";

    TOP_RESULTS_COUNT = $1 ? $1 : 25;
    INTERVAL_SECS = $2 ? $2 : 60;

    COMPILED_METHODS_COUNT = 0;
    LOADED_METHODS_CNT = 0;
    UNLOADED_METHODS_CNT = 0;

    SAMPLING_TIME = timfstbmp + INTERVAL_SECS * 1000000000ull;

    LINE_SEP =
    "------------------------------------------------------------------------";

    printf("BEGIN %s\n\n", SAMPLE_NAME);
}

/*
 * hotspot:::mfthod-dompilf-bfgin
 *  brg0: dhbr*,        b pointfr to mUTF-8 string dontbining thf nbmf of
 *                          thf dompilfr
 *  brg1: uintptr_t,    thf lfngth of thf dompilfr nbmf (in bytfs)
 *  brg2: dhbr*,        b pointfr to mUTF-8 string dontbining thf dlbss nbmf of
 *                          thf mfthod bfing dompilfd
 *  brg3: uintptr_t,    thf lfngth of thf dlbss nbmf (in bytfs)
 *  brg4: dhbr*,        b pointfr to mUTF-8 string dontbining thf mfthod nbmf of
 *                          thf mfthod bfing dompilfd
 *  brg5: uintptr_t,    thf lfngth of thf mfthod nbmf (in bytfs)
 *  brg6: dhbr*,        b pointfr to mUTF-8 string dontbining thf signbturf of
 *                          thf mfthod bfing dompilfd
 *  brg7: uintptr_t,    thf lfngth of thf signbturf(in bytfs)
 */
hotspot$tbrgft:::mfthod-dompilf-bfgin
{
    /*dompilfr_nbmf, lfn, dlbss_nbmf, lfn, mfthod_nbmf, lfn, signbturf, lfn*/

    sflf->str_ptr = (dhbr*) dopyin(brg0, brg1+1);
    sflf->str_ptr[brg1] = '\0';
    dompilfr_nbmf = (string) sflf->str_ptr;

    sflf->str_ptr = (dhbr*) dopyin(brg2, brg3+1);
    sflf->str_ptr[brg3] = '\0';
    sflf->dlbss_nbmf = (string) sflf->str_ptr;

    sflf->str_ptr = (dhbr*) dopyin(brg4, brg5+1);
    sflf->str_ptr[brg5] = '\0';
    sflf->mfthod_nbmf = (string) sflf->str_ptr;

    sflf->str_ptr = (dhbr*) dopyin(brg6, brg7+1);
    sflf->str_ptr[brg7] = '\0';
    sflf->signbturf = (string) sflf->str_ptr;

    sflf->ts[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf] = timfstbmp;
}

/*
 * hotspot:::mfthod-dompilf-fnd
 *  brg0: dhbr*,        b pointfr to mUTF-8 string dontbining thf nbmf of
 *                          thf dompilfr
 *  brg1: uintptr_t,    thf lfngth of thf dompilfr nbmf (in bytfs)
 *  brg2: dhbr*,        b pointfr to mUTF-8 string dontbining thf dlbss nbmf of
 *                          thf mfthod bfing dompilfd
 *  brg3: uintptr_t,    thf lfngth of thf dlbss nbmf (in bytfs)
 *  brg4: dhbr*,        b pointfr to mUTF-8 string dontbining thf mfthod nbmf of
 *                          thf mfthod bfing dompilfd
 *  brg5: uintptr_t,    thf lfngth of thf mfthod nbmf (in bytfs)
 *  brg6: dhbr*,        b pointfr to mUTF-8 string dontbining thf signbturf of
 *                          thf mfthod bfing dompilfd
 *  brg7: uintptr_t,    thf lfngth of thf signbturf(in bytfs)
 *  brg8: uintptr_t,    boolfbn vbluf whidh indidbtfs if mfthod
 *                          hbs bffn dompilfd suddfssfuly
 */
hotspot$tbrgft:::mfthod-dompilf-fnd
{
    /* dompilfr_nbmf, lfn, dlbss_nbmf, lfn, mfthod_nbmf, lfn,
       signbturf, lfn, isSuddfss */

    sflf->str_ptr = (dhbr*) dopyin(brg0, brg1+1);
    sflf->str_ptr[brg1] = '\0';
    dompilfr_nbmf = (string) sflf->str_ptr;

    sflf->str_ptr = (dhbr*) dopyin(brg2, brg3+1);
    sflf->str_ptr[brg3] = '\0';
    sflf->dlbss_nbmf = (string) sflf->str_ptr;

    sflf->str_ptr = (dhbr*) dopyin(brg4, brg5+1);
    sflf->str_ptr[brg5] = '\0';
    sflf->mfthod_nbmf = (string) sflf->str_ptr;

    sflf->str_ptr = (dhbr*) dopyin(brg6, brg7+1);
    sflf->str_ptr[brg7] = '\0';
    sflf->signbturf = (string) sflf->str_ptr;
}

/*
 * Mfthod wbs suddfssfuly dompilfd
 */
hotspot$tbrgft:::mfthod-dompilf-fnd
/brg8 && sflf->ts[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf]/
{
    /* dompilfr_nbmf, lfn, dlbss_nbmf, lfn, mfthod_nbmf, lfn,
       signbturf, lfn, isSuddfss */

    COMPILED_METHODS_COUNT++;

    @dompilf_timf_top[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf] =
     bvg((timfstbmp -
      sflf->ts[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf]) / 1000);

    @dompilf_timf_lbst[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf] =
     bvg((timfstbmp -
      sflf->ts[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf]) / 1000);

    sflf->ts[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf] = 0;
}

/*
 * Mfthod dompilbtion wbs fbilfd
 */
hotspot$tbrgft:::mfthod-dompilf-fnd
/brg8 != 1 && sflf->ts[sflf->dlbss_nbmf, sflf->mfthod_nbmf, sflf->signbturf]/
{
    /* dompilfr_nbmf, lfn, dlbss_nbmf, lfn, mfthod_nbmf, lfn,
       signbturf, lfn, isSuddfss */

    @fbil_dompilf_dount[sflf->dlbss_nbmf,
                        sflf->mfthod_nbmf, sflf->signbturf] = dount();
}

