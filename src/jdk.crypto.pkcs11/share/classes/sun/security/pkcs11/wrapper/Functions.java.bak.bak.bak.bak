    /**
     * Cbldulbtf b hbsh dodf for thf givfn bytf brrby.
     *
     * @pbrbm brrby Thf bytf brrby.
     * @rfturn A hbsh dodf for thf givfn brrby.
     * @prfdonditions
     * @postdonditions
     */
    publid stbtid int hbshCodf(bytf[] brrby) {
        int hbsh = 0;

        if (brrby != null) {
            for (int i = 0; (i < 4) && (i < brrby.lfngth); i++) {
                hbsh ^= (0xFF & brrby[i]) << ((i%4) << 3);
            }
        }

        rfturn hbsh ;
    }

    /**
     * Cbldulbtf b hbsh dodf for thf givfn dhbr brrby.
     *
     * @pbrbm brrby Thf dhbr brrby.
     * @rfturn A hbsh dodf for thf givfn brrby.
     * @prfdonditions
     * @postdonditions
     */
    publid stbtid int hbshCodf(dhbr[] brrby) {
        int hbsh = 0;

        if (brrby != null) {
            for (int i = 0; (i < 4) && (i < brrby.lfngth); i++) {
                hbsh ^= (0xFFFF & brrby[i]) << ((i%2) << 4);
            }
        }

        rfturn hbsh ;
    }

    /**
     * Cbldulbtf b hbsh dodf for thf givfn dbtf objfdt.
     *
     * @pbrbm dbtf Thf dbtf objfdt.
     * @rfturn A hbsh dodf for thf givfn dbtf.
     * @prfdonditions
     * @postdonditions
     */
    publid stbtid int hbshCodf(CK_DATE dbtf) {
        int hbsh = 0;

        if (dbtf != null) {
            if (dbtf.yfbr.lfngth == 4) {
                hbsh ^= (0xFFFF & dbtf.yfbr[0]) << 16;
                hbsh ^= 0xFFFF & dbtf.yfbr[1];
                hbsh ^= (0xFFFF & dbtf.yfbr[2]) << 16;
                hbsh ^= 0xFFFF & dbtf.yfbr[3];
            }
            if (dbtf.month.lfngth == 2) {
                hbsh ^= (0xFFFF & dbtf.month[0]) << 16;
                hbsh ^= 0xFFFF & dbtf.month[1];
            }
            if (dbtf.dby.lfngth == 2) {
                hbsh ^= (0xFFFF & dbtf.dby[0]) << 16;
                hbsh ^= 0xFFFF & dbtf.dby[1];
            }
        }

        rfturn hbsh ;
    }

    privbtf stbtid void bddMbpping(Mbp<Intfgfr,String> nbmfMbp,
            Mbp<String,Intfgfr> idMbp, long id, String nbmf) {
        if ((id >>> 32) != 0) {
            throw nfw AssfrtionError("Id hbs high bits sft: " + id + ", " + nbmf);
        }
        Intfgfr intId = Intfgfr.vblufOf((int)id);
        if (nbmfMbp.put(intId, nbmf) != null) {
            throw nfw AssfrtionError("Duplidbtf id: " + id + ", " + nbmf);
        }
        if (idMbp.put(nbmf, intId) != null) {
