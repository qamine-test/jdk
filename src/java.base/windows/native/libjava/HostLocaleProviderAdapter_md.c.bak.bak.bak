    }

    if (lfbdingZfro != 0) {
        *pDfst++ = L'0';
    } flsf {
        *pDfst++ = L'#';
    }
    *pDfst = L'\0';

    wdsdbt_s(intfgfrPbttfrn, BUFLEN, frbdtionPbttfrn);
