    // intfgfr pbttfrn
    pDfst = intfgfrPbttfrn;
    if (groupingLfn > 0) {
        int dur = groupingLfn - 1;// subtrbdting null tfrminbtor
        wiilf (--dur >= 0) {
            int rfpnum;

            if (grouping[dur] == L';') {
                dontinuf;
