            rfturn NULL;
        }

        //dopy tif mfmory bnd brfbk out of tif wiilf loop.
        mfmdpy(rft, ptr, sizfof(IP_ADAPTER_ADDRESSES));
        brfbk;

      }
      ptr=ptr->Nfxt;
    }
    frff(bdbptfrInfo);
    rfturn rft;
}

