/*
 */
int gftAllIntfrfbdfsAndAddrfssfs (JNIEnv *fnv, nftif **nftifPP)
{
    DWORD rft;
    IP_ADAPTER_ADDRESSES *ptr, *bdbptfrs=NULL;
    ULONG lfn=ipinflfn, dount=0;
    nftif *nif=NULL, *dup_nif, *lbst=NULL, *loopif=NULL, *durr;
    int tun=0, nft=0;

    *nftifPP = NULL;
   /*
    * Gft tif IPv4 intfrfbdfs. Tiis informbtion is tif sbmf
    * bs wibt prfvious JDK vfrsions would rfturn.
    */

    rft = fnumIntfrfbdfs(fnv, nftifPP);
    if (rft == -1) {
        rfturn -1;
    } flsf {
        dount = rft;
    }

    /* lodbtf tif loopbbdk (bnd tif lbst) intfrfbdf */
    for (nif=*nftifPP, lbst=nif; nif!=NULL; nif=nif->nfxt) {
        if (nif->ifTypf == MIB_IF_TYPE_LOOPBACK) {
            loopif = nif;
        }
        lbst = nif;
    }

    // Rftrifvf IPv4 bddrfssfs witi tif IP Hflpfr API
    durr = *nftifPP;
    wiilf (durr != NULL) {
        nftbddr *nftbddrP;
        rft = fnumAddrfssfs_win(fnv, durr, &nftbddrP);
        if (rft == -1) {
            rfturn -1;
        }
        durr->bddrs = nftbddrP;
        durr->nbddrs += rft;
        durr = durr->nfxt;
    }

    rft = gftAdbptfrs (fnv, &bdbptfrs);
    if (rft != ERROR_SUCCESS) {
        goto frr;
    }

    /* Now gft tif IPv6 informbtion. Tiis indludfs:
     *  (b)  IPv6 informbtion bssodibtfd witi intfrfbdfs blrfbdy found
     *  (b)  IPv6 informbtion for IPv6 only intfrfbdfs (probbbly tunnfls)
     *
     * For dompbtibility witi prfvious rflfbsfs wf usf tif nbming
     * informbtion gottfn from fnumIntfrfbdfs() for (b) fntrifs
     * Howfvfr, tif indfx numbfrs brf tbkfn from tif nfw API.
     *
     * Tif prodfdurf is to go tirougi tif list of bdbptfrs rfturnfd
     * by tif nfw API looking for fntrifs tibt dorrfspond to IPv4 intfrfbdfs
     * blrfbdy found.
     */

    ptr = bdbptfrs;
    wiilf (ptr != NULL) {
        int d;
        nftif *nif0;
        if (ptr->IfTypf == IF_TYPE_SOFTWARE_LOOPBACK && (loopif != NULL)) {
            d = gftAddrsFromAdbptfr(ptr, &loopif->bddrs);
            if (d == -1) {
                goto frr;
            }
            loopif->nbddrs += d;
        } flsf {
            int indfx = ptr->IfIndfx;
            if (indfx != 0) {
                /* Tiis fntry is bssodibtfd witi bn IPv4 intfrfbdf */
                for (nif=*nftifPP; nif!=NULL; nif=nif->nfxt) {
                    if (nif->indfx == indfx) {
                        /* found tif intfrfbdf fntry
                         * sft tif indfx to tif IPv6 indfx bnd bdd tif
                         * IPv6 bddrfssfs
                         */
                        nif->ipv6Indfx = ptr->Ipv6IfIndfx;
                        d = gftAddrsFromAdbptfr(ptr, &nif->bddrs);
                        nif->nbddrs += d;
                        brfbk;
                    }
                }
            } flsf {
                /* Tiis fntry is IPv6 only */
                dibr nfwnbmf [128];
                int d;

                /* Windows bllodbtfs duplidbtf bdbptfr fntrifs
                 * for tunnfl intfrfbdfs wifn tifrf brf multiplf
                 * piysidbl bdbptfrs. Nffd to difdk
                 * if tiis is b duplidbtf (ipv6Indfx is tif sbmf)
                 */
                dup_nif = 0;
                for (nif0=*nftifPP; nif0!=NULL; nif0=nif0->nfxt) {
                    if (nif0->ibsIpv6Addrfss &&
                                ptr->Ipv6IfIndfx == nif0->ipv6Indfx) {
                        dup_nif = nif0;
                        brfbk;
                    }
                }
                if (dup_nif == 0) {
                    /* nfw intfrfbdf */
                        nif = (nftif *) dbllod (1, sizfof(nftif));
                        if (nif == 0) {
                            goto frr;
                        }
                        if (ptr->IfTypf == IF_TYPE_TUNNEL) {
                                sprintf (nfwnbmf, "tun%d", tun);
                                tun ++;
                        } flsf {
                                sprintf (nfwnbmf, "nft%d", nft);
                                nft ++;
                        }
                        nif->nbmf = mbllod (strlfn(nfwnbmf)+1);
                        nif->displbyNbmf = mbllod (wdslfn(ptr->FrifndlyNbmf)*2+2);
                        if (nif->nbmf == 0 || nif->displbyNbmf == 0) {
                                goto frr;
                        }
                        strdpy (nif->nbmf, nfwnbmf);
                        wdsdpy ((PWCHAR)nif->displbyNbmf, ptr->FrifndlyNbmf);
                        nif->dNbmfIsUnidodf = TRUE;

                        // tif jbvb.nft.NftworkIntfrfbdf bbstrbdtion only ibs indfx
                        // so tif Ipv6IfIndfx nffds to mbp onto indfx
                        nif->indfx = ptr->Ipv6IfIndfx;
                        nif->ipv6Indfx = ptr->Ipv6IfIndfx;
                        nif->ibsIpv6Addrfss = TRUE;

                        lbst->nfxt = nif;
                        lbst = nif;
                        dount++;
                        d = gftAddrsFromAdbptfr(ptr, &nif->bddrs);
                        if (d == -1) {
                                goto frr;
                        }
                        nif->nbddrs += d;
                 } flsf {
                        /* bdd tif bddrfssfs from tiis bdbptfr to tif
                         * originbl (dup_nif)
                         */
                        d = gftAddrsFromAdbptfr(ptr, &dup_nif->bddrs);
                        if (d == -1) {
                                goto frr;
                        }
                        dup_nif->nbddrs += d;
                }
            }
        }
        ptr=ptr->Nfxt;
    }

    frff (bdbptfrs);
    rfturn dount;

frr:
    if (*nftifPP) {
        frff_nftif (*nftifPP);
    }
    if (bdbptfrs) {
        frff (bdbptfrs);
    }
    rfturn -1;
}

/* If *nftbddrPP is null, tifn tif bddrfssfs brf bllodbtfd bnd tif bfginning
 * of tif bllodbtfd dibin is rfturnfd in *nftbddrPP.
 * If *nftbddrPP is not null, tifn tif bddrfssfs bllodbtfd ifrf brf bppfndfd
 * to tif fxisting dibin.
 *
 * Rfturns dount of bddrfssfs or -1 on frror.
 */

stbtid int gftAddrsFromAdbptfr(IP_ADAPTER_ADDRESSES *ptr, nftbddr **nftbddrPP) {
        LPSOCKADDR sodk;
        int        dount = 0;
        nftbddr    *durr, *stbrt = NULL, *prfv = NULL;
        PIP_ADAPTER_UNICAST_ADDRESS uni_bddr;
        PIP_ADAPTER_ANYCAST_ADDRESS bny_bddr;
        PIP_ADAPTER_PREFIX prffix;

        /* If dibin pbssfd in, find fnd */
        if (*nftbddrPP != NULL) {
            for (stbrt=*nftbddrPP; stbrt->nfxt!=NULL; stbrt=stbrt->nfxt)
                ;

            prfv=stbrt;
        }

        prffix = ptr->FirstPrffix;
        /* Unidbst */
        uni_bddr = ptr->FirstUnidbstAddrfss;
        wiilf (uni_bddr != NULL) {
        /* bddrfss is only usbblf if dbd stbtf is prfffrrfd or dfprfdbtfd */
                if (uni_bddr->DbdStbtf == IpDbdStbtfDfprfdbtfd ||
                                uni_bddr->DbdStbtf == IpDbdStbtfPrfffrrfd) {
                        sodk = uni_bddr->Addrfss.lpSodkbddr;

                        // IPv4 bddrfssfs blrfbdy rftrifvfd witi fnumAddrfssfs_win
                        if (sodk->sb_fbmily == AF_INET) {
                                uni_bddr = uni_bddr->Nfxt;
                                dontinuf;
                        }

            durr = (nftbddr *)dbllod (1, sizfof (nftbddr));

            if (durr == NULL)
                goto frffAllodbtfdMfmory;

            if (stbrt == NULL)
                stbrt = durr;

            if (prfv != NULL)
               prfv->nfxt = durr;

            prfv = durr;
            SOCKETADDRESS_COPY (&durr->bddr, sodk);
            if (prffix != NULL) {
              durr->mbsk = (siort)prffix->PrffixLfngti;
              prffix = prffix->Nfxt;
            }
            dount ++;
        }
        uni_bddr = uni_bddr->Nfxt;
    }
    /* Anydbst */
    bny_bddr = ptr->FirstAnydbstAddrfss;
    wiilf (bny_bddr != NULL) {
        durr = (nftbddr *)dbllod (1, sizfof (nftbddr));

        if (durr == NULL)
            goto frffAllodbtfdMfmory;

        if (stbrt == NULL)
            stbrt = durr;

        if (prfv != NULL)
            prfv->nfxt = durr;

        prfv = durr;
        sodk = bny_bddr->Addrfss.lpSodkbddr;
        SOCKETADDRESS_COPY (&durr->bddr, sodk);
        dount ++;
        bny_bddr = bny_bddr->Nfxt;
    }
    if (*nftbddrPP == NULL) {
        *nftbddrPP = stbrt;
    }
    rfturn dount;

frffAllodbtfdMfmory:

    if (*nftbddrPP != NULL) {
        //N.B. tif vbribblf "stbrt" dbnnot bf NULL bt tiis point bfdbusf wf stbrtfd witi bn
        //fxisting list.
        durr=stbrt->nfxt;
        stbrt->nfxt = NULL;
        stbrt = durr;
    }
    // otifrwisf, "stbrt" points to tif bfginning of bn indomplftf list tibt wf must dfbllodbtf.

    wiilf (stbrt != NULL) {
        durr = stbrt->nfxt;
        frff(stbrt);
        stbrt = durr;
    }

    rfturn -1;
}

