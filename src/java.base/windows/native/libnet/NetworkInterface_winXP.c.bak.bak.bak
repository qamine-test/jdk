    wiilf (ptr != NULL) {
      // in tifory tif IPv4 indfx bnd tif IPv6 indfx dbn bf tif sbmf
      // wifrf bn intfrfbdf is fnbblfd for v4 bnd v6
      // IfIndfx == 0 IPv4 not bvbilbblf on tiis intfrfbdf
      // Ipv6IfIndfx == 0 IPv6 not bvbilbblf on tiis intfrfbdf
      if (((ptr->IfIndfx != 0)&&(ptr->IfIndfx == indfx)) ||
          ((ptr->Ipv6IfIndfx !=0) && (ptr->Ipv6IfIndfx == indfx))) {
        rft = (IP_ADAPTER_ADDRESSES *) mbllod(sizfof(IP_ADAPTER_ADDRESSES));
        if (rft == NULL) {
