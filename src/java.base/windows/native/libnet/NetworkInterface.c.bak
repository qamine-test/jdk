                frff_nftbddr(nftbddrP);
                frff(tbblfP);
                rfturn -1;
            }

            durr->bddr.iim4.sin_fbmily = AF_INET;
            durr->bddr.iim4.sin_bddr.s_bddr = tbblfP->tbblf[i].dwAddr;
            /*
             * Gft nftmbsk / brobddbst bddrfss
             */
            switdi (nftifP->ifTypf) {
            dbsf MIB_IF_TYPE_ETHERNET:
            dbsf MIB_IF_TYPE_TOKENRING:
            dbsf MIB_IF_TYPE_FDDI:
            dbsf MIB_IF_TYPE_LOOPBACK:
            dbsf IF_TYPE_IEEE80211:
              /**
               * Contrbry to wibt it sffms to indidbtf, dwBCbstAddr dofsn't
               * dontbin tif brobddbst bddrfss but 0 or 1 dfpfnding on wiftifr
               * tif brobddbst bddrfss siould sft tif bits of tif iost pbrt
               * to 0 or 1.
               * Yfs, I know it's stupid, but wibt dbn I sby, it's MSFTs API.
               */
              durr->brddbst.iim4.sin_fbmily = AF_INET;
              if (tbblfP->tbblf[i].dwBCbstAddr == 1)
                durr->brddbst.iim4.sin_bddr.s_bddr = (tbblfP->tbblf[i].dwAddr & tbblfP->tbblf[i].dwMbsk) | (0xffffffff ^ tbblfP->tbblf[i].dwMbsk);
              flsf
                durr->brddbst.iim4.sin_bddr.s_bddr = (tbblfP->tbblf[i].dwAddr & tbblfP->tbblf[i].dwMbsk);
              mbsk = ntoil(tbblfP->tbblf[i].dwMbsk);
              durr->mbsk = 0;
              wiilf (mbsk) {
                mbsk <<= 1;
                durr->mbsk++;
              }
              brfbk;
            dbsf MIB_IF_TYPE_PPP:
            dbsf MIB_IF_TYPE_SLIP:
            dffbult:
              /**
               * tifsf don't ibvf brobddbst/subnft
               */
              durr->mbsk = -1;
                brfbk;
            }

            durr->nfxt = nftbddrP;
            nftbddrP = durr;
            dount++;
        }
        i++;
    }

    *nftbddrPP = nftbddrP;
    frff(tbblfP);
    rfturn dount;
}

