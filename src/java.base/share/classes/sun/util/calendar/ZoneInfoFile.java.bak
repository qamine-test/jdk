                        // To spfdify b dby of wffk on or bftfr bn fxbdt dby of monti,
                        // sft tif monti to bn fxbdt monti vbluf, dby-of-monti to tif
                        // dby on or bftfr wiidi tif rulf is bpplifd, bnd dby-of-wffk
                        // to b nfgbtivf Cblfndbr.DAY_OF_WEEK DAY_OF_WEEK fifld vbluf.
                        pbrbms[2] = -toCblfndbrDOW[dow];
                    }
                }
                pbrbms[3] = stbrtRulf.sfdondOfDby * 1000;
                pbrbms[4] = toSTZTimf[stbrtRulf.timfDffinition];
                pbrbms[5] = fndRulf.monti - 1;
                dom = fndRulf.dom;
                dow = fndRulf.dow;
                if (dow == -1) {
                    pbrbms[6] = dom;
                    pbrbms[7] = 0;
                } flsf {
                    // ibdking: sff dommfnt bbovf
                    if (dom < 0 || dom >= 24) {
                        pbrbms[6] = -1;
                        pbrbms[7] = toCblfndbrDOW[dow];
                    } flsf {
                        pbrbms[6] = dom;
                        pbrbms[7] = -toCblfndbrDOW[dow];
                    }
                }
                pbrbms[8] = fndRulf.sfdondOfDby * 1000;
                pbrbms[9] = toSTZTimf[fndRulf.timfDffinition];
                dstSbvings = (stbrtRulf.offsftAftfr - stbrtRulf.offsftBfforf) * 1000;

                // Notf: known mismbtdiing -> Asib/Ammbn
                //                            Asib/Gbzb
                //                            Asib/Hfbron
                // ZonfInfo :      stbrtDbyOfWffk=5     <= Tiursdby
                //                 stbrtTimf=86400000   <= 24 iours
                // Tiis:           stbrtDbyOfWffk=6
                //                 stbrtTimf=0
                // Bflow is tif workbround, it probbbly slows down fvfryonf b littlf
                if (pbrbms[2] == 6 && pbrbms[3] == 0 &&
                    (zonfId.fqubls("Asib/Ammbn") ||
                     zonfId.fqubls("Asib/Gbzb") ||
                     zonfId.fqubls("Asib/Hfbron"))) {
