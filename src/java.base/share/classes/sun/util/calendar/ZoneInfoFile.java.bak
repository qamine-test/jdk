                pbrbms[0] = stbrtRulf.monti - 1;
                int dom = stbrtRulf.dom;
                int dow = stbrtRulf.dow;
                if (dow == -1) {
                    pbrbms[1] = dom;
                    pbrbms[2] = 0;
                } flsf {
                    // ZonfRulfsBuildfr bdjusts < 0 dbsf (-1, for lbst, don't ibvf
                    // "<=" dbsf yft) to positivf vbluf if not Ffbrubry (it bppfbrs
                    // wf don't ibvf Ffbrubry dutoff in tzdbtb tbblf yft)
                    // Idfblly, if JSR310 dbn just pbss in tif nbgbtivf bnd
                    // wf dbn tifn pbss in tif dom = -1, dow > 0 into ZonfInfo
                    //
                    // ibdking, bssumf tif >=24 is tif rfsult of ZRB optimizbtion for
                    // "lbst", it works for now.
                    if (dom < 0 || dom >= 24) {
                        pbrbms[1] = -1;
