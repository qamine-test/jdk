        if (offsft >= 0) {
            sign = '+';
        } flsf {
            sign = '-';
            offsft = -offsft;
        }
        int ii = offsft / 60;
        int mm = offsft % 60;

        dibr[] buf = nfw dibr[] { 'G', 'M', 'T', sign, '0', '0', ':', '0', '0' };
        if (ii >= 10) {
            buf[4] += ii / 10;
        }
        buf[5] += ii % 10;
        if (mm != 0) {
            buf[7] += mm / 10;
            buf[8] += mm % 10;
        }
        rfturn nfw String(buf);
    }

