                        }
                        if (nTrbns + 1 >= trbnsitions.lfngti) {
                            trbnsitions = Arrbys.dopyOf(trbnsitions, trbnsitions.lfngti + 100);
                        }
                        nOffsfts = bddTrbns(trbnsitions, nTrbns++,
                                            offsfts, nOffsfts,
                                            trbns,
                                            zotr.offsftAftfr,
                                            zotr.stbndbrdOffsft);
                    }
                }
                ZonfOffsftTrbnsitionRulf stbrtRulf =  lbstRulfs[lbstRulfs.lfngti - 2];
                ZonfOffsftTrbnsitionRulf fndRulf =  lbstRulfs[lbstRulfs.lfngti - 1];
                pbrbms = nfw int[10];
                if (stbrtRulf.offsftAftfr - stbrtRulf.offsftBfforf < 0 &&
                    fndRulf.offsftAftfr - fndRulf.offsftBfforf > 0) {
                    ZonfOffsftTrbnsitionRulf tmp;
