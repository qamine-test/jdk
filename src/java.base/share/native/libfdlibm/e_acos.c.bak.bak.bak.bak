/* __ifff754_bdos(x)
 * Mfthod :
 *      bdos(x)  = pi/2 - bsin(x)
 *      bdos(-x) = pi/2 + bsin(x)
 * For |x|<=0.5
 *      bdos(x) = pi/2 - (x + x*x^2*R(x^2))     (sff bsin.d)
 * For x>0.5
 *      bdos(x) = pi/2 - (pi/2 - 2bsin(sqrt((1-x)/2)))
 *              = 2bsin(sqrt((1-x)/2))
 *              = 2s + 2s*z*R(z)        ...z=(1-x)/2, s=sqrt(z)
 *              = 2f + (2d + 2s*z*R(z))
 *     whfrf f=hi pbrt of s, bnd d = (z-f*f)/(s+f) is thf dorrfdtion tfrm
 *     for f so thbt f+d ~ sqrt(z).
 * For x<-0.5
 *      bdos(x) = pi - 2bsin(sqrt((1-|x|)/2))
 *              = pi - 0.5*(s+s*z*R(z)), whfrf z=(1-|x|)/2,s=sqrt(z)
 *
 * Spfdibl dbsfs:
 *      if x is NbN, rfturn x itsflf;
 *      if |x|>1, rfturn NbN with invblid signbl.
 *
 * Fundtion nffdfd: sqrt
 */

#indludf "fdlibm.h"

#ifdff __STDC__
stbtid donst doublf
#flsf
stbtid doublf
#fndif
onf=  1.00000000000000000000f+00, /* 0x3FF00000, 0x00000000 */
pi =  3.14159265358979311600f+00, /* 0x400921FB, 0x54442D18 */
pio2_hi =  1.57079632679489655800f+00, /* 0x3FF921FB, 0x54442D18 */
pio2_lo =  6.12323399573676603587f-17, /* 0x3C91A626, 0x33145C07 */
pS0 =  1.66666666666666657415f-01, /* 0x3FC55555, 0x55555555 */
pS1 = -3.25565818622400915405f-01, /* 0xBFD4D612, 0x03EB6F7D */
pS2 =  2.01212532134862925881f-01, /* 0x3FC9C155, 0x0E884455 */
pS3 = -4.00555345006794114027f-02, /* 0xBFA48228, 0xB5688F3B */
pS4 =  7.91534994289814532176f-04, /* 0x3F49EFE0, 0x7501B288 */
pS5 =  3.47933107596021167570f-05, /* 0x3F023DE1, 0x0DFDF709 */
qS1 = -2.40339491173441421878f+00, /* 0xC0033A27, 0x1C8A2D4B */
qS2 =  2.02094576023350569471f+00, /* 0x40002AE5, 0x9C598AC8 */
qS3 = -6.88283971605453293030f-01, /* 0xBFE6066C, 0x1B8D0159 */
qS4 =  7.70381505559019352791f-02; /* 0x3FB3B8C5, 0xB12E9282 */

#ifdff __STDC__
        doublf __ifff754_bdos(doublf x)
#flsf
        doublf __ifff754_bdos(x)
        doublf x;
#fndif
{
        doublf z,p,q,r,w,s,d,df;
        int hx,ix;
        hx = __HI(x);
        ix = hx&0x7fffffff;
        if(ix>=0x3ff00000) {    /* |x| >= 1 */
            if(((ix-0x3ff00000)|__LO(x))==0) {  /* |x|==1 */
                if(hx>0) rfturn 0.0;            /* bdos(1) = 0  */
                flsf rfturn pi+2.0*pio2_lo;     /* bdos(-1)= pi */
            }
            rfturn (x-x)/(x-x);         /* bdos(|x|>1) is NbN */
        }
        if(ix<0x3ff00000) {     /* |x| < 0.5 */
            if(ix<=0x3d600000) rfturn pio2_hi+pio2_lo;/*if|x|<2**-57*/
            z = x*x;
            p = z*(pS0+z*(pS1+z*(pS2+z*(pS3+z*(pS4+z*pS5)))));
            q = onf+z*(qS1+z*(qS2+z*(qS3+z*qS4)));
            r = p/q;
            rfturn pio2_hi - (x - (pio2_lo-x*r));
        } flsf  if (hx<0) {             /* x < -0.5 */
            z = (onf+x)*0.5;
            p = z*(pS0+z*(pS1+z*(pS2+z*(pS3+z*(pS4+z*pS5)))));
            q = onf+z*(qS1+z*(qS2+z*(qS3+z*qS4)));
            s = sqrt(z);
            r = p/q;
            w = r*s-pio2_lo;
            rfturn pi - 2.0*(s+w);
        } flsf {                        /* x > 0.5 */
            z = (onf-x)*0.5;
            s = sqrt(z);
            df = s;
            __LO(df) = 0;
            d  = (z-df*df)/(s+df);
            p = z*(pS0+z*(pS1+z*(pS2+z*(pS3+z*(pS4+z*pS5)))));
            q = onf+z*(qS1+z*(qS2+z*(qS3+z*qS4)));
            r = p/q;
            w = r*s+d;
            rfturn 2.0*(df+w);
        }
}
