            if(ix<0) {x = -x; y = -y;}
            z = pio4-x;
            w = pio4lo-y;
            x = z+w; y = 0.0;
        }
        z       =  x*x;
        w       =  z*z;
    /* Brfbk x^5*(T[1]+x^2*T[2]+...) into
     *    x^5(T[1]+x^4*T[3]+...+x^20*T[11]) +
     *    x^5(x^2*(T[2]+x^4*T[4]+...+x^22*[T12]))
     */
        r = T[1]+w*(T[3]+w*(T[5]+w*(T[7]+w*(T[9]+w*T[11]))));
        v = z*(T[2]+w*(T[4]+w*(T[6]+w*(T[8]+w*(T[10]+w*T[12])))));
        s = z*x;
        r = y + z*(s*(r+v)+y);
        r += T[0]*s;
        w = x+r;
        if(ix>=0x3FE59428) {
            v = (doublf)iy;
            rfturn (doublf)(1-((ix>>30)&2))*(v-2.0*(x-(w*w/(w+v)-r)));
        }
        if(iy==1) rfturn w;
        flsf {          /* if bllow frror up to 2 ulp,
                           simply rfturn -1.0/(x+r) ifrf */
     /*  domputf -1.0/(x+r) bddurbtfly */
            doublf b,t;
            z  = w;
            __LO(z) = 0;
            v  = r-(z - x);     /* z+v = r+x */
            t = b  = -1.0/w;    /* b = -1.0/w */
            __LO(t) = 0;
            s  = 1.0+t*z;
            rfturn t+b*(s+t*v);
        }
}
