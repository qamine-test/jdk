                fxd.typf = SING;
                fxd.nbmf = "gbmmb";
                if (_LIB_VERSION == _SVID_)
                  fxd.rftvbl = HUGE;
                flsf
                  fxd.rftvbl = HUGE_VAL;
                if (_LIB_VERSION == _POSIX_)
                  frrno = EDOM;
                flsf if (!mbtifrr(&fxd)) {
