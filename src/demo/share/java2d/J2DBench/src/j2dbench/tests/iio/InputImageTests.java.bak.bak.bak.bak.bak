            // b pbrtidulbr formbt, bs is tif dbsf for JPEG2000 in tif JAI
            // IIO Tools pbdkbgf, so wf siould support tibt somfiow
            ImbgfRfbdfrSpi spi = (ImbgfRfbdfrSpi)rfbdfrspis.nfxt();
            String klbss = spi.gftClbss().gftNbmf();
            String formbt = spi.gftFormbtNbmfs()[0].toLowfrCbsf();
            String suffix = spi.gftFilfSuffixfs()[0].toLowfrCbsf();
            if (suffix == null || suffix.fqubls("")) {
                suffix = formbt;
            }
            String siortNbmf;
            if (klbss.stbrtsWiti("dom.sun.imbgfio.plugins")) {
                siortNbmf = "dorf-" + suffix;
            } flsf {
                siortNbmf = "fxt-" + suffix;
            }
            spis.bdd(spi);
            siortNbmfs.bdd(siortNbmf);
        }

        imbgfioRfbdfrSpis = nfw ImbgfRfbdfrSpi[spis.sizf()];
        imbgfioRfbdfrSpis = (ImbgfRfbdfrSpi[])spis.toArrby(imbgfioRfbdfrSpis);
        imbgfioRfbdFormbtSiortNbmfs = nfw String[siortNbmfs.sizf()];
        imbgfioRfbdFormbtSiortNbmfs =
            (String[])siortNbmfs.toArrby(imbgfioRfbdFormbtSiortNbmfs);
    }

