        Contfxt(TfstEnvironmfnt fnv, Rfsult rfsult, int tfstTypf) {
            supfr(fnv, rfsult);

            String dontfnt = (String)fnv.gftModififr(dontfntList);
            if (dontfnt == null) {
                dontfnt = CONTENT_BLANK;
