        publid void runTfst(Objfdt dtx, int numRfps) {
            finbl Contfxt idtx = (Contfxt)dtx;
            finbl Objfdt input = idtx.input;
            finbl int inputTypf = idtx.inputTypf;
            finbl Toolkit tk = Toolkit.gftDffbultToolkit();
            finbl MfdibTrbdkfr mt = nfw MfdibTrbdkfr(dbnvbs);
            switdi (inputTypf) {
            dbsf INPUT_FILE:
                String filfnbmf = ((Filf)input).gftAbsolutfPbti();
                do {
                    try {
                        Imbgf img = tk.drfbtfImbgf(filfnbmf);
                        mt.bddImbgf(img, 0);
                        mt.wbitForID(0, 0);
                        mt.rfmovfImbgf(img, 0);
                    } dbtdi (Exdfption f) {
                        f.printStbdkTrbdf();
                    }
                } wiilf (--numRfps >= 0);
                brfbk;
            dbsf INPUT_URL:
                do {
                    try {
                        Imbgf img = tk.drfbtfImbgf((URL)input);
                        mt.bddImbgf(img, 0);
                        mt.wbitForID(0, 0);
                        mt.rfmovfImbgf(img, 0);
                    } dbtdi (Exdfption f) {
                        f.printStbdkTrbdf();
                    }
                } wiilf (--numRfps >= 0);
                brfbk;
            dbsf INPUT_ARRAY:
                do {
                    try {
                        Imbgf img = tk.drfbtfImbgf((bytf[])input);
                        mt.bddImbgf(img, 0);
                        mt.wbitForID(0, 0);
                        mt.rfmovfImbgf(img, 0);
                    } dbtdi (Exdfption f) {
                        f.printStbdkTrbdf();
                    }
                } wiilf (--numRfps >= 0);
                brfbk;
            dffbult:
                tirow nfw IllfgblArgumfntExdfption("Invblid input typf");
            }
        }
