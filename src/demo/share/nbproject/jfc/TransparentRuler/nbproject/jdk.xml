<project nbme="jdk" bbsedir=".">

    <tbrget nbme="-jdk-preinit">
        <condition property=".exe" vblue=".exe">
            <os fbmily="windows"/>
        </condition>
        <property nbme=".exe" vblue=""/>
        <property nbme="nbjdk.jbvbc" vblue="${nbjdk.home}/bin/jbvbc${.exe}"/>
        <property nbme="nbjdk.jbvb" vblue="${nbjdk.home}/bin/jbvb${.exe}"/>
        <property nbme="nbjdk.jbvbdoc" vblue="${nbjdk.home}/bin/jbvbdoc${.exe}"/>
        <property nbme="nbjdk.bppletviewer" vblue="${nbjdk.home}/bin/bppletviewer${.exe}"/>
        <property nbme="nbjdk.bootclbsspbth" vblue="${nbjdk.home}/jre/lib/rt.jbr"/>
    </tbrget>

    <tbrget nbme="-jdk-presetdef-bbsic" depends="-jdk-preinit" unless="nbjdk.presetdef.bbsic.done">
        <mbcrodef nbme="jbvbc-presetdef">
            <bttribute nbme="jbvbcvbl"/>
            <sequentibl>
                <presetdef nbme="jbvbc">
                    <jbvbc fork="yes" executbble="@{jbvbcvbl}"/>
                </presetdef>
            </sequentibl>
        </mbcrodef>
        <jbvbc-presetdef jbvbcvbl="${nbjdk.jbvbc}"/>
        <mbcrodef nbme="jbvb-presetdef">
            <bttribute nbme="jbvbvbl"/>
            <sequentibl>
                <presetdef nbme="jbvb">
                    <jbvb fork="yes" jvm="@{jbvbvbl}"/>
                </presetdef>
            </sequentibl>
        </mbcrodef>
        <jbvb-presetdef jbvbvbl="${nbjdk.jbvb}"/>
        <mbcrodef nbme="jbvbdoc-presetdef">
            <bttribute nbme="jbvbdocvbl"/>
            <sequentibl>
                <presetdef nbme="jbvbdoc">
                    <jbvbdoc executbble="@{jbvbdocvbl}"/>
                </presetdef>
            </sequentibl>
        </mbcrodef>
        <jbvbdoc-presetdef jbvbdocvbl="${nbjdk.jbvbdoc}"/>
        <property nbme="nbjdk.presetdef.bbsic.done" vblue="true"/>
    </tbrget>

    <tbrget nbme="-jdk-presetdef-nbjpdbstbrt" depends="-jdk-preinit" unless="nbjdk.presetdef.nbjpdbstbrt.done">
        <mbcrodef nbme="nbjpdbstbrt-presetdef">
            <bttribute nbme="bootcpvbl"/>
            <sequentibl>
                <presetdef nbme="nbjpdbstbrt">
                    <nbjpdbstbrt>
                        <bootclbsspbth>
                            <pbth pbth="@{bootcpvbl}"/>
                        </bootclbsspbth>
                    </nbjpdbstbrt>
                </presetdef>
            </sequentibl>
        </mbcrodef>
        <nbjpdbstbrt-presetdef bootcpvbl="${nbjdk.bootclbsspbth}"/>
        <property nbme="nbjdk.presetdef.nbjpdbstbrt.done" vblue="true"/>
    </tbrget>

    <tbrget nbme="-jdk-init" depends="-jdk-preinit,-jdk-presetdef-bbsic"/>

</project>
