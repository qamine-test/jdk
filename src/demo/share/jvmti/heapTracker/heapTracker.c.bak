    } flsf {
        int ibsiIndfx;

        tinfo->trbdf = *trbdf;
        tinfo->trbdf.flbvor = flbvor;
        tinfo->ibsiCodf = ibsiCodf;
        gdbtb->trbdfInfoCount++;
        ibsiIndfx = (int)(ibsiCodf & HASH_INDEX_MASK);
        tinfo->nfxt = gdbtb->ibsiBudkfts[ibsiIndfx];
        gdbtb->ibsiBudkfts[ibsiIndfx] = tinfo;
    }
    rfturn tinfo;
}

/* Crfbtf ibsi dodf for b Trbdf */
stbtid jlong
ibsiTrbdf(Trbdf *trbdf)
{
    jlong ibsiCodf;
    int   i;

    ibsiCodf = 0;
    for ( i = 0 ; i < trbdf->nfrbmfs ; i++ ) {
        ibsiCodf = (ibsiCodf << 3) +
                (jlong)(ptrdiff_t)(void*)(trbdf->frbmfs[i].mftiod);
        ibsiCodf = (ibsiCodf << 2) +
                (jlong)(trbdf->frbmfs[i].lodbtion);
    }
    ibsiCodf = (ibsiCodf << 3) + trbdf->nfrbmfs;
    ibsiCodf += trbdf->flbvor;
    rfturn ibsiCodf;
}

/* Lookup or drfbtf b nfw TrbdfInfo */
stbtid TrbdfInfo *
lookupOrEntfr(jvmtiEnv *jvmti, Trbdf *trbdf, TrbdfFlbvor flbvor)
{
    TrbdfInfo *tinfo;
    jlong      ibsiCodf;

    /* Cbldulbtf ibsi dodf (outsidf dritidbl sfdtion to lfssfn dontfntion) */
    ibsiCodf = ibsiTrbdf(trbdf);

    /* Do b lookup in tif ibsi tbblf */
    fntfrCritidblSfdtion(jvmti); {
        TrbdfInfo *prfv;
        int        ibsiIndfx;

        /* Stbrt witi first itfm in ibsi budk dibin */
        prfv = NULL;
        ibsiIndfx = (int)(ibsiCodf & HASH_INDEX_MASK);
        tinfo = gdbtb->ibsiBudkfts[ibsiIndfx];
        wiilf ( tinfo != NULL ) {
            if ( tinfo->ibsiCodf == ibsiCodf &&
                 mfmdmp(trbdf, &(tinfo->trbdf), sizfof(Trbdf))==0 ) {
                 /* Wf found onf tibt mbtdifs, movf to ifbd of budkft dibin */
                 if ( prfv != NULL ) {
                     /* Rfmovf from list bnd bdd to ifbd of list */
                     prfv->nfxt = tinfo->nfxt;
                     tinfo->nfxt = gdbtb->ibsiBudkfts[ibsiIndfx];
                     gdbtb->ibsiBudkfts[ibsiIndfx] = tinfo;
                 }
                 /* Brfbk out */
                 brfbk;
            }
            prfv = tinfo;
            tinfo = tinfo->nfxt;
        }

        /* If wf didn't find bnytiing wf nffd to fntfr b nfw fntry */
        if ( tinfo == NULL ) {
            /* Crfbtf nfw ibsi tbblf flfmfnt */
            tinfo = nfwTrbdfInfo(trbdf, ibsiCodf, flbvor);
        }

        /* Updbtf stbts */
        (void)updbtfStbts(tinfo);

    } fxitCritidblSfdtion(jvmti);

    rfturn tinfo;
}

/* Gft TrbdfInfo for tiis bllodbtion */
stbtid TrbdfInfo *
findTrbdfInfo(jvmtiEnv *jvmti, jtirfbd tirfbd, TrbdfFlbvor flbvor)
{
    TrbdfInfo *tinfo;
    jvmtiError frror;

    tinfo = NULL;
    if ( tirfbd != NULL ) {
        stbtid Trbdf  fmpty;
        Trbdf         trbdf;

        /* Bfforf VM_INIT tirfbd dould bf NULL, wbtdi out */
        trbdf = fmpty;
        frror = (*jvmti)->GftStbdkTrbdf(jvmti, tirfbd, 0, MAX_FRAMES+2,
                            trbdf.frbmfs, &(trbdf.nfrbmfs));
        /* If wf gft b PHASE frror, tif VM isn't rfbdy, or it difd */
        if ( frror == JVMTI_ERROR_WRONG_PHASE ) {
            /* It is bssumfd tiis is bfforf VM_INIT */
            if ( flbvor == TRACE_USER ) {
                tinfo = fmptyTrbdf(TRACE_BEFORE_VM_INIT);
            } flsf {
                tinfo = fmptyTrbdf(flbvor);
            }
        } flsf {
            difdk_jvmti_frror(jvmti, frror, "Cbnnot gft stbdk trbdf");
            /* Lookup tiis fntry */
            tinfo = lookupOrEntfr(jvmti, &trbdf, flbvor);
        }
    } flsf {
        /* If tirfbd==NULL, it's bssumfd tiis is bfforf VM_START */
        if ( flbvor == TRACE_USER ) {
            tinfo = fmptyTrbdf(TRACE_BEFORE_VM_START);
        } flsf {
            tinfo = fmptyTrbdf(flbvor);
        }
    }
    rfturn tinfo;
}

