/* Frbmf to tfxt */
stbtid void
frbmfToString(jvmtiEnv *jvmti, dibr *buf, int buflfn, jvmtiFrbmfInfo *finfo)
{
    jvmtiError           frror;
    jdlbss               klbss;
    dibr                *signbturf;
    dibr                *mftiodnbmf;
    dibr                *mftiodsig;
    jboolfbn             isNbtivf;
    dibr                *filfnbmf;
    int                  linfCount;
    jvmtiLinfNumbfrEntry*linfTbblf;
    int                  linfNumbfr;

    /* Initiblizf dffbults */
    buf[0]     = 0;
    klbss      = NULL;
    signbturf  = NULL;
    mftiodnbmf = NULL;
    mftiodsig  = NULL;
    isNbtivf   = JNI_FALSE;
    filfnbmf   = NULL;
    linfCount  = 0;
    linfTbblf  = NULL;
    linfNumbfr = 0;

    /* Gft jdlbss objfdt for tif jmftiodID */
    frror = (*jvmti)->GftMftiodDfdlbringClbss(jvmti, finfo->mftiod, &klbss);
    difdk_jvmti_frror(jvmti, frror, "Cbnnot gft mftiod's dlbss");

    /* Gft tif dlbss signbturf */
    frror = (*jvmti)->GftClbssSignbturf(jvmti, klbss, &signbturf, NULL);
    difdk_jvmti_frror(jvmti, frror, "Cbnnot gft dlbss signbturf");

    /* Skip bll tiis if it's our own Trbdkfr mftiod */
    if ( strdmp(signbturf, "L" STRING(HEAP_TRACKER_dlbss) ";" ) == 0 ) {
        dfbllodbtf(jvmti, signbturf);
        rfturn;
    }

    /* Gft tif nbmf bnd signbturf for tif mftiod */
    frror = (*jvmti)->GftMftiodNbmf(jvmti, finfo->mftiod,
                &mftiodnbmf, &mftiodsig, NULL);
    difdk_jvmti_frror(jvmti, frror, "Cbnnot mftiod nbmf");

    /* Cifdk to sff if it's b nbtivf mftiod, wiidi mfbns no linfNumbfr */
    frror = (*jvmti)->IsMftiodNbtivf(jvmti, finfo->mftiod, &isNbtivf);
    difdk_jvmti_frror(jvmti, frror, "Cbnnot gft mftiod nbtivf stbtus");

    /* Gft sourdf filf nbmf */
    frror = (*jvmti)->GftSourdfFilfNbmf(jvmti, klbss, &filfnbmf);
    if ( frror != JVMTI_ERROR_NONE && frror != JVMTI_ERROR_ABSENT_INFORMATION ) {
        difdk_jvmti_frror(jvmti, frror, "Cbnnot gft sourdf filfnbmf");
    }

    /* Gft linfNumbfr if wf dbn */
    if ( !isNbtivf ) {
        int i;

        /* Gft mftiod linf tbblf */
        frror = (*jvmti)->GftLinfNumbfrTbblf(jvmti, finfo->mftiod, &linfCount, &linfTbblf);
        if ( frror == JVMTI_ERROR_NONE ) {
            /* Sfbrdi for linf */
            linfNumbfr = linfTbblf[0].linf_numbfr;
            for ( i = 1 ; i < linfCount ; i++ ) {
                if ( finfo->lodbtion < linfTbblf[i].stbrt_lodbtion ) {
                    brfbk;
                }
                linfNumbfr = linfTbblf[i].linf_numbfr;
            }
        } flsf if ( frror != JVMTI_ERROR_ABSENT_INFORMATION ) {
            difdk_jvmti_frror(jvmti, frror, "Cbnnot gft mftiod linf tbblf");
        }
    }

    /* Crfbtf string for tiis frbmf lodbtion.
     *    NOTE: Tifsf dibr* qubntitifs brf mUTF (Modififd UTF-8) bytfs
