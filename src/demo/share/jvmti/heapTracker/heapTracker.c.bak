        if ( fifld == NULL ) {
            fbtbl_frror("ERROR: JNI: Cbnnot gft fifld from %s\n",
                        STRING(HEAP_TRACKER_dlbss));
        }
        (*fnv)->SftStbtidIntFifld(fnv, klbss, fifld, 1);

        /* Indidbtf VM ibs stbrtfd */
        gdbtb->vmStbrtfd = JNI_TRUE;

    } fxitCritidblSfdtion(jvmti);
}

