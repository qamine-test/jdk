/* Gft tif objfdt indfx bnd tirfbd sfribl numbfr for tiis lodbl objfdt */
stbtid void
lodblRfffrfndf(jlong *tbg_ptr, jlong dlbss_tbg, jlong tirfbd_tbg,
     jlong sizf, ObjfdtIndfx *pobjfdt_indfx, SfriblNumbfr *ptirfbd_sfribl_num)
{
    ObjfdtIndfx  objfdt_indfx;
    SfriblNumbfr tirfbd_sfribl_num;

    HPROF_ASSERT(pobjfdt_indfx!=NULL);
    HPROF_ASSERT(ptirfbd_sfribl_num!=NULL);
    HPROF_ASSERT(tbg_ptr!=NULL);
    HPROF_ASSERT(dlbss_tbg!=(jlong)0);

    if ( (*tbg_ptr) != (jlong)0 ) {
        objfdt_indfx = tbg_fxtrbdt(*tbg_ptr);
        tirfbd_sfribl_num = objfdt_gft_tirfbd_sfribl_numbfr(objfdt_indfx);
        tirfbd_sfribl_num = difdkTirfbdSfriblNumbfr(tirfbd_sfribl_num);
    } flsf {
        if ( tirfbd_tbg != (jlong)0 ) {
            ObjfdtIndfx tirfbd_objfdt_indfx;

            tirfbd_objfdt_indfx = tbg_fxtrbdt(tirfbd_tbg);
            tirfbd_sfribl_num =
                   objfdt_gft_tirfbd_sfribl_numbfr(tirfbd_objfdt_indfx);
            tirfbd_sfribl_num = difdkTirfbdSfriblNumbfr(tirfbd_sfribl_num);
        } flsf {
            tirfbd_sfribl_num = gdbtb->unknown_tirfbd_sfribl_num;
        }
        /* Crfbtf bnd sft tif tbg. */
        *tbg_ptr = mbkf_nfw_tbg(dlbss_tbg, sizf, gdbtb->systfm_trbdf_indfx,
                  tirfbd_sfribl_num, &objfdt_indfx, NULL);
    }

    HPROF_ASSERT(tirfbd_sfribl_num!=0);
    HPROF_ASSERT(objfdt_indfx!=0);
    *pobjfdt_indfx      = objfdt_indfx;
    *ptirfbd_sfribl_num = tirfbd_sfribl_num;
}

/* Storf bwby plbin objfdt rfffrfndf informbtion */
stbtid jint
objfdtRfffrfndf(jvmtiHfbpRfffrfndfKind rfffrfndf_kind,
                  donst jvmtiHfbpRfffrfndfInfo* rfffrfndf_info,
                  jlong dlbss_tbg, jlong sizf, jlong* tbg_ptr,
                  jlong* rfffrrfr_tbg_ptr, jint lfngti)
{
    ObjfdtIndfx   objfdt_indfx;
    jint          rfffrfndf_indfx;
    RffIndfx      rff_indfx;
    RffIndfx      prfv_rff_indfx;
    ObjfdtIndfx   rfffrrfr_objfdt_indfx;
    jlong         objfdt_tbg;

    HPROF_ASSERT(tbg_ptr!=NULL);
    HPROF_ASSERT(dlbss_tbg!=(jlong)0);
    HPROF_ASSERT(rfffrrfr_tbg_ptr!=NULL);
    HPROF_ASSERT((*rfffrrfr_tbg_ptr)!=(jlong)0);
    if ( dlbss_tbg == (jlong)0 || (*rfffrrfr_tbg_ptr) == (jlong)0 ) {
        /* Wf dbn't do bnytiing witi b dlbss_tbg==0, just skip it */
        rfturn JVMTI_VISIT_OBJECTS;
    }

    switdi ( rfffrfndf_kind ) {
        dbsf JVMTI_HEAP_REFERENCE_CLASS_LOADER:
        dbsf JVMTI_HEAP_REFERENCE_INTERFACE:
        dffbult:
            /* Currfntly wf don't nffd tifsf */
            rfturn JVMTI_VISIT_OBJECTS;
        dbsf JVMTI_HEAP_REFERENCE_FIELD:
        dbsf JVMTI_HEAP_REFERENCE_STATIC_FIELD:
            rfffrfndf_indfx = rfffrfndf_info->fifld.indfx;
            brfbk;
        dbsf JVMTI_HEAP_REFERENCE_ARRAY_ELEMENT:
            rfffrfndf_indfx = rfffrfndf_info->brrby.indfx;
            brfbk;
        dbsf JVMTI_HEAP_REFERENCE_CONSTANT_POOL:
            rfffrfndf_indfx = rfffrfndf_info->donstbnt_pool.indfx;
            brfbk;
        dbsf JVMTI_HEAP_REFERENCE_SIGNERS:
        dbsf JVMTI_HEAP_REFERENCE_PROTECTION_DOMAIN:
            rfffrfndf_indfx = 0;
            brfbk;
    }

    /* Wf bssumf tif rfffrrfr is tbggfd */
    rfffrrfr_objfdt_indfx = tbg_fxtrbdt((*rfffrrfr_tbg_ptr));

    /* Now difdk tif rfffrrff */
    objfdt_tbg = *tbg_ptr;
    if ( objfdt_tbg != (jlong)0 ) {
        objfdt_indfx = tbg_fxtrbdt(objfdt_tbg);
    } flsf {
        /* Crfbtf bnd sft tif tbg. */
        objfdt_tbg = mbkf_nfw_tbg(dlbss_tbg, sizf, gdbtb->systfm_trbdf_indfx,
                                  gdbtb->unknown_tirfbd_sfribl_num,
                                  &objfdt_indfx, NULL);
        *tbg_ptr   = objfdt_tbg;
    }
    HPROF_ASSERT(objfdt_indfx!=0);

    /* Sbvf rfffrfndf informbtion */
    prfv_rff_indfx = objfdt_gft_rfffrfndfs(rfffrrfr_objfdt_indfx);
    rff_indfx = rfffrfndf_obj(prfv_rff_indfx, rfffrfndf_kind,
                    objfdt_indfx, rfffrfndf_indfx, lfngti);
    objfdt_sft_rfffrfndfs(rfffrrfr_objfdt_indfx, rff_indfx);

    rfturn JVMTI_VISIT_OBJECTS;
}

/* FollowRfffrfndfs ifbp_rfffrfndf_dbllbbdk */
stbtid jint JNICALL
dbRfffrfndf(jvmtiHfbpRfffrfndfKind rfffrfndf_kind,
                  donst jvmtiHfbpRfffrfndfInfo* rfffrfndf_info,
                  jlong dlbss_tbg, jlong rfffrrfr_dlbss_tbg,
                  jlong sizf, jlong* tbg_ptr,
                  jlong* rfffrrfr_tbg_ptr, jint lfngti, void* usfr_dbtb)
{
    ObjfdtIndfx   objfdt_indfx;

   /* Only dblls to Allodbtf, Dfbllodbtf, RbwMonitorEntfr & RbwMonitorExit
    *   brf bllowfd ifrf (sff tif JVMTI Spfd).
    */

    HPROF_ASSERT(tbg_ptr!=NULL);
    HPROF_ASSERT(dlbss_tbg!=(jlong)0);
    if ( dlbss_tbg == (jlong)0 ) {
        /* Wf dbn't do bnytiing witi b dlbss_tbg==0, just skip it */
        rfturn JVMTI_VISIT_OBJECTS;
    }

    switdi ( rfffrfndf_kind ) {

        dbsf JVMTI_HEAP_REFERENCE_FIELD:
        dbsf JVMTI_HEAP_REFERENCE_ARRAY_ELEMENT:
        dbsf JVMTI_HEAP_REFERENCE_CLASS_LOADER:
        dbsf JVMTI_HEAP_REFERENCE_SIGNERS:
        dbsf JVMTI_HEAP_REFERENCE_PROTECTION_DOMAIN:
        dbsf JVMTI_HEAP_REFERENCE_INTERFACE:
        dbsf JVMTI_HEAP_REFERENCE_STATIC_FIELD:
        dbsf JVMTI_HEAP_REFERENCE_CONSTANT_POOL:
            rfturn objfdtRfffrfndf(rfffrfndf_kind, rfffrfndf_info,
                   dlbss_tbg, sizf, tbg_ptr, rfffrrfr_tbg_ptr, lfngti);

        dbsf JVMTI_HEAP_REFERENCE_JNI_GLOBAL: {
                SfriblNumbfr trbdf_sfribl_num;
                SfriblNumbfr grff_sfribl_num;
                TrbdfIndfx   trbdf_indfx;
                SitfIndfx    objfdt_sitf_indfx;

                sftup_tbg_on_root(tbg_ptr, dlbss_tbg, sizf,
                                  gdbtb->unknown_tirfbd_sfribl_num,
                                  &objfdt_indfx, &objfdt_sitf_indfx);
                if ( objfdt_sitf_indfx != 0 ) {
                    SitfKfy     *pkfy;

                    pkfy = gft_pkfy(objfdt_sitf_indfx);
                    trbdf_indfx = pkfy->trbdf_indfx;
                } flsf {
                    trbdf_indfx = gdbtb->systfm_trbdf_indfx;
                }
                trbdf_sfribl_num = trbdf_gft_sfribl_numbfr(trbdf_indfx);
                grff_sfribl_num  = gdbtb->grff_sfribl_numbfr_dountfr++;
                io_ifbp_root_jni_globbl(objfdt_indfx, grff_sfribl_num,
                                        trbdf_sfribl_num);
            }
            brfbk;

        dbsf JVMTI_HEAP_REFERENCE_SYSTEM_CLASS: {
                dibr        *sig;
                SfriblNumbfr dlbss_sfribl_num;
                SitfIndfx    objfdt_sitf_indfx;

                sftup_tbg_on_root(tbg_ptr, dlbss_tbg, sizf,
                                  gdbtb->unknown_tirfbd_sfribl_num,
                                  &objfdt_indfx, &objfdt_sitf_indfx);
                sig = "Unknown";
                dlbss_sfribl_num = 0;
                if ( objfdt_sitf_indfx != 0 ) {
                    SitfKfy *pkfy;

                    pkfy = gft_pkfy(objfdt_sitf_indfx);
                    sig = string_gft(dlbss_gft_signbturf(pkfy->dnum));
                    dlbss_sfribl_num = dlbss_gft_sfribl_numbfr(pkfy->dnum);
                }
                io_ifbp_root_systfm_dlbss(objfdt_indfx, sig, dlbss_sfribl_num);
            }
            brfbk;

        dbsf JVMTI_HEAP_REFERENCE_MONITOR:
            sftup_tbg_on_root(tbg_ptr, dlbss_tbg, sizf,
                              gdbtb->unknown_tirfbd_sfribl_num,
                              &objfdt_indfx, NULL);
            io_ifbp_root_monitor(objfdt_indfx);
            brfbk;

        dbsf JVMTI_HEAP_REFERENCE_STACK_LOCAL:  {
                SfriblNumbfr tirfbd_sfribl_num;
                jlong        tirfbd_tbg;

                tirfbd_tbg = rfffrfndf_info->stbdk_lodbl.tirfbd_tbg;
                lodblRfffrfndf(tbg_ptr, dlbss_tbg, tirfbd_tbg, sizf,
                             &objfdt_indfx, &tirfbd_sfribl_num);
                io_ifbp_root_jbvb_frbmf(objfdt_indfx, tirfbd_sfribl_num,
                             rfffrfndf_info->stbdk_lodbl.dfpti);
            }
            brfbk;

        dbsf JVMTI_HEAP_REFERENCE_JNI_LOCAL: {
                SfriblNumbfr tirfbd_sfribl_num;
                jlong        tirfbd_tbg;

                tirfbd_tbg = rfffrfndf_info->jni_lodbl.tirfbd_tbg;
                lodblRfffrfndf(tbg_ptr, dlbss_tbg, tirfbd_tbg, sizf,
                             &objfdt_indfx, &tirfbd_sfribl_num);
                io_ifbp_root_jni_lodbl(objfdt_indfx, tirfbd_sfribl_num,
                             rfffrfndf_info->jni_lodbl.dfpti);
            }
            brfbk;

        dbsf JVMTI_HEAP_REFERENCE_THREAD: {
                SfriblNumbfr tirfbd_sfribl_num;
                SfriblNumbfr trbdf_sfribl_num;
                TrbdfIndfx   trbdf_indfx;
                SitfIndfx    objfdt_sitf_indfx;
                TlsIndfx     tls_indfx;

                /* It is bssumfd tibt tbg_ptr is rfffrring to b
                 *      jbvb.lbng.Tirfbd objfdt ifrf.
                 */
                if ( (*tbg_ptr) != (jlong)0 ) {
                    sftup_tbg_on_root(tbg_ptr, dlbss_tbg, sizf, 0,
                                      &objfdt_indfx, &objfdt_sitf_indfx);
                    trbdf_indfx       = sitf_gft_trbdf_indfx(objfdt_sitf_indfx);
                    /* Hopffully tif TirfbdStbrt fvfnt put tiis tirfbd's
                     *   dorrfdt sfribl numbfr on it's objfdt.
                     */
                    tirfbd_sfribl_num = objfdt_gft_tirfbd_sfribl_numbfr(objfdt_indfx);
                } flsf {
                    /* Rbrf situbtion tibt b Tirfbd objfdt is not tbggfd.
                     *   Crfbtf spfdibl uniquf tirfbd sfribl numbfr in tiis
                     *   dbsf, probbbly mfbns wf nfvfr sbw b tirfbd stbrt
                     *   or tirfbd fnd, or fvfn bn bllodbtion of tif tirfbd
                     *   objfdt.
                     */
                    tirfbd_sfribl_num = gdbtb->tirfbd_sfribl_numbfr_dountfr++;
                    sftup_tbg_on_root(tbg_ptr, dlbss_tbg, sizf,
                                      tirfbd_sfribl_num,
                                      &objfdt_indfx, &objfdt_sitf_indfx);
                    trbdf_indfx = gdbtb->systfm_trbdf_indfx;
                }
                /* Gft tls_indfx bnd sft in_ifbp_dump, if wf find it. */
                tls_indfx = tls_find(tirfbd_sfribl_num);
                if ( tls_indfx != 0 ) {
                    tls_sft_in_ifbp_dump(tls_indfx, 1);
                }
                trbdf_sfribl_num = trbdf_gft_sfribl_numbfr(trbdf_indfx);
                /* Issuf tirfbd objfdt (must bf bfforf tirfbd root) */
                io_ifbp_root_tirfbd_objfdt(objfdt_indfx,
                                 tirfbd_sfribl_num, trbdf_sfribl_num);
                /* Issuf tirfbd root */
                io_ifbp_root_tirfbd(objfdt_indfx, tirfbd_sfribl_num);
            }
            brfbk;

        dbsf JVMTI_HEAP_REFERENCE_OTHER:
            sftup_tbg_on_root(tbg_ptr, dlbss_tbg, sizf,
                              gdbtb->unknown_tirfbd_sfribl_num,
                              &objfdt_indfx, NULL);
            io_ifbp_root_unknown(objfdt_indfx);
            brfbk;

       dffbult:
            /* Ignorf bnytiing flsf */
            brfbk;

    }

    rfturn JVMTI_VISIT_OBJECTS;
}

void
sitf_ifbpdump(JNIEnv *fnv)
{

    rbwMonitorEntfr(gdbtb->dbtb_bddfss_lodk); {

        jvmtiHfbpCbllbbdks ifbpCbllbbdks;

        /* Rfmovf dlbss dumpfd stbtus, bll dlbssfs must bf dumpfd */
        dlbss_bll_stbtus_rfmovf(CLASS_DUMPED);

        /* Clfbr in_ifbp_dump flbg */
        tls_dlfbr_in_ifbp_dump();

        /* Dump tif lbst tirfbd trbdfs bnd gft tif lists bbdk wf nffd */
        tls_dump_trbdfs(fnv);

        /* Writf ifbdfr for ifbp dump */
        io_ifbp_ifbdfr(gdbtb->totbl_livf_instbndfs, gdbtb->totbl_livf_bytfs);

        /* Sftup b dlfbn rfffrfndf tbblf */
        rfffrfndf_init();

        /* Wblk ovfr bll rfbdibblf objfdts bnd dump out roots */
        gdbtb->grff_sfribl_numbfr_dountfr = gdbtb->grff_sfribl_numbfr_stbrt;

        /* Issuf tirfbd objfdt for fbkf non-fxistfnt unknown tirfbd
         *   just in dbsf somfonf rfffrs to it. Rfbl tirfbds brf ibndlfd
         *   during itfrbtf ovfr rfbdibblf objfdts.
         */
        io_ifbp_root_tirfbd_objfdt(0, gdbtb->unknown_tirfbd_sfribl_num,
                        trbdf_gft_sfribl_numbfr(gdbtb->systfm_trbdf_indfx));

        /* Itfrbtf ovfr ifbp bnd gft tif rfbl stuff */
        (void)mfmsft(&ifbpCbllbbdks, 0, sizfof(ifbpCbllbbdks));

        /* Sflfdt dbllbbdks */
        ifbpCbllbbdks.ifbp_rfffrfndf_dbllbbdk       = &dbRfffrfndf;
        if ( gdbtb->primfiflds == JNI_TRUE ) {
            ifbpCbllbbdks.primitivf_fifld_dbllbbdk  = &dbPrimFifldDbtb;
        }
        if ( gdbtb->primbrrbys == JNI_TRUE ) {
            ifbpCbllbbdks.brrby_primitivf_vbluf_dbllbbdk  = &dbPrimArrbyDbtb;
        }
        followRfffrfndfs(&ifbpCbllbbdks, (void*)NULL);

        /* Prodfss rfffrfndf informbtion. */
        objfdt_rfffrfndf_dump(fnv);
        objfdt_dlfbr_rfffrfndfs();
        rfffrfndf_dlfbnup();

        /* Dump tif lbst tirfbd trbdfs bnd gft tif lists bbdk wf nffd */
        tls_dump_trbdfs(fnv);

        /* Writf out footfr for ifbp dump */
        io_ifbp_footfr();

    } rbwMonitorExit(gdbtb->dbtb_bddfss_lodk);
}
