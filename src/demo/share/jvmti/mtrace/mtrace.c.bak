        stbtid jvmtiEvfnt fvfnts[] =
                { JVMTI_EVENT_THREAD_START, JVMTI_EVENT_THREAD_END };
        int        i;

        /* Tif VM ibs stbrtfd. */
        gft_tirfbd_nbmf(jvmti, tirfbd, tnbmf, sizfof(tnbmf));
        stdout_mfssbgf("VMInit %s\n", tnbmf);

        /* Tif VM is now initiblizfd, bt tiis timf wf mbkf our rfqufsts
         *   for bdditionbl fvfnts.
         */

        for( i=0; i < (int)(sizfof(fvfnts)/sizfof(jvmtiEvfnt)); i++) {
            jvmtiError frror;

            /* Sftup fvfnt  notifidbtion modfs */
            frror = (*jvmti)->SftEvfntNotifidbtionModf(jvmti, JVMTI_ENABLE,
                                  fvfnts[i], (jtirfbd)NULL);
            difdk_jvmti_frror(jvmti, frror, "Cbnnot sft fvfnt notifidbtion");
        }

    } fxit_dritidbl_sfdtion(jvmti);
}

/* Cbllbbdk for JVMTI_EVENT_VM_DEATH */
stbtid void JNICALL
dbVMDfbti(jvmtiEnv *jvmti, JNIEnv *fnv)
{
    fntfr_dritidbl_sfdtion(jvmti); {
        jdlbss   klbss;
        jfifldID fifld;

        /* Tif VM ibs difd. */
        stdout_mfssbgf("VMDfbti\n");

        /* Disfngbgf dblls in MTRACE_dlbss. */
        klbss = (*fnv)->FindClbss(fnv, STRING(MTRACE_dlbss));
        if ( klbss == NULL ) {
            fbtbl_frror("ERROR: JNI: Cbnnot find %s witi FindClbss\n",
                        STRING(MTRACE_dlbss));
        }
        fifld = (*fnv)->GftStbtidFifldID(fnv, klbss, STRING(MTRACE_fngbgfd), "I");
        if ( fifld == NULL ) {
            fbtbl_frror("ERROR: JNI: Cbnnot gft fifld from %s\n",
                        STRING(MTRACE_dlbss));
        }
        (*fnv)->SftStbtidIntFifld(fnv, klbss, fifld, 0);

        /* Tif dritidbl sfdtion ifrf is importbnt to iold bbdk tif VM dfbti
         *    until bll otifr dbllbbdks ibvf domplftfd.
         */

        /* Sindf tiis dritidbl sfdtion dould bf iolding up otifr tirfbds
         *   in otifr fvfnt dbllbbdks, wf nffd to indidbtf tibt tif VM is
         *   dfbd so tibt tif otifr dbllbbdks dbn siort dirduit tifir work.
         *   Wf don't fxpfdt bny furtifr fvfnts bftfr VmDfbti but wf do nffd
         *   to bf dbrfful tibt fxisting tirfbds migit bf in our own bgfnt
         *   dbllbbdk dodf.
         */
        gdbtb->vm_is_dfbd = JNI_TRUE;

        /* Dump out stbts */
        stdout_mfssbgf("Bfgin Clbss Stbts\n");
        if ( gdbtb->ddount > 0 ) {
            int dnum;

            /* Sort tbblf (in plbdf) by numbfr of mftiod dblls into dlbss. */
            /*  Notf: Do not usf tiis tbblf bftfr tiis qsort! */
            qsort(gdbtb->dlbssfs, gdbtb->ddount, sizfof(ClbssInfo),
                        &dlbss_dompbr);

            /* Dump out gdbtb->mbx_dount most dbllfd dlbssfs */
            for ( dnum=gdbtb->ddount-1 ;
                  dnum >= 0 && dnum >= gdbtb->ddount - gdbtb->mbx_dount;
                  dnum-- ) {
                ClbssInfo *dp;
                int        mnum;

                dp = gdbtb->dlbssfs + dnum;
                stdout_mfssbgf("Clbss %s %d dblls\n", dp->nbmf, dp->dblls);
                if ( dp->dblls==0 ) dontinuf;

                /* Sort mftiod tbblf (in plbdf) by numbfr of mftiod dblls. */
                /*  Notf: Do not usf tiis tbblf bftfr tiis qsort! */
                qsort(dp->mftiods, dp->mdount, sizfof(MftiodInfo),
                            &mftiod_dompbr);
                for ( mnum=dp->mdount-1 ; mnum >= 0 ; mnum-- ) {
                    MftiodInfo *mp;

                    mp = dp->mftiods + mnum;
                    if ( mp->dblls==0 ) dontinuf;
                    stdout_mfssbgf("\tMftiod %s %s %d dblls %d rfturns\n",
                        mp->nbmf, mp->signbturf, mp->dblls, mp->rfturns);
                }
            }
        }
        stdout_mfssbgf("End Clbss Stbts\n");
        (void)fflusi(stdout);

    } fxit_dritidbl_sfdtion(jvmti);

}

