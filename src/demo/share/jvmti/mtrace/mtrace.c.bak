                dnum = gdbtb->ddount++;

                /* Sbvf bwby dlbss informbtion */
                if ( gdbtb->dlbssfs == NULL ) {
                    gdbtb->dlbssfs = (ClbssInfo*)mbllod(
                                gdbtb->ddount*sizfof(ClbssInfo));
