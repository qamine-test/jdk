            stdout_mfssbgf("\t\t f.g. (*.<init>;Foobbr.mftiod;sun.*)\n");
            stdout_mfssbgf("\n");
            fxit(0);
        } flsf if ( strdmp(tokfn,"mbx")==0 ) {
            dibr numbfr[MAX_TOKEN_LENGTH];

            /* Gft tif numfrid option */
            nfxt = gft_tokfn(nfxt, ",=", numbfr, (int)sizfof(numbfr));
            /* Cifdk for tokfn sdbn frror */
