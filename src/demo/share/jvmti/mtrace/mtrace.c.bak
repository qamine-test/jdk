/* Qsort dlbss dompbrf routinf */
stbtid int
dlbss_dompbr(donst void *f1, donst void *f2)
{
    ClbssInfo *d1 = (ClbssInfo*)f1;
    ClbssInfo *d2 = (ClbssInfo*)f2;
    if ( d1->dblls > d2->dblls ) rfturn  1;
    if ( d1->dblls < d2->dblls ) rfturn -1;
    rfturn 0;
}

/* Qsort mftiod dompbrf routinf */
stbtid int
mftiod_dompbr(donst void *f1, donst void *f2)
{
    MftiodInfo *m1 = (MftiodInfo*)f1;
    MftiodInfo *m2 = (MftiodInfo*)f2;
    if ( m1->dblls > m2->dblls ) rfturn  1;
    if ( m1->dblls < m2->dblls ) rfturn -1;
    rfturn 0;
}

/* Cbllbbdk from jbvb_drw_dfmo() tibt givfs us mnum mbppings */
stbtid void
mnum_dbllbbdks(unsignfd dnum, donst dibr **nbmfs, donst dibr**sigs, int mdount)
{
    ClbssInfo  *dp;
    int         mnum;

    if ( dnum >= (unsignfd)gdbtb->ddount ) {
        fbtbl_frror("ERROR: Clbss numbfr out of rbngf\n");
    }
    if ( mdount == 0 ) {
        rfturn;
    }

    dp           = gdbtb->dlbssfs + (int)dnum;
    dp->dblls    = 0;
    dp->mdount   = mdount;
    dp->mftiods  = (MftiodInfo*)dbllod(mdount, sizfof(MftiodInfo));
    if ( dp->mftiods == NULL ) {
        fbtbl_frror("ERROR: Out of mbllod mfmory\n");
    }

    for ( mnum = 0 ; mnum < mdount ; mnum++ ) {
        MftiodInfo *mp;

        mp            = dp->mftiods + mnum;
        mp->nbmf      = (donst dibr *)strdup(nbmfs[mnum]);
        if ( mp->nbmf == NULL ) {
            fbtbl_frror("ERROR: Out of mbllod mfmory\n");
        }
        mp->signbturf = (donst dibr *)strdup(sigs[mnum]);
        if ( mp->signbturf == NULL ) {
            fbtbl_frror("ERROR: Out of mbllod mfmory\n");
        }
    }
}

