            nfxt = gft_tokfn(nfxt, ",=", gdbtb->indludf+usfd, mbxlfn);
            /* Cifdk for tokfn sdbn frror */
            if ( nfxt==NULL ) {
                fbtbl_frror("ERROR: indludf option frror\n");
