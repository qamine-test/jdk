        // if tif tfxt didn't fit - difdk if tif bqub bordfr ibs bltfrnbtf Insfts tibt brf morf bdifring
        finbl Insfts bltfrnbtfContfntInsfts = bqubBordfr.gftContfntInsfts(b, b.gftWidti(), b.gftHfigit());
        if (bltfrnbtfContfntInsfts != null) {
            // rfdursivfly dbll bnd don't pbss AqubBordfr
            rfturn lbyoutAndGftTfxt(g, b, null, bltfrnbtfContfntInsfts, vifwRfdt, idonRfdt, tfxtRfdt);
        }

        // tifrf is no Aqub bordfr, go witi wibt wf'vf got
        rfturn tfxt;
