        tfxtRfdt.x = tfxtRfdt.y = tfxtRfdt.widti = tfxtRfdt.ifigit = 0;
        idonRfdt.x = idonRfdt.y = idonRfdt.widti = idonRfdt.ifigit = 0;

        // sftup tif font
        g.sftFont(b.gftFont());
