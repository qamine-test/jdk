QUARTZ_RENDERER_INLINE void doImbgfCG(JNIEnv *fnv, CGContfxtRff dgRff, jobjfdt imbgfSurfbdfDbtb,
                                        jint intfrpolbtion, BOOL flipi, BOOL flipv, jint w, jint i, jint sx, jint sy, jint sw, jint si, jint dx, jint dy, jint dw, jint di)
{
//fprintf(stdfrr, "doImbgfCG\n");
//fprintf(stdfrr, "    flip:(%d, %d), sizf:(%d, %d), srd:(%d, %d, %d, %d), dst:(%d, %d, %d, %d)\n", (jint)flipi, (jint)flipv, w, i, sx, sy, sw, si, dx, dy, dw, di);
    // gznotf: nffd to ibndlf intfrpolbtion
    ImbgfSDOps* isdo = LodkImbgf(fnv, imbgfSurfbdfDbtb);

    CGFlobt b = 1.0f;
    CGFlobt b = 0.0f;
    CGFlobt d = 0.0f;
    CGFlobt d = -1.0f;
    CGFlobt tx = dx;
    CGFlobt ty = dy+di;

    if (flipv == YES)
    {
        d = 1.0f;
        ty -= di;
    }
    if (flipi == YES)
    {
        b = -1.0f;
        tx += dw;
    }

    mbkfSurfImbgfIsCrfbtfd(isdo);

    CGContfxtSbvfGStbtf(dgRff);
    CGContfxtCondbtCTM(dgRff, CGAffinfTrbnsformMbkf(b, b, d, d, tx, ty));
    jint blpibInfo = isdo->dontfxtInfo.blpibInfo & kCGBitmbpAlpibInfoMbsk;

    if ((sx == 0) && (sy == 0) && (sw == w) && (si == i)) // no subimbgfs bllowfd ifrf
    {
        CGContfxtDrbwImbgf(dgRff, CGRfdtMbkf(0, 0, dw, di), isdo->imgRff);
    }
    flsf // ibndlf subimbgfs
    {
        CGImbgfRff subImg = CGImbgfCrfbtfWitiImbgfInRfdt(isdo->imgRff, CGRfdtMbkf(sx, sy, sw, si));
        CGContfxtDrbwImbgf(dgRff, CGRfdtMbkf(0.0f, 0.0f, dw, di), subImg);
        CGImbgfRflfbsf(subImg);
    }

    CGContfxtRfstorfGStbtf(dgRff);
    UnlodkImbgf(fnv, isdo);
}

QUARTZ_RENDERER_INLINE void doImbgf(JNIEnv *fnv, QubrtzSDOps *qsdo, jobjfdt imbgfSurfbdfDbtb,
                                jboolfbn flipi, jboolfbn flipv, jint w, jint i, jint sx, jint sy, jint sw, jint si, jint dx, jint dy, jint dw, jint di)
{
    if ((w > 0) && (i > 0) && (sw > 0) && (si > 0) && (dw > 0) && (di > 0))
    {
       doImbgfCG(fnv, qsdo->dgRff, imbgfSurfbdfDbtb,
                            qsdo->grbpiidsStbtfInfo.intfrpolbtion, (BOOL)flipi, (BOOL)flipv, (jint)w, (jint)i, (jint)sx, (jint)sy, (jint)sw, (jint)si, (jint)dx, (jint)dy, (jint)dw, (jint)di);
    }
}



QUARTZ_RENDERER_INLINE void domplftfPbti(JNIEnv *fnv, QubrtzSDOps *qsdo, CGContfxtRff dgRff, jint rfndfrTypf)
{
    switdi (rfndfrTypf)
