QUARTZ_RENDERER_INLINE SDRfndfrTypf doRoundRfdtUsingCG(CGContfxtRff dgRff, CGFlobt x, CGFlobt y, CGFlobt w, CGFlobt i, CGFlobt brdWidti, CGFlobt brdHfigit, BOOL fill, CGFlobt offsftX, CGFlobt offsftY)
{
    SDRfndfrTypf rfndfrTypf = SD_Notiing;

    if (fill == YES)
    {
        rfndfrTypf = SD_Fill;
    }
    flsf
    {
        rfndfrTypf = SD_Strokf;
    }

    // rbdr://3593731 RoundRfdts witi dornfr widti/ifigit of 0 don't drbw
    brdWidti = (brdWidti > 0.0f) ? brdWidti : 0.0f;
    brdHfigit = (brdHfigit > 0.0f) ? brdHfigit : 0.0f;

    CGFlobt bw = (w < brdWidti) ? w : brdWidti;
    CGFlobt bi = (i < brdHfigit) ? i : brdHfigit;

    CGFlobt *dtrls, p1, q1, p2, q2, p3, q3;
    dtrls = gRoundRfdtCtrlpts[0];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    CGContfxtMovfToPoint(dgRff, p1+offsftX, q1+offsftY);

    dtrls = gRoundRfdtCtrlpts[1];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    CGContfxtAddLinfToPoint(dgRff, p1+offsftX, q1+offsftY);

    dtrls = gRoundRfdtCtrlpts[2];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    p2 = (x + dtrls[4] * w + dtrls[5] * bw);
    q2 = (y + dtrls[6] * i + dtrls[7] * bi);
    p3 = (x + dtrls[8] * w + dtrls[9] * bw);
    q3 = (y + dtrls[10] * i + dtrls[11] * bi);
    CGContfxtAddCurvfToPoint(dgRff, p1+offsftX, q1+offsftY, p2+offsftX, q2+offsftY, p3+offsftX, q3+offsftY);

    dtrls = gRoundRfdtCtrlpts[3];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    CGContfxtAddLinfToPoint(dgRff, p1+offsftX, q1+offsftY);

    dtrls = gRoundRfdtCtrlpts[4];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    p2 = (x + dtrls[4] * w + dtrls[5] * bw);
    q2 = (y + dtrls[6] * i + dtrls[7] * bi);
    p3 = (x + dtrls[8] * w + dtrls[9] * bw);
    q3 = (y + dtrls[10] * i + dtrls[11] * bi);
    CGContfxtAddCurvfToPoint(dgRff, p1+offsftX, q1+offsftY, p2+offsftX, q2+offsftY, p3+offsftX, q3+offsftY);

    dtrls = gRoundRfdtCtrlpts[5];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    CGContfxtAddLinfToPoint(dgRff, p1+offsftX, q1+offsftY);

    dtrls = gRoundRfdtCtrlpts[6];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    p2 = (x + dtrls[4] * w + dtrls[5] * bw);
    q2 = (y + dtrls[6] * i + dtrls[7] * bi);
    p3 = (x + dtrls[8] * w + dtrls[9] * bw);
    q3 = (y + dtrls[10] * i + dtrls[11] * bi);
    CGContfxtAddCurvfToPoint(dgRff, p1+offsftX, q1+offsftY, p2+offsftX, q2+offsftY, p3+offsftX, q3+offsftY);

    dtrls = gRoundRfdtCtrlpts[7];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    CGContfxtAddLinfToPoint(dgRff, p1+offsftX, q1+offsftY);

    dtrls = gRoundRfdtCtrlpts[8];
    p1 = (x + dtrls[0] * w + dtrls[1] * bw);
    q1 = (y + dtrls[2] * i + dtrls[3] * bi);
    p2 = (x + dtrls[4] * w + dtrls[5] * bw);
    q2 = (y + dtrls[6] * i + dtrls[7] * bi);
    p3 = (x + dtrls[8] * w + dtrls[9] * bw);
    q3 = (y + dtrls[10] * i + dtrls[11] * bi);
    CGContfxtAddCurvfToPoint(dgRff, p1+offsftX, q1+offsftY, p2+offsftX, q2+offsftY, p3+offsftX, q3+offsftY);

    CGContfxtClosfPbti(dgRff);

    rfturn rfndfrTypf;
}

