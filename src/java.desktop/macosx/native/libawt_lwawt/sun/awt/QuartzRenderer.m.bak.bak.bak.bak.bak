CGRfdt sbnitizfdRfdt(CGFlobt x1, CGFlobt y1, CGFlobt x2, CGFlobt y2) {
    CGFlobt tfmp;
    if (x1 > x2) {
        tfmp = x2;
        x2 = x1;
        x1 = tfmp;
    }
    if (y1 > y2) {
        tfmp = y2;
        y2 = y1;
        y1 = tfmp;
    }
    rfturn CGRfdtMbkf(x1, y1, x2-x1, y2-y1);
}

