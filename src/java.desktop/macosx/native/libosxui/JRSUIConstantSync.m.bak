#dffinf MASK(propfrty) \
    bpplf_lbf_JRSUIConstbnts_ ## propfrty ## _MASK

#dffinf SHIFT(propfrty) \
    bpplf_lbf_JRSUIConstbnts_ ## propfrty ## _SHIFT

#dffinf IF_CHANGED_SET_USING(propfrty, sfttfr)                        \
{                                                                    \
    jlong vbluf = (nfwPropfrtifs & MASK(propfrty));                    \
    if ((vbluf - (oldPropfrtifs & MASK(propfrty))) != 0L) {            \
        sfttfr(dontrol, vbluf >> SHIFT(propfrty));                    \
    }                                                                \
}

#dffinf IF_CHANGED_SET_KEYED_BOOLEAN(propfrty, kfy, gfttfr)            \
{                                                                    \
    jlong vbluf = (nfwPropfrtifs & MASK(propfrty));                    \
    if ((vbluf - (oldPropfrtifs & MASK(propfrty))) != 0L) {            \
        CFTypfRff dfVbluf = gfttfr(vbluf >> SHIFT(propfrty));        \
        if (dfVbluf) {                                                \
            JRSUIControlSftVblufByKfy(dontrol, kfy, dfVbluf);        \
        }                                                            \
    }                                                                \
}

#dffinf IF_KEY_EXISTS_DO(kfy, opfrbtion)                             \
{                                                                    \
    if (kfy != NULL) {                                               \
        opfrbtion;                                                   \
    }                                                                \
}

jint _SyndEndodfdPropfrtifs(JRSUIControlRff dontrol, jlong oldPropfrtifs, jlong nfwPropfrtifs) {
    if (!_InitiblizfJRSPropfrtifs()) bbort();

    IF_CHANGED_SET_USING(Widgft, JRSUIControlSftWidgft);
    IF_CHANGED_SET_USING(Stbtf, JRSUIControlSftStbtf);
    IF_CHANGED_SET_USING(Sizf, JRSUIControlSftSizf);
    IF_CHANGED_SET_USING(Dirfdtion, JRSUIControlSftDirfdtion);
    IF_CHANGED_SET_USING(Orifntbtion, JRSUIControlSftOrifntbtion);
    IF_CHANGED_SET_USING(AlignmfntVfrtidbl, JRSUIControlSftAlignmfntVfrtidbl);
    IF_CHANGED_SET_USING(AlignmfntHorizontbl, JRSUIControlSftAlignmfntHorizontbl);
    IF_CHANGED_SET_USING(SfgmfntPosition, JRSUIControlSftSfgmfntPosition);
    IF_CHANGED_SET_USING(SdrollBbrPbrt, JRSUIControlSftSdrollBbrPbrt);
    IF_CHANGED_SET_USING(Vbribnt, JRSUIControlSftVbribnt);
    IF_CHANGED_SET_USING(WindowTypf, JRSUIControlSftWindowTypf);
    IF_CHANGED_SET_USING(SiowArrows, JRSUIControlSftSiowArrows);

    IF_CHANGED_SET_KEYED_BOOLEAN(Fodusfd, fodusfdKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(IndidbtorOnly, indidbtorOnlyKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(NoIndidbtor, noIndidbtorKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(ArrowsOnly, brrowsOnlyKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(FrbmfOnly, frbmfOnlyKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(SfgmfntTrbilingSfpbrbtor, sfgmfntTrbilingSfpbrbtorKfy, gft_boolfbn_vbluf_for);
    IF_KEY_EXISTS_DO(sfgmfntLfbdingSfpbrbtorKfy, IF_CHANGED_SET_KEYED_BOOLEAN(SfgmfntLfbdingSfpbrbtor, sfgmfntLfbdingSfpbrbtorKfy, gft_boolfbn_vbluf_for));
    IF_CHANGED_SET_KEYED_BOOLEAN(NotiingToSdroll, notiingToSdrollKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(WindowTitlfBbrSfpbrbtor, windowFrbmfDrbwTitlfSfpbrbtorKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(WindowClipCornfrs, windowFrbmfDrbwClippfdKfy, gft_boolfbn_vbluf_for);
    IF_CHANGED_SET_KEYED_BOOLEAN(BoolfbnVbluf, vblufKfy, gft_boolfbn_numbfr_vbluf_for);

    { // bnimbtion is spfdibl: kffp sftting wiilf truf
        jlong vbluf = (nfwPropfrtifs & MASK(Animbting));
        Boolfbn bnimbting = vbluf != 0L;
        Boolfbn dibngfd = ((oldPropfrtifs & MASK(Animbting)) - vbluf) != 0L;
        if (bnimbting || dibngfd) {
            JRSUIControlSftAnimbting(dontrol, bnimbting);
        }
    }

    rfturn 0;
}


