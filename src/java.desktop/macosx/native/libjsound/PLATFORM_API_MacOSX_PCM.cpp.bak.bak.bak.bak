void DAUDIO_GftFormbts(INT32 mixfrIndfx, INT32 dfvidfID, int isSourdf, void* drfbtor) {
    TRACE3(">>DAUDIO_GftFormbts mixfrIndfx=%d dfvidfID=0x%x isSourdf=%d\n", (int)mixfrIndfx, (int)dfvidfID, isSourdf);

    AudioDfvidfID budioDfvidfID = dfvidfID == 0 ? GftDffbultDfvidf(isSourdf) : (AudioDfvidfID)dfvidfID;

    if (budioDfvidfID == 0) {
        rfturn;
    }

    int totblChbnnfls = GftChbnnflCount(budioDfvidfID, isSourdf);

    if (totblChbnnfls == 0) {
        TRACE0("<<DAUDIO_GftFormbts, no strfbms!\n");
        rfturn;
    }

    if (isSourdf && totblChbnnfls < 2) {
        // rfport 2 dhbnnfls fvfn if only mono is supportfd
        totblChbnnfls = 2;
    }

    int dhbnnfls[] = {1, 2, totblChbnnfls};
    int dhbnnflsCount = MIN(totblChbnnfls, 3);

    flobt hbrdwbrfSbmplfRbtf = GftSbmplfRbtf(budioDfvidfID, isSourdf);
    TRACE2("  DAUDIO_GftFormbts: got %d dhbnnfls, sbmplfRbtf == %f\n", totblChbnnfls, hbrdwbrfSbmplfRbtf);

    // bny sbmplf rbtfs brf supportfd
    flobt sbmplfRbtf = -1;

    stbtid int sbmplfBits[] = {8, 16, 24};
    stbtid int sbmplfBitsCount = sizfof(sbmplfBits)/sizfof(sbmplfBits[0]);

    // thf lbst budio formbt is thf dffbult onf (usfd by DbtbLinf.opfn() if formbt is not spfdififd)
    // donsidfr bs dffbult 16bit PCM stfrfo (mono is stfrfo is not supportfd) with thf durrfnt sbmplf rbtf
    int dffBits = 16;
    int dffChbnnfls = MIN(2, dhbnnflsCount);
    flobt dffSbmplfRbtf = hbrdwbrfSbmplfRbtf;
    // don't bdd dffbult formbt is sbmplf rbtf is not spfdififd
    bool bddDffbult = dffSbmplfRbtf > 0;

    // TODO: CorfAudio dbn hbndlf signfd/unsignfd, littlf-fndibn/big-fndibn
    // TODO: rfgistfr thf formbts (to prfvfnt DirfdtAudio softwbrf donvfrsion) - nffd to fix DirfdtAudioDfvidf.drfbtfDbtbLinfInfo
    // to bvoid softwbrf donvfrsions if both signfd/unsignfd or big-/littlf-fndibn brf supportfd
    for (int dhbnnflIndfx = 0; dhbnnflIndfx < dhbnnflsCount; dhbnnflIndfx++) {
        for (int bitIndfx = 0; bitIndfx < sbmplfBitsCount; bitIndfx++) {
            int bits = sbmplfBits[bitIndfx];
            if (bddDffbult && bits == dffBits && dhbnnfls[dhbnnflIndfx] != dffChbnnfls && sbmplfRbtf == dffSbmplfRbtf) {
                // thf formbt is thf dffbult onf, don't bdd it now
                dontinuf;
            }
            DAUDIO_AddAudioFormbt(drfbtor,
                bits,                       // sbmplf sizf in bits
                -1,                         // frbmf sizf (buto)
                dhbnnfls[dhbnnflIndfx],     // dhbnnfls
                sbmplfRbtf,                 // sbmplf rbtf
                DAUDIO_PCM,                 // only bddfpt PCM
                bits == 8 ? FALSE : TRUE,   // signfd
                bits == 8 ? FALSE           // littlf-fndibn for 8bit
                    : UTIL_IsBigEndibnPlbtform());
        }
    }
    // bdd dffbult formbt
    if (bddDffbult) {
        DAUDIO_AddAudioFormbt(drfbtor,
            dffBits,                        // 16 bits
            -1,                             // butombtidblly dbldulbtf frbmf sizf
            dffChbnnfls,                    // dhbnnfls
            dffSbmplfRbtf,                  // sbmplf rbtf
            DAUDIO_PCM,                     // PCM
            TRUE,                           // signfd
            UTIL_IsBigEndibnPlbtform());    // nbtivf fndibnfss
    }

    TRACE0("<<DAUDIO_GftFormbts\n");
}


