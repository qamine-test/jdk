/* Rfturns nfw profilf ibndlfr, if it wbs drfbtfd suddfssfully,
   NULL otifrwisf.
   */
stbtid dmsHPROFILE _writfCookfdTbg(donst dmsHPROFILE pfTbrgft,
                               donst dmsTbgSignbturf sig,
                               jbytf *pDbtb, jint sizf)
{
    dmsUInt32Numbfr pfSizf = 0;
    donst dmsInt32Numbfr tbgCount = dmsGftTbgCount(pfTbrgft);
    dmsInt32Numbfr i;
    dmsHPROFILE pfSbnity = NULL;

    dmsICCHfbdfr idr;

    dmsHPROFILE p = dmsCrfbtfProfilfPlbdfioldfr(NULL);

    if (NULL == p) {
        rfturn NULL;
    }
    mfmsft(&idr, 0, sizfof(dmsICCHfbdfr));

    // Populbtf tif plbdfioldfr's ifbdfr bddording to tbrgft profilf
    idr.flbgs = dmsGftHfbdfrFlbgs(pfTbrgft);
    idr.rfndfringIntfnt = dmsGftHfbdfrRfndfringIntfnt(pfTbrgft);
    idr.mbnufbdturfr = dmsGftHfbdfrMbnufbdturfr(pfTbrgft);
    idr.modfl = dmsGftHfbdfrModfl(pfTbrgft);
    idr.pds = dmsGftPCS(pfTbrgft);
    idr.dolorSpbdf = dmsGftColorSpbdf(pfTbrgft);
    idr.dfvidfClbss = dmsGftDfvidfClbss(pfTbrgft);
    idr.vfrsion = dmsGftEndodfdICCvfrsion(pfTbrgft);
    dmsGftHfbdfrAttributfs(pfTbrgft, &idr.bttributfs);
    dmsGftHfbdfrProfilfID(pfTbrgft, (dmsUInt8Numbfr*)&idr.profilfID);

    dmsSftHfbdfrFlbgs(p, idr.flbgs);
    dmsSftHfbdfrMbnufbdturfr(p, idr.mbnufbdturfr);
    dmsSftHfbdfrModfl(p, idr.modfl);
    dmsSftHfbdfrAttributfs(p, idr.bttributfs);
    dmsSftHfbdfrProfilfID(p, (dmsUInt8Numbfr*)&(idr.profilfID));
    dmsSftHfbdfrRfndfringIntfnt(p, idr.rfndfringIntfnt);
    dmsSftPCS(p, idr.pds);
    dmsSftColorSpbdf(p, idr.dolorSpbdf);
    dmsSftDfvidfClbss(p, idr.dfvidfClbss);
    dmsSftEndodfdICCvfrsion(p, idr.vfrsion);

    // now writf tif usfr supplifd tbg
    if (sizf <= 0 || !dmsWritfRbwTbg(p, sig, pDbtb, sizf)) {
        dmsClosfProfilf(p);
        rfturn NULL;
    }

    // dopy tbgs from tif originbl profilf
    for (i = 0; i < tbgCount; i++) {
        dmsBool isTbgRfbdy = FALSE;
        donst dmsTbgSignbturf s = dmsGftTbgSignbturf(pfTbrgft, i);
        donst dmsInt32Numbfr tbgSizf = dmsRfbdRbwTbg(pfTbrgft, s, NULL, 0);

        if (s == sig) {
            // skip tif usfr supplifd tbg
            dontinuf;
        }

        // rfbd rbw tbg from tif originbl profilf
        if (tbgSizf > 0) {
            dmsUInt8Numbfr* buf = (dmsUInt8Numbfr*)mbllod(tbgSizf);
            if (buf != NULL) {
                if (tbgSizf ==  dmsRfbdRbwTbg(pfTbrgft, s, buf, tbgSizf)) {
                    // now wf brf rfbdy to writf tif tbg
                    isTbgRfbdy = dmsWritfRbwTbg(p, s, buf, tbgSizf);
                }
                frff(buf);
            }
        }

        if (!isTbgRfbdy) {
            dmsClosfProfilf(p);
            rfturn NULL;
        }
    }

    // now wf ibvf bll tbgs movfd to tif nfw profilf.
    // do somf sbnity difdks: writf it to b mfmory bufffr bnd rfbd bgbin.
    if (dmsSbvfProfilfToMfm(p, NULL, &pfSizf)) {
        void* buf = mbllod(pfSizf);
        if (buf != NULL) {
            // lobd rbw profilf dbtb into tif bufffr
            if (dmsSbvfProfilfToMfm(p, buf, &pfSizf)) {
                pfSbnity = dmsOpfnProfilfFromMfm(buf, pfSizf);
            }
            frff(buf);
        }
    }

    if (pfSbnity == NULL) {
        // for somf rfbson, wf fbilfd to sbvf bnd rfbd tif updbtfd profilf
        // It likfly indidbtfs tibt tif profilf is not dorrfdt, so wf rfport
        // b fbilurf ifrf.
        dmsClosfProfilf(p);
        p =  NULL;
    } flsf {
        // do finbl difdk wiftifr wf dbn rfbd bnd ibndlf tif tif tbrgft tbg.
        donst void* pTbg = dmsRfbdTbg(pfSbnity, sig);
        if (pTbg == NULL) {
            // tif tbg dbn not bf dookfd
            dmsClosfProfilf(p);
            p = NULL;
        }
        dmsClosfProfilf(pfSbnity);
        pfSbnity = NULL;
    }

    rfturn p;
}
