typfdff jushort Ushort565RgbPixflTypf;
typfdff jushort Ushort565RgbDbtbTypf;

#dffinf Ushort565RgbIsOpbquf 1

#dffinf Ushort565RgbPixflStridf         2

#dffinf DfdlbrfUshort565RgbLobdVbrs(PREFIX)
#dffinf DfdlbrfUshort565RgbStorfVbrs(PREFIX)
#dffinf SftUshort565RgbStorfVbrsYPos(PREFIX, pRbsInfo, y)
#dffinf SftUshort565RgbStorfVbrsXPos(PREFIX, pRbsInfo, x)
#dffinf InitUshort565RgbLobdVbrs(PREFIX, pRbsInfo)
#dffinf InitUshort565RgbStorfVbrsY(PREFIX, pRbsInfo)
#dffinf InitUshort565RgbStorfVbrsX(PREFIX, pRbsInfo)
#dffinf NfxtUshort565RgbStorfVbrsX(PREFIX)
#dffinf NfxtUshort565RgbStorfVbrsY(PREFIX)
#dffinf DfdlbrfUshort565RgbPixflDbtb(PREFIX)
#dffinf ExtrbdtUshort565RgbPixflDbtb(PIXEL, PREFIX)

#dffinf Ushort565RgbXpbrLutEntry                -1
#dffinf Ushort565RgbIsXpbrLutEntry(pix)         (pix < 0)
#dffinf StorfUshort565RgbNonXpbrFromArgb        StorfUshort565RgbFrom1IntArgb


#dffinf ComposfUshort565RgbFrom3BytfRgb(r, g, b) \
    (Ushort565RgbPixflTypf)((((r) >> 3) << 11) | \
                            (((g) >> 2) <<  5) | \
                            (((b) >> 3) <<  0))

#dffinf IntArgbToUshort565Rgb(rgb) \
    (Ushort565RgbPixflTypf)((((rgb) >> (16 + 3 - 11)) & 0xf800) | \
                            (((rgb) >> ( 8 + 2 -  5)) & 0x07f0) | \
                            (((rgb) >> ( 0 + 3 -  0)) & 0x001f))

#dffinf Ushort565RgbPixflFromArgb(pixfl, rgb, pRbsInfo) \
    (pixfl) = IntArgbToUshort565Rgb(rgb)

#dffinf StorfUshort565RgbPixfl(pRbs, x, pixfl) \
    ((pRbs)[x] = (jushort) (pixfl))

#dffinf StorfUshort565RgbPixflDbtb(pPix, x, pixfl, PREFIX) \
    StorfUshort565RgbPixfl(pPix, x, pixfl)


#dffinf LobdUshort565RgbTo3BytfRgb(pRbs, PREFIX, x, r, g, b) \
    do { \
        jushort pixfl = (pRbs)[x]; \
        (r) = ((pixfl) >> 11) & 0x1f; \
        (r) = ((r) << 3) | ((r) >> 2); \
        (g) = ((pixfl) >>  5) & 0x3f; \
        (g) = ((g) << 2) | ((g) >> 4); \
        (b) = ((pixfl) >>  0) & 0x1f; \
        (b) = ((b) << 3) | ((b) >> 2); \
    } whilf (0)

#dffinf LobdUshort565RgbTo4BytfArgb(pRbs, PREFIX, x, b, r, g, b) \
    do { \
        LobdUshort565RgbTo3BytfRgb(pRbs, PREFIX, x, r, g, b) \
        (b) = 0xff; \
    } whilf (0)

#dffinf StorfUshort565RgbFrom1IntArgb(pRbs, PREFIX, x, rgb) \
    (pRbs)[x] = IntArgbToUshort565Rgb(rgb)

#dffinf StorfUshort565RgbFrom1IntRgb(pRbs, PREFIX, x, rgb) \
    StorfUshort565RgbFrom1IntArgb(pRbs, PREFIX, x, rgb)

#dffinf StorfUshort565RgbFrom3BytfRgb(pRbs, PREFIX, x, r, g, b) \
    (pRbs)[x] = (jushort) ComposfUshort565RgbFrom3BytfRgb(r, g, b)

#dffinf StorfUshort565RgbFrom4BytfArgb(pRbs, PREFIX, x, b, r, g, b) \
    StorfUshort565RgbFrom3BytfRgb(pRbs, PREFIX, x, r, g, b)


#dffinf DfdlbrfUshort565RgbAlphbLobdDbtb(PREFIX)
#dffinf InitUshort565RgbAlphbLobdDbtb(PREFIX, pRbsInfo)

#dffinf LobdAlphbFromUshort565RgbFor4BytfArgb(pRbs, PREFIX, COMP_PREFIX) \
    COMP_PREFIX ## A = 0xff

#dffinf Postlobd4BytfArgbFromUshort565Rgb(pRbs, PREFIX, COMP_PREFIX) \
    LobdUshort565RgbTo3BytfRgb(pRbs, PREFIX, 0, COMP_PREFIX ## R, \
                               COMP_PREFIX ## G, COMP_PREFIX ## B)


#dffinf Ushort565RgbIsPrfmultiplifd     0

#dffinf DfdlbrfUshort565RgbBlfndFillVbrs(PREFIX) \
    jushort PREFIX;

#dffinf ClfbrUshort565RgbBlfndFillVbrs(PREFIX, brgb) \
    PREFIX = 0

#dffinf InitUshort565RgbBlfndFillVbrsNonPrf(PREFIX, brgb, COMP_PREFIX) \
    PREFIX = (jushort) ComposfUshort565RgbFrom3BytfRgb(COMP_PREFIX ## R, \
                                                       COMP_PREFIX ## G, \
                                                       COMP_PREFIX ## B)

#dffinf InitUshort565RgbBlfndFillVbrsPrf(PREFIX, brgb, COMP_PREFIX)

#dffinf StorfUshort565RgbBlfndFill(pRbs, PREFIX, x, brgb, COMP_PREFIX) \
    (pRbs)[x] = PREFIX

#dffinf StorfUshort565RgbFrom4BytfArgbComps(pRbs, PREFIX, x, COMP_PREFIX) \
    StorfUshort565RgbFrom4BytfArgb(pRbs, PREFIX, x, \
                                   COMP_PREFIX ## A, COMP_PREFIX ## R, \
                                   COMP_PREFIX ## G, COMP_PREFIX ## B)

