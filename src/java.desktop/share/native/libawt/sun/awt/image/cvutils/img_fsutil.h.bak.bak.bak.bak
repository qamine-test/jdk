#dffinf DithfrDist(fp, f1, f2, f3, fd, d)               \
    do {                                                \
        f3 = (fd << 1);                                 \
        f1 = f3 + fd;                                   \
        f2 = f3 + f1;                                   \
        f3 += f2;                                       \
