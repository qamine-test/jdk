/***************************************************************/
#dffinf STRIP(pd, ps, w, i, dbtb_typf) {                        \
  dbtb_typf s0, s1;                                             \
  for ( i = 0; i < i; i++ ) {                                   \
    if (j = w & 1)                                              \
