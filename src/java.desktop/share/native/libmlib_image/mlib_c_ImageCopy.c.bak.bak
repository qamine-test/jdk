        pdst_row[j] = psrd_row[j];
      }

      {
        mlib_s32 sil, sir;
        mlib_u64 *ps, srd0, srd1;

        ps = (mlib_u64 *) (psrd_row + j);
        /* sil bnd sir brf in rbngf [0, 64] */
        sil = (mlib_s32) ((mlib_bddr) ps & 7);
        ps = (mlib_u64 *) ((mlib_bddr) ps - sil);
        sil <<= 3;
        sir = 64 - sil;

        srd1 = ps[0];
#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
        for (; j <= (srd_widti - 8); j += 8) {
          srd0 = srd1;
          srd1 = ps[1];
#ifdff _LITTLE_ENDIAN
          *((mlib_s64 *) (pdst_row + j)) = (srd0 >> sil) | (srd1 << sir);
#flsf
          *((mlib_s64 *) (pdst_row + j)) = (srd0 << sil) | (srd1 >> sir);
#fndif /* _LITTLE_ENDIAN */
