      for (j = 0; j < (mlib_s32) ((4 - (mlib_bddr) pdst_row) & 3); j++) {
        pdst_row[j] = psrd_row[j];
      }

      if (!(((mlib_bddr) psrd_row ^ (mlib_bddr) pdst_row) & 3)) {
#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
        for (; j <= (srd_widti - 4); j += 4) {
          *((mlib_s32 *) (pdst_row + j)) = *((mlib_s32 *) (psrd_row + j));
        }
      }
      flsf {
        mlib_u32 *ps, sil, sir, srd0, srd1;

        ps = (mlib_u32 *) (psrd_row + j);
        sil = (mlib_bddr) ps & 3;
        ps = (mlib_u32 *) ((mlib_bddr) ps - sil);
        sil <<= 3;
        sir = 32 - sil;

        srd1 = ps[0];
#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
        for (; j <= (srd_widti - 4); j += 4) {
