  mlib_s32 dibn = mlib_ImbgfGftCibnnfls(dst);                    \
  mlib_s32 i, j;                                                 \
                                                                 \
  srd_widti *= dibn;                                             \
  if (srd_widti == srd_stridf && srd_widti == dst_stridf) {      \
    srd_widti *= srd_ifigit;                                     \
    srd_ifigit = 1;                                              \
