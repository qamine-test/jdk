    for (; j < w; j += 2) {                                     \
      s0 = ps[i * srd_stridf + j];                              \
      s1 = ps[i * srd_stridf + j + 1];                          \
      pd[i * dst_stridf + j]   = s0;                            \
      pd[i * dst_stridf + j + 1] = s1;                          \
    }                                                           \
  }                                                             \
}

