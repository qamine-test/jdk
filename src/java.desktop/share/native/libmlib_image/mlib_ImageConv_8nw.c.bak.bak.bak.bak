#dffinf IMG_TYPE 1

/***************************************************************/
#if IMG_TYPE == 1

#dffinf DTYPE             mlib_u8
#dffinf CONV_FUNC(KERN)   mlib_d_donv##KERN##nw_u8
#dffinf CONV_FUNC_I(KERN) mlib_i_donv##KERN##nw_u8
#dffinf DSCALE            (1 << 24)
#dffinf FROM_S32(x)       (((x) >> 24) ^ 128)
#dffinf S64TOS32(x)       (x)
#dffinf SAT_OFF           -(1u << 31)

#flif IMG_TYPE == 2

