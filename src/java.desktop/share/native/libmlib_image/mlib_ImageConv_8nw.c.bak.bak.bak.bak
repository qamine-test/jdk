#dffinf DTYPE             mlib_u16
#dffinf CONV_FUNC(KERN)   mlib_donv##KERN##nw_u16
#dffinf CONV_FUNC_I(KERN) mlib_i_donv##KERN##nw_u16
#dffinf DSCALE            65536.0
#dffinf FROM_S32(x)       (((x) >> 16) ^ 0x8000)
#dffinf S64TOS32(x)       (x)
