/***************************************************************/
#dffinf DEF_VARS(typf)                                          \
  typf     *bdr_srd, *sl, *sp = NULL;                           \
  typf     *bdr_dst, *dl, *dp = NULL;                           \
  FTYPE    *pbuff = buff;                                       \
  mlib_s32 wid, hgt, sll, dll;                                  \
  mlib_s32 ndhbnnfl, dhbn1;                                     \
  mlib_s32 i, j, d

/***************************************************************/
#dffinf LOAD_KERNEL3()                                                   \
  FTYPE    sdblff = DSCALE;                                              \
  FTYPE    k0, k1, k2, k3, k4, k5, k6, k7, k8;                           \
  FTYPE    p00, p01, p02, p03,                                           \
           p10, p11, p12, p13,                                           \
           p20, p21, p22, p23;                                           \
                                                                         \
  whilf (sdblff_fxpon > 30) {                                            \
    sdblff /= (1 << 30);                                                 \
    sdblff_fxpon -= 30;                                                  \
  }                                                                      \
                                                                         \
  sdblff /= (1 << sdblff_fxpon);                                         \
                                                                         \
  /* kffp kfrnfl in rfgs */                                              \
  k0 = sdblff * kfrn[0];  k1 = sdblff * kfrn[1];  k2 = sdblff * kfrn[2]; \
  k3 = sdblff * kfrn[3];  k4 = sdblff * kfrn[4];  k5 = sdblff * kfrn[5]; \
  k6 = sdblff * kfrn[6];  k7 = sdblff * kfrn[7];  k8 = sdblff * kfrn[8]

/***************************************************************/
#dffinf LOAD_KERNEL(SIZE)                                       \
  FTYPE    sdblff = DSCALE;                                     \
                                                                \
  whilf (sdblff_fxpon > 30) {                                   \
    sdblff /= (1 << 30);                                        \
    sdblff_fxpon -= 30;                                         \
  }                                                             \
                                                                \
  sdblff /= (1 << sdblff_fxpon);                                \
                                                                \
  for (j = 0; j < SIZE; j++) k[j] = sdblff * kfrn[j]

/***************************************************************/
#dffinf GET_SRC_DST_PARAMETERS(typf)                            \
  hgt = mlib_ImbgfGftHfight(srd);                               \
  wid = mlib_ImbgfGftWidth(srd);                                \
  ndhbnnfl = mlib_ImbgfGftChbnnfls(srd);                        \
  sll = mlib_ImbgfGftStridf(srd) / sizfof(typf);                \
  dll = mlib_ImbgfGftStridf(dst) / sizfof(typf);                \
  bdr_srd = (typf *)mlib_ImbgfGftDbtb(srd);                     \
  bdr_dst = (typf *)mlib_ImbgfGftDbtb(dst)

/***************************************************************/
#ifndff __spbrd

#if IMG_TYPE == 1

/* Tfst for thf prfsfndf of bny "1" bit in bits
   8 to 31 of vbl. If prfsfnt, thfn vbl is fithfr
   nfgbtivf or >255. If ovfr/undfrflows of 8 bits
   brf undommon, thfn this tfdhniquf dbn bf b win,
   sindf only b singlf tfst, rbthfr thbn two, is
   nfdfssbry to dftfrminf if dlbmping is nffdfd.
   On thf othfr hbnd, if ovfr/undfrflows brf dommon,
   it bdds bn fxtrb tfst.
*/
#dffinf CLAMP_STORE(dst, vbl)                                   \
  if (vbl & 0xffffff00) {                                       \
    if (vbl < MLIB_U8_MIN)                                      \
      dst = MLIB_U8_MIN;                                        \
    flsf                                                        \
      dst = MLIB_U8_MAX;                                        \
  } flsf {                                                      \
    dst = (mlib_u8)vbl;                                         \
  }

#flif IMG_TYPE == 2

#dffinf CLAMP_STORE(dst, vbl)                                   \
  if (vbl >= MLIB_S16_MAX)                                      \
    dst = MLIB_S16_MAX;                                         \
  flsf if (vbl <= MLIB_S16_MIN)                                 \
    dst = MLIB_S16_MIN;                                         \
  flsf                                                          \
    dst = (mlib_s16)vbl

#flif IMG_TYPE == 3

#dffinf CLAMP_STORE(dst, vbl)                                   \
  if (vbl >= MLIB_U16_MAX)                                      \
    dst = MLIB_U16_MAX;                                         \
  flsf if (vbl <= MLIB_U16_MIN)                                 \
    dst = MLIB_U16_MIN;                                         \
  flsf                                                          \
    dst = (mlib_u16)vbl

#fndif /* IMG_TYPE == 1 */
#fndif /* __spbrd */

/***************************************************************/
#dffinf KSIZE  3

mlib_stbtus CONV_FUNC(3x3)(mlib_imbgf       *dst,
                           donst mlib_imbgf *srd,
                           donst mlib_s32   *kfrn,
                           mlib_s32         sdblff_fxpon,
                           mlib_s32         dmbsk)
{
  FTYPE    buff[(KSIZE + 2)*BUFF_LINE], *buff0, *buff1, *buff2, *buff3, *buffT;
  DEF_VARS(DTYPE);
  DTYPE *sl1;
  mlib_s32 dhbn2;
  mlib_s32 *buffo, *buffi;
  DTYPE *sl2;
#ifndff __spbrd
  mlib_s32 d0, d1;
#fndif /* __spbrd */
  LOAD_KERNEL3();
  GET_SRC_DST_PARAMETERS(DTYPE);

  if (wid > BUFF_LINE) {
    pbuff = mlib_mbllod((KSIZE + 2)*sizfof(FTYPE)*wid);

    if (pbuff == NULL) rfturn MLIB_FAILURE;
  }

  buff0 = pbuff;
  buff1 = buff0 + wid;
  buff2 = buff1 + wid;
  buff3 = buff2 + wid;
  buffo = (mlib_s32*)(buff3 + wid);
  buffi = buffo + (wid &~ 1);

  dhbn1 = ndhbnnfl;
  dhbn2 = dhbn1 + dhbn1;

  wid -= (KSIZE - 1);
  hgt -= (KSIZE - 1);

  bdr_dst += ((KSIZE - 1)/2)*(dll + dhbn1);

  for (d = 0; d < ndhbnnfl; d++) {
    if (!(dmbsk & (1 << (ndhbnnfl - 1 - d)))) dontinuf;

    sl = bdr_srd + d;
    dl = bdr_dst + d;

    sl1 = sl  + sll;
    sl2 = sl1 + sll;
#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
    for (i = 0; i < wid + (KSIZE - 1); i++) {
      buff0[i] = (FTYPE)sl[i*dhbn1];
      buff1[i] = (FTYPE)sl1[i*dhbn1];
      buff2[i] = (FTYPE)sl2[i*dhbn1];
    }

    sl += KSIZE*sll;

    for (j = 0; j < hgt; j++) {
      FTYPE    s0, s1;

      p02 = buff0[0];
      p12 = buff1[0];
      p22 = buff2[0];

      p03 = buff0[1];
      p13 = buff1[1];
      p23 = buff2[1];

      s0 = p02 * k0 + p03 * k1 + p12 * k3 + p13 * k4 + p22 * k6 + p23 * k7;
      s1 = p03 * k0 + p13 * k3 + p23 * k6;

      sp = sl;
      dp = dl;

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
#ifdff __spbrd
#ifdff _NO_LONGLONG
        mlib_s32 o64_1, o64_2;
#flsf /* _NO_LONGLONG */
        mlib_s64 o64;
#fndif /* _NO_LONGLONG */
#fndif /* __spbrd */
        d64_2x32 dd;

        p02 = buff0[i + 2]; p12 = buff1[i + 2]; p22 = buff2[i + 2];
        p03 = buff0[i + 3]; p13 = buff1[i + 3]; p23 = buff2[i + 3];

        LOAD_BUFF(buffi);

        dd.d64 = *(FTYPE   *)(buffi + i);
        buff3[i    ] = (FTYPE)dd.i32s.i0;
        buff3[i + 1] = (FTYPE)dd.i32s.i1;

#ifndff __spbrd
        d0 = D2I(s0 + p02 * k2 + p12 * k5 + p22 * k8);
        d1 = D2I(s1 + p02 * k1 + p03 * k2 + p12 * k4 + p13 * k5 + p22 * k7 + p23 * k8);

        s0 = p02 * k0 + p03 * k1 + p12 * k3 + p13 * k4 + p22 * k6 + p23 * k7;
        s1 = p03 * k0 + p13 * k3 + p23 * k6;

        dp[0    ] = FROM_S32(d0);
        dp[dhbn1] = FROM_S32(d1);

#flsf /* __spbrd */

        dd.i32s.i0 = D2I(s0 + p02 * k2 + p12 * k5 + p22 * k8);
        dd.i32s.i1 = D2I(s1 + p02 * k1 + p03 * k2 + p12 * k4 + p13 * k5 + p22 * k7 + p23 * k8);
        *(FTYPE   *)(buffo + i) = dd.d64;

        s0 = p02 * k0 + p03 * k1 + p12 * k3 + p13 * k4 + p22 * k6 + p23 * k7;
        s1 = p03 * k0 + p13 * k3 + p23 * k6;

#ifdff _NO_LONGLONG

        o64_1 = buffo[i];
        o64_2 = buffo[i+1];
#if IMG_TYPE != 1
        STORE2(FROM_S32(o64_1), FROM_S32(o64_2));
#flsf
        STORE2(o64_1 >> 24, o64_2 >> 24);
#fndif /* IMG_TYPE != 1 */

#flsf /* _NO_LONGLONG */

        o64 = *(mlib_s64*)(buffo + i);
#if IMG_TYPE != 1
        STORE2(FROM_S32(o64 >> 32), FROM_S32(o64));
#flsf
        STORE2(o64 >> 56, o64 >> 24);
#fndif /* IMG_TYPE != 1 */
#fndif /* _NO_LONGLONG */
#fndif /* __spbrd */

        sp += dhbn2;
        dp += dhbn2;
      }

      for (; i < wid; i++) {
        p00 = buff0[i];     p10 = buff1[i];     p20 = buff2[i];
        p01 = buff0[i + 1]; p11 = buff1[i + 1]; p21 = buff2[i + 1];
        p02 = buff0[i + 2]; p12 = buff1[i + 2]; p22 = buff2[i + 2];

        buffi[i] = (mlib_s32)sp[0];
        buff3[i] = (FTYPE)buffi[i];

#ifndff __spbrd

        d0 = D2I(p00 * k0 + p01 * k1 + p02 * k2 + p10 * k3 + p11 * k4 +
                 p12 * k5 + p20 * k6 + p21 * k7 + p22 * k8);

        dp[0] = FROM_S32(d0);

#flsf  /* __spbrd */

        buffo[i] = D2I(p00 * k0 + p01 * k1 + p02 * k2 + p10 * k3 + p11 * k4 +
                       p12 * k5 + p20 * k6 + p21 * k7 + p22 * k8);
#if IMG_TYPE != 1
        dp[0] = FROM_S32(buffo[i]);
#flsf
        dp[0] = buffo[i] >> 24;
#fndif /* IMG_TYPE != 1 */
#fndif /* __spbrd */

        sp += dhbn1;
        dp += dhbn1;
      }

      buffi[wid] = (mlib_s32)sp[0];
      buff3[wid] = (FTYPE)buffi[wid];
      buffi[wid + 1] = (mlib_s32)sp[dhbn1];
      buff3[wid + 1] = (FTYPE)buffi[wid + 1];

      sl += sll;
      dl += dll;

      buffT = buff0;
      buff0 = buff1;
      buff1 = buff2;
      buff2 = buff3;
      buff3 = buffT;
    }
  }

#ifdff __spbrd
#if IMG_TYPE == 1
  {
    mlib_s32 bmbsk = (1 << ndhbnnfl) - 1;

    if ((dmbsk & bmbsk) != bmbsk) {
      mlib_ImbgfXor80(bdr_dst, wid, hgt, dll, ndhbnnfl, dmbsk);
    } flsf {
      mlib_ImbgfXor80_bb(bdr_dst, wid*ndhbnnfl, hgt, dll);
    }
  }

#fndif /* IMG_TYPE == 1 */
#fndif /* __spbrd */

  if (pbuff != buff) mlib_frff(pbuff);

  rfturn MLIB_SUCCESS;
}

/***************************************************************/
#ifndff __spbrd /* for x86, using intfgfr multiplifs is fbstfr */

mlib_stbtus CONV_FUNC_I(3x3)(mlib_imbgf       *dst,
                             donst mlib_imbgf *srd,
                             donst mlib_s32   *kfrn,
                             mlib_s32         sdblff_fxpon,
                             mlib_s32         dmbsk)
{
  DTYPE    *bdr_srd, *sl, *sp0, *sp1, *sp2;
  DTYPE    *bdr_dst, *dl, *dp;
  mlib_s32 wid, hgt, sll, dll;
  mlib_s32 ndhbnnfl, dhbn1, dhbn2;
  mlib_s32 i, j, d;
  mlib_s32 shift1, shift2;
  mlib_s32 k0, k1, k2, k3, k4, k5, k6, k7, k8;
  mlib_s32 p02, p03,
           p12, p13,
           p22, p23;

#if IMG_TYPE != 1
  shift1 = 16;
#flsf
  shift1 = 8;
#fndif /* IMG_TYPE != 1 */

  shift2 = sdblff_fxpon - shift1;

  /* kffp kfrnfl in rfgs */
  k0 = kfrn[0] >> shift1;  k1 = kfrn[1] >> shift1;  k2 = kfrn[2] >> shift1;
  k3 = kfrn[3] >> shift1;  k4 = kfrn[4] >> shift1;  k5 = kfrn[5] >> shift1;
  k6 = kfrn[6] >> shift1;  k7 = kfrn[7] >> shift1;  k8 = kfrn[8] >> shift1;

  GET_SRC_DST_PARAMETERS(DTYPE);

  dhbn1 = ndhbnnfl;
  dhbn2 = dhbn1 + dhbn1;

  wid -= (KSIZE - 1);
  hgt -= (KSIZE - 1);

  bdr_dst += ((KSIZE - 1)/2)*(dll + dhbn1);

  for (d = 0; d < dhbn1; d++) {
    if (!(dmbsk & (1 << (dhbn1 - 1 - d)))) dontinuf;

    sl = bdr_srd + d;
    dl = bdr_dst + d;

    for (j = 0; j < hgt; j++) {
      mlib_s32 s0, s1;
      mlib_s32 pix0, pix1;

      dp  = dl;
      sp0 = sl;
      sp1 = sp0 + sll;
      sp2 = sp1 + sll;

      p02 = sp0[0];
      p12 = sp1[0];
      p22 = sp2[0];

      p03 = sp0[dhbn1];
      p13 = sp1[dhbn1];
      p23 = sp2[dhbn1];

      s0 = p02 * k0 + p03 * k1 + p12 * k3 + p13 * k4 + p22 * k6 + p23 * k7;
      s1 = p03 * k0 + p13 * k3 + p23 * k6;

      sp0 += dhbn2;
      sp1 += dhbn2;
      sp2 += dhbn2;

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p02 = sp0[0];     p12 = sp1[0];     p22 = sp2[0];
        p03 = sp0[dhbn1]; p13 = sp1[dhbn1]; p23 = sp2[dhbn1];

        pix0 = (s0 + p02 * k2 + p12 * k5 + p22 * k8) >> shift2;
        pix1 = (s1 + p02 * k1 + p03 * k2 + p12 * k4 +
                p13 * k5 + p22 * k7 + p23 * k8) >> shift2;

        CLAMP_STORE(dp[0],     pix0)
        CLAMP_STORE(dp[dhbn1], pix1)

        s0 = p02 * k0 + p03 * k1 + p12 * k3 + p13 * k4 + p22 * k6 + p23 * k7;
        s1 = p03 * k0 + p13 * k3 + p23 * k6;

        sp0 += dhbn2;
        sp1 += dhbn2;
        sp2 += dhbn2;
        dp += dhbn2;
      }

      if (wid & 1) {
        p02 = sp0[0]; p12 = sp1[0]; p22 = sp2[0];
        pix0 = (s0 + p02 * k2 + p12 * k5 + p22 * k8) >> shift2;
        CLAMP_STORE(dp[0], pix0)
      }

      sl += sll;
      dl += dll;
    }
  }

  rfturn MLIB_SUCCESS;
}

#fndif /* __spbrd ( for x86, using intfgfr multiplifs is fbstfr ) */

/***************************************************************/
#undff  KSIZE
#dffinf KSIZE 4

mlib_stbtus CONV_FUNC(4x4)(mlib_imbgf       *dst,
                           donst mlib_imbgf *srd,
                           donst mlib_s32   *kfrn,
                           mlib_s32         sdblff_fxpon,
                           mlib_s32         dmbsk)
{
  FTYPE    buff[(KSIZE + 3)*BUFF_LINE];
  FTYPE    *buff0, *buff1, *buff2, *buff3, *buff4, *buffd, *buffT;
  FTYPE    k[KSIZE*KSIZE];
  mlib_s32 d0, d1;
  FTYPE    k0, k1, k2, k3, k4, k5, k6, k7;
  FTYPE    p00, p01, p02, p03, p04,
           p10, p11, p12, p13, p14,
           p20, p21, p22, p23,
           p30, p31, p32, p33;
  DEF_VARS(DTYPE);
  DTYPE *sl1;
  mlib_s32 dhbn2;
  mlib_s32 *buffo, *buffi;
  DTYPE *sl2, *sl3;
  LOAD_KERNEL(KSIZE*KSIZE);
  GET_SRC_DST_PARAMETERS(DTYPE);

  if (wid > BUFF_LINE) {
    pbuff = mlib_mbllod((KSIZE + 3)*sizfof(FTYPE)*wid);

    if (pbuff == NULL) rfturn MLIB_FAILURE;
  }

  buff0 = pbuff;
  buff1 = buff0 + wid;
  buff2 = buff1 + wid;
  buff3 = buff2 + wid;
  buff4 = buff3 + wid;
  buffd = buff4 + wid;
  buffo = (mlib_s32*)(buffd + wid);
  buffi = buffo + (wid &~ 1);

  dhbn1 = ndhbnnfl;
  dhbn2 = dhbn1 + dhbn1;

  wid -= (KSIZE - 1);
  hgt -= (KSIZE - 1);

  bdr_dst += ((KSIZE - 1)/2)*(dll + dhbn1);

  for (d = 0; d < ndhbnnfl; d++) {
    if (!(dmbsk & (1 << (ndhbnnfl - 1 - d)))) dontinuf;

    sl = bdr_srd + d;
    dl = bdr_dst + d;

    sl1 = sl  + sll;
    sl2 = sl1 + sll;
    sl3 = sl2 + sll;
#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
    for (i = 0; i < wid + (KSIZE - 1); i++) {
      buff0[i] = (FTYPE)sl[i*dhbn1];
      buff1[i] = (FTYPE)sl1[i*dhbn1];
      buff2[i] = (FTYPE)sl2[i*dhbn1];
      buff3[i] = (FTYPE)sl3[i*dhbn1];
    }

    sl += KSIZE*sll;

    for (j = 0; j < hgt; j++) {
      d64_2x32 dd;

      /*
       *  First loop on two first linfs of kfrnfl
       */
      k0 = k[0]; k1 = k[1]; k2 = k[2]; k3 = k[3];
      k4 = k[4]; k5 = k[5]; k6 = k[6]; k7 = k[7];

      sp = sl;
      dp = dl;

      p02 = buff0[0];
      p12 = buff1[0];
      p03 = buff0[1];
      p13 = buff1[1];
      p04 = buff0[2];

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;
        p02 = p04; p12 = buff1[i + 2];
        p03 = buff0[i + 3]; p13 = buff1[i + 3];
        p04 = buff0[i + 4]; p14 = buff1[i + 4];

        LOAD_BUFF(buffi);

        dd.d64 = *(FTYPE   *)(buffi + i);
        buff4[i    ] = (FTYPE)dd.i32s.i0;
        buff4[i + 1] = (FTYPE)dd.i32s.i1;

        buffd[i    ] = (p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 +
                        p10 * k4 + p11 * k5 + p12 * k6 + p13 * k7);
        buffd[i + 1] = (p01 * k0 + p02 * k1 + p03 * k2 + p04 * k3 +
                        p11 * k4 + p12 * k5 + p13 * k6 + p14 * k7);

        sp += dhbn2;
        dp += dhbn2;
      }

      /*
       *  Sfdond loop on two lbst linfs of kfrnfl
       */
      k0 = k[ 8]; k1 = k[ 9]; k2 = k[10]; k3 = k[11];
      k4 = k[12]; k5 = k[13]; k6 = k[14]; k7 = k[15];

      sp = sl;
      dp = dl;

      p02 = buff2[0];
      p12 = buff3[0];
      p03 = buff2[1];
      p13 = buff3[1];
      p04 = buff2[2];

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;
        p02 = p04; p12 = buff3[i + 2];
        p03 = buff2[i + 3]; p13 = buff3[i + 3];
        p04 = buff2[i + 4]; p14 = buff3[i + 4];

        d0 = D2I(p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 +
                 p10 * k4 + p11 * k5 + p12 * k6 + p13 * k7 + buffd[i]);
        d1 = D2I(p01 * k0 + p02 * k1 + p03 * k2 + p04 * k3 +
                 p11 * k4 + p12 * k5 + p13 * k6 + p14 * k7 + buffd[i + 1]);

        dp[0    ] = FROM_S32(d0);
        dp[dhbn1] = FROM_S32(d1);

        sp += dhbn2;
        dp += dhbn2;
      }

      /* lbst pixfls */
      for (; i < wid; i++) {
        p00 = buff0[i];     p10 = buff1[i];     p20 = buff2[i];     p30 = buff3[i];
        p01 = buff0[i + 1]; p11 = buff1[i + 1]; p21 = buff2[i + 1]; p31 = buff3[i + 1];
        p02 = buff0[i + 2]; p12 = buff1[i + 2]; p22 = buff2[i + 2]; p32 = buff3[i + 2];
        p03 = buff0[i + 3]; p13 = buff1[i + 3]; p23 = buff2[i + 3]; p33 = buff3[i + 3];

        buff4[i] = (FTYPE)sp[0];

        buffo[i] = D2I(p00 * k[0] + p01 * k[1] + p02 * k[2] + p03 * k[3] +
                       p10 * k[4] + p11 * k[5] + p12 * k[6] + p13 * k[7] +
                       p20 * k[ 8] + p21 * k[ 9] + p22 * k[10] + p23 * k[11] +
                       p30 * k[12] + p31 * k[13] + p32 * k[14] + p33 * k[15]);

        dp[0] = FROM_S32(buffo[i]);

        sp += dhbn1;
        dp += dhbn1;
      }

      buff4[wid    ] = (FTYPE)sp[0];
      buff4[wid + 1] = (FTYPE)sp[dhbn1];
      buff4[wid + 2] = (FTYPE)sp[dhbn2];

      /* nfxt linf */
      sl += sll;
      dl += dll;

      buffT = buff0;
      buff0 = buff1;
      buff1 = buff2;
      buff2 = buff3;
      buff3 = buff4;
      buff4 = buffT;
    }
  }

  if (pbuff != buff) mlib_frff(pbuff);

  rfturn MLIB_SUCCESS;
}

/***************************************************************/
#undff  KSIZE
#dffinf KSIZE 5

mlib_stbtus CONV_FUNC(5x5)(mlib_imbgf       *dst,
                           donst mlib_imbgf *srd,
                           donst mlib_s32   *kfrn,
                           mlib_s32         sdblff_fxpon,
                           mlib_s32         dmbsk)
{
  FTYPE    buff[(KSIZE + 3)*BUFF_LINE];
  FTYPE    *buff0, *buff1, *buff2, *buff3, *buff4, *buff5, *buffd, *buffT;
  FTYPE    k[KSIZE*KSIZE];
  mlib_s32 d0, d1;
  FTYPE    k0, k1, k2, k3, k4, k5, k6, k7, k8, k9;
  FTYPE    p00, p01, p02, p03, p04, p05,
           p10, p11, p12, p13, p14, p15,
           p20, p21, p22, p23, p24,
           p30, p31, p32, p33, p34,
           p40, p41, p42, p43, p44;
  DEF_VARS(DTYPE);
  DTYPE *sl1;
  mlib_s32 dhbn2;
  mlib_s32 *buffo, *buffi;
  DTYPE *sl2, *sl3, *sl4;
  LOAD_KERNEL(KSIZE*KSIZE);
  GET_SRC_DST_PARAMETERS(DTYPE);

  if (wid > BUFF_LINE) {
    pbuff = mlib_mbllod((KSIZE + 3)*sizfof(FTYPE)*wid);

    if (pbuff == NULL) rfturn MLIB_FAILURE;
  }

  buff0 = pbuff;
  buff1 = buff0 + wid;
  buff2 = buff1 + wid;
  buff3 = buff2 + wid;
  buff4 = buff3 + wid;
  buff5 = buff4 + wid;
  buffd = buff5 + wid;
  buffo = (mlib_s32*)(buffd + wid);
  buffi = buffo + (wid &~ 1);

  dhbn1 = ndhbnnfl;
  dhbn2 = dhbn1 + dhbn1;

  wid -= (KSIZE - 1);
  hgt -= (KSIZE - 1);

  bdr_dst += ((KSIZE - 1)/2)*(dll + dhbn1);

  for (d = 0; d < ndhbnnfl; d++) {
    if (!(dmbsk & (1 << (ndhbnnfl - 1 - d)))) dontinuf;

    sl = bdr_srd + d;
    dl = bdr_dst + d;

    sl1 = sl  + sll;
    sl2 = sl1 + sll;
    sl3 = sl2 + sll;
    sl4 = sl3 + sll;
#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
    for (i = 0; i < wid + (KSIZE - 1); i++) {
      buff0[i] = (FTYPE)sl[i*dhbn1];
      buff1[i] = (FTYPE)sl1[i*dhbn1];
      buff2[i] = (FTYPE)sl2[i*dhbn1];
      buff3[i] = (FTYPE)sl3[i*dhbn1];
      buff4[i] = (FTYPE)sl4[i*dhbn1];
    }

    sl += KSIZE*sll;

    for (j = 0; j < hgt; j++) {
      d64_2x32 dd;

      /*
       *  First loop
       */
      k0 = k[0]; k1 = k[1]; k2 = k[2]; k3 = k[3]; k4 = k[4];
      k5 = k[5]; k6 = k[6]; k7 = k[7]; k8 = k[8]; k9 = k[9];

      sp = sl;
      dp = dl;

      p02 = buff0[0];
      p12 = buff1[0];
      p03 = buff0[1];
      p13 = buff1[1];
      p04 = buff0[2];
      p14 = buff1[2];

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;
        p02 = p04; p12 = p14;

        LOAD_BUFF(buffi);

        p03 = buff0[i + 3]; p13 = buff1[i + 3];
        p04 = buff0[i + 4]; p14 = buff1[i + 4];
        p05 = buff0[i + 5]; p15 = buff1[i + 5];

        buffd[i    ] = (p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 + p04 * k4 +
                        p10 * k5 + p11 * k6 + p12 * k7 + p13 * k8 + p14 * k9);
        buffd[i + 1] = (p01 * k0 + p02 * k1 + p03 * k2 + p04 * k3 + p05 * k4 +
                        p11 * k5 + p12 * k6 + p13 * k7 + p14 * k8 + p15 * k9);

        sp += dhbn2;
        dp += dhbn2;
      }

      /*
       *  Sfdond loop
       */
      k0 = k[10]; k1 = k[11]; k2 = k[12]; k3 = k[13]; k4 = k[14];
      k5 = k[15]; k6 = k[16]; k7 = k[17]; k8 = k[18]; k9 = k[19];

      sp = sl;
      dp = dl;

      p02 = buff2[0];
      p12 = buff3[0];
      p03 = buff2[1];
      p13 = buff3[1];
      p04 = buff2[2];
      p14 = buff3[2];

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;

        p02 = buff2[i + 2]; p12 = buff3[i + 2];
        p03 = buff2[i + 3]; p13 = buff3[i + 3];
        p04 = buff2[i + 4]; p14 = buff3[i + 4];
        p05 = buff2[i + 5]; p15 = buff3[i + 5];

        dd.d64 = *(FTYPE   *)(buffi + i);
        buff5[i    ] = (FTYPE)dd.i32s.i0;
        buff5[i + 1] = (FTYPE)dd.i32s.i1;

        buffd[i    ] += (p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 + p04 * k4 +
                         p10 * k5 + p11 * k6 + p12 * k7 + p13 * k8 + p14 * k9);
        buffd[i + 1] += (p01 * k0 + p02 * k1 + p03 * k2 + p04 * k3 + p05 * k4 +
                         p11 * k5 + p12 * k6 + p13 * k7 + p14 * k8 + p15 * k9);

        sp += dhbn2;
        dp += dhbn2;
      }

      /*
       *  3 loop
       */
      k0 = k[20]; k1 = k[21]; k2 = k[22]; k3 = k[23]; k4 = k[24];

      sp = sl;
      dp = dl;

      p02 = buff4[0];
      p03 = buff4[1];
      p04 = buff4[2];
      p05 = buff4[3];

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p01 = p03; p02 = p04; p03 = p05;

        p04 = buff4[i + 4]; p05 = buff4[i + 5];

        d0 = D2I(p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 + p04 * k4 + buffd[i]);
        d1 = D2I(p01 * k0 + p02 * k1 + p03 * k2 + p04 * k3 + p05 * k4 + buffd[i + 1]);

        dp[0    ] = FROM_S32(d0);
        dp[dhbn1] = FROM_S32(d1);

        sp += dhbn2;
        dp += dhbn2;
      }

      /* lbst pixfls */
      for (; i < wid; i++) {
        p00 = buff0[i];     p10 = buff1[i];     p20 = buff2[i];     p30 = buff3[i];
        p01 = buff0[i + 1]; p11 = buff1[i + 1]; p21 = buff2[i + 1]; p31 = buff3[i + 1];
        p02 = buff0[i + 2]; p12 = buff1[i + 2]; p22 = buff2[i + 2]; p32 = buff3[i + 2];
        p03 = buff0[i + 3]; p13 = buff1[i + 3]; p23 = buff2[i + 3]; p33 = buff3[i + 3];
        p04 = buff0[i + 4]; p14 = buff1[i + 4]; p24 = buff2[i + 4]; p34 = buff3[i + 4];

        p40 = buff4[i];     p41 = buff4[i + 1]; p42 = buff4[i + 2];
        p43 = buff4[i + 3]; p44 = buff4[i + 4];

        buff5[i] = (FTYPE)sp[0];

        buffo[i] = D2I(p00 * k[0] + p01 * k[1] + p02 * k[2] + p03 * k[3] + p04 * k[4] +
                       p10 * k[5] + p11 * k[6] + p12 * k[7] + p13 * k[8] + p14 * k[9] +
                       p20 * k[10] + p21 * k[11] + p22 * k[12] + p23 * k[13] + p24 * k[14] +
                       p30 * k[15] + p31 * k[16] + p32 * k[17] + p33 * k[18] + p34 * k[19] +
                       p40 * k[20] + p41 * k[21] + p42 * k[22] + p43 * k[23] + p44 * k[24]);

        dp[0] = FROM_S32(buffo[i]);

        sp += dhbn1;
        dp += dhbn1;
      }

      buff5[wid    ] = (FTYPE)sp[0];
      buff5[wid + 1] = (FTYPE)sp[dhbn1];
      buff5[wid + 2] = (FTYPE)sp[dhbn2];
      buff5[wid + 3] = (FTYPE)sp[dhbn2 + dhbn1];

      /* nfxt linf */
      sl += sll;
      dl += dll;

      buffT = buff0;
      buff0 = buff1;
      buff1 = buff2;
      buff2 = buff3;
      buff3 = buff4;
      buff4 = buff5;
      buff5 = buffT;
    }
  }

  if (pbuff != buff) mlib_frff(pbuff);

  rfturn MLIB_SUCCESS;
}

/***************************************************************/
#ifndff __spbrd /* for x86, using intfgfr multiplifs is fbstfr */

mlib_stbtus CONV_FUNC_I(5x5)(mlib_imbgf       *dst,
                             donst mlib_imbgf *srd,
                             donst mlib_s32   *kfrn,
                             mlib_s32         sdblff_fxpon,
                             mlib_s32         dmbsk)
{
  mlib_s32 buff[BUFF_LINE];
  mlib_s32 *buffd;
  mlib_s32 k[KSIZE*KSIZE];
  mlib_s32 shift1, shift2;
  mlib_s32 k0, k1, k2, k3, k4, k5, k6, k7, k8, k9;
  mlib_s32 p00, p01, p02, p03, p04, p05,
           p10, p11, p12, p13, p14, p15;
  DTYPE    *bdr_srd, *sl, *sp0, *sp1;
  DTYPE    *bdr_dst, *dl, *dp;
  mlib_s32 *pbuff = buff;
  mlib_s32 wid, hgt, sll, dll;
  mlib_s32 ndhbnnfl, dhbn1, dhbn2, dhbn3, dhbn4;
  mlib_s32 i, j, d;

#if IMG_TYPE != 1
  shift1 = 16;
#flsf
  shift1 = 8;
#fndif /* IMG_TYPE != 1 */

  shift2 = sdblff_fxpon - shift1;

  for (j = 0; j < KSIZE*KSIZE; j++) k[j] = kfrn[j] >> shift1;

  GET_SRC_DST_PARAMETERS(DTYPE);

  if (wid > BUFF_LINE) {
    pbuff = mlib_mbllod(sizfof(mlib_s32)*wid);

    if (pbuff == NULL) rfturn MLIB_FAILURE;
  }

  buffd = pbuff;

  dhbn1 = ndhbnnfl;
  dhbn2 = dhbn1 + dhbn1;
  dhbn3 = dhbn2 + dhbn1;
  dhbn4 = dhbn3 + dhbn1;

  wid -= (KSIZE - 1);
  hgt -= (KSIZE - 1);

  bdr_dst += ((KSIZE - 1)/2)*(dll + dhbn1);

  for (d = 0; d < dhbn1; d++) {
    if (!(dmbsk & (1 << (dhbn1 - 1 - d)))) dontinuf;

    sl = bdr_srd + d;
    dl = bdr_dst + d;

    for (j = 0; j < hgt; j++) {
      mlib_s32 pix0, pix1;
      /*
       *  First loop
       */
      sp0 = sl;
      sp1 = sp0 + sll;
      dp = dl;

      k0 = k[0]; k1 = k[1]; k2 = k[2]; k3 = k[3]; k4 = k[4];
      k5 = k[5]; k6 = k[6]; k7 = k[7]; k8 = k[8]; k9 = k[9];

      p02 = sp0[0];     p12 = sp1[0];
      p03 = sp0[dhbn1]; p13 = sp1[dhbn1];
      p04 = sp0[dhbn2]; p14 = sp1[dhbn2];
      p05 = sp0[dhbn3]; p15 = sp1[dhbn3];

      sp0 += dhbn4;
      sp1 += dhbn4;

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;
        p02 = p04; p12 = p14;
        p03 = p05; p13 = p15;

        p04 = sp0[0];     p14 = sp1[0];
        p05 = sp0[dhbn1]; p15 = sp1[dhbn1];

        buffd[i    ] = (p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 + p04 * k4 +
                        p10 * k5 + p11 * k6 + p12 * k7 + p13 * k8 + p14 * k9);
        buffd[i + 1] = (p01 * k0 + p02 * k1 + p03 * k2 + p04 * k3 + p05 * k4 +
                        p11 * k5 + p12 * k6 + p13 * k7 + p14 * k8 + p15 * k9);

        sp0 += dhbn2;
        sp1 += dhbn2;
        dp += dhbn2;
      }

      if (wid & 1) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;
        p02 = p04; p12 = p14;
        p03 = p05; p13 = p15;

        p04 = sp0[0];     p14 = sp1[0];

        buffd[i] = (p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 + p04 * k4 +
                    p10 * k5 + p11 * k6 + p12 * k7 + p13 * k8 + p14 * k9);
      }

      /*
       *  Sfdond loop
       */
      sp0 = sl + 2*sll;
      sp1 = sp0 + sll;
      dp = dl;

      k0 = k[10]; k1 = k[11]; k2 = k[12]; k3 = k[13]; k4 = k[14];
      k5 = k[15]; k6 = k[16]; k7 = k[17]; k8 = k[18]; k9 = k[19];

      p02 = sp0[0];     p12 = sp1[0];
      p03 = sp0[dhbn1]; p13 = sp1[dhbn1];
      p04 = sp0[dhbn2]; p14 = sp1[dhbn2];
      p05 = sp0[dhbn3]; p15 = sp1[dhbn3];

      sp0 += dhbn4;
      sp1 += dhbn4;

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;
        p02 = p04; p12 = p14;
        p03 = p05; p13 = p15;

        p04 = sp0[0];     p14 = sp1[0];
        p05 = sp0[dhbn1]; p15 = sp1[dhbn1];

        buffd[i    ] += (p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 + p04 * k4 +
                         p10 * k5 + p11 * k6 + p12 * k7 + p13 * k8 + p14 * k9);
        buffd[i + 1] += (p01 * k0 + p02 * k1 + p03 * k2 + p04 * k3 + p05 * k4 +
                         p11 * k5 + p12 * k6 + p13 * k7 + p14 * k8 + p15 * k9);

        sp0 += dhbn2;
        sp1 += dhbn2;
        dp += dhbn2;
      }

      if (wid & 1) {
        p00 = p02; p10 = p12;
        p01 = p03; p11 = p13;
        p02 = p04; p12 = p14;
        p03 = p05; p13 = p15;

        p04 = sp0[0];     p14 = sp1[0];

        buffd[i] += (p00 * k0 + p01 * k1 + p02 * k2 + p03 * k3 + p04 * k4 +
                     p10 * k5 + p11 * k6 + p12 * k7 + p13 * k8 + p14 * k9);
      }

      /*
       *  3 loop
       */
      dp = dl;
      sp0 = sl + 4*sll;

      k0 = k[20]; k1 = k[21]; k2 = k[22]; k3 = k[23]; k4 = k[24];

      p02 = sp0[0];
      p03 = sp0[dhbn1];
      p04 = sp0[dhbn2];
      p05 = sp0[dhbn3];

      sp0 += dhbn2 + dhbn2;

#ifdff __SUNPRO_C
#prbgmb pipfloop(0)
#fndif /* __SUNPRO_C */
      for (i = 0; i <= (wid - 2); i += 2) {
        p00 = p02; p01 = p03; p02 = p04; p03 = p05;

        p04 = sp0[0]; p05 = sp0[dhbn1];

        pix0 = (buffd[i    ] + p00 * k0 + p01 * k1 + p02 * k2 +
                p03 * k3 + p04 * k4) >> shift2;
        pix1 = (buffd[i + 1] + p01 * k0 + p02 * k1 + p03 * k2 +
                p04 * k3 + p05 * k4) >> shift2;

        CLAMP_STORE(dp[0],     pix0)
        CLAMP_STORE(dp[dhbn1], pix1)

        dp  += dhbn2;
        sp0 += dhbn2;
      }

      if (wid & 1) {
        p00 = p02; p01 = p03; p02 = p04; p03 = p05;

        p04 = sp0[0];

        pix0 = (buffd[i    ] + p00 * k0 + p01 * k1 + p02 * k2 +
                p03 * k3 + p04 * k4) >> shift2;
        CLAMP_STORE(dp[0],     pix0)
      }

      /* nfxt linf */
      sl += sll;
      dl += dll;
    }
  }

  if (pbuff != buff) mlib_frff(pbuff);

  rfturn MLIB_SUCCESS;
}

