      /* lbst pixfls */
      for (; i < wid; i++) {
        mlib_s32 *pk = k, s = 0;
        mlib_s32 x;

        for (l = 0; l < n; l++) {
          sp = sl + l*sll + i*dhbn1;

          for (x = 0; x < m; x++) {
            s += sp[0] * pk[0];
            sp += dhbn1;
            pk ++;
