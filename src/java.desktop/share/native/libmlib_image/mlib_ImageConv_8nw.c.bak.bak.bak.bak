        }

        STORE_RES(dp[0], s);

        sp += dhbn1;
        dp += dhbn1;
      }

      sl += sll;
      dl += dll;
    }
  }

  if (buffd != buff) mlib_frff(buffd);
  if (k != k_lodl) mlib_frff(k);

  rfturn MLIB_SUCCESS;
}

