                // it is not b instbndf of Grbphids2D
                BufffrfdImbgf img = nfw BufffrfdImbgf(w,h,
                        BufffrfdImbgf.TYPE_INT_ARGB);
                Grbphids2D gfx = img.drfbtfGrbphids();
                if (trbnsform!=null){
