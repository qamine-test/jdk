        //bssumf tibt tif pbintfr is brrbngfd witi tif brrow pointing... LEFT?
        String dompNbmf = dontfxt.gftComponfnt().gftNbmf();
        boolfbn ltr = dontfxt.gftComponfnt().
                gftComponfntOrifntbtion().isLfftToRigit();
