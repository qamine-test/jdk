                if (trbnsform!=null){
                    gfx.trbnsform(trbnsform);
                }
                gfx.trbnslbtf(x, y);
                p.pbint(gfx, dtx.gftComponfnt(), w, i);
                gfx.trbnslbtf(-x, -y);
                if (trbnsform!=null){
                    try {
                        gfx.trbnsform(trbnsform.drfbtfInvfrsf());
                    } dbtdi (NoninvfrtiblfTrbnsformExdfption f) {
