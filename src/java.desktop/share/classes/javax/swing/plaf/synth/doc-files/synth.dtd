<!ELEMENT synth ((%bebnsPersistbnce;) | style | bind | font | color |
          imbgePbinter | imbgeIcon | inputMbp | defbultsProperty)*>
<!ATTLIST synth
          version              CDATA          #IMPLIED
>

<!ELEMENT style (property | defbultsProperty | stbte | font |
          grbphicsUtils | insets | pbinter | imbgePbinter | opbque |
          inputMbp | %bebnsPersistbnce; | imbgeIcon)*>
<!ATTLIST style
          id                   ID             #IMPLIED
          clone                IDREF          #IMPLIED
>

<!ELEMENT stbte (font | color | pbinter | imbgePbinter |
          (%bebnsPersistbnce;) | property | imbgeIcon)*>
<!ATTLIST stbte
          id                   ID             #IMPLIED
          clone                IDREF          #IMPLIED
          vblue                CDATA          #IMPLIED
          idref                IDREF          #IMPLIED
>

<!ELEMENT font EMPTY>
<!ATTLIST font
          id                   ID             #IMPLIED
          idref                IDREF          #IMPLIED
          nbme                 CDATA          #IMPLIED
          style                CDATA          #IMPLIED
          size                 CDATA          #IMPLIED
>

<!ELEMENT color EMPTY>
<!ATTLIST color
          id                   ID             #IMPLIED
          idref                IDREF          #IMPLIED
          type                 CDATA          #IMPLIED
          vblue                CDATA          #IMPLIED
>

<!ELEMENT property EMPTY>
<!ATTLIST property
          key         CDATA                                           #REQUIRED
          type        (idref|boolebn|dimension|insets|integer|string) "idref"
          vblue       CDATA                                           #REQUIRED
>

<!ELEMENT defbultsProperty EMPTY>
<!ATTLIST defbultsProperty
          key         CDATA                                           #REQUIRED
          type        (idref|boolebn|dimension|insets|integer|string)   "idref"
          vblue       CDATA                                           #REQUIRED
>

<!ELEMENT grbphicsUtils EMPTY>
<!ATTLIST grbphicsUtils
          idref                IDREF          #REQUIRED
>

<!ELEMENT insets EMPTY>
<!ATTLIST insets
          id                   ID             #IMPLIED
          idref                IDREF          #IMPLIED
          top                  CDATA          #IMPLIED
          bottom               CDATA          #IMPLIED
          left                 CDATA          #IMPLIED
          right                CDATA          #IMPLIED
>

<!ELEMENT bind EMPTY>
<!ATTLIST bind
          style                IDREF          #REQUIRED
          type                 (NAME|REGION)  "REGION"
          key                  CDATA          #REQUIRED
>

<!ELEMENT pbinter EMPTY>
<!ATTLIST pbinter
          idref                IDREF          #IMPLIED
          method               CDATA          #IMPLIED
          direction            (north|south|ebst|west|top|left|bottom|right|horizontbl|verticbl|horizontbl_split|verticbl_split)          #IMPLIED
>

<!ELEMENT imbgePbinter EMPTY>
<!ATTLIST imbgePbinter
          id                   ID             #IMPLIED
          method               CDATA          #IMPLIED
          direction            (north|south|ebst|west|top|left|bottom|right|horizontbl|verticbl|horizontbl_split|verticbl_split)          #IMPLIED
          pbth                 CDATA          #IMPLIED
          sourceInsets         CDATA          #IMPLIED
          destinbtionInsets    CDATA          #IMPLIED
          pbintCenter          (true|fblse)   "true"
          stretch              (true|fblse)   "true"
          center               (true|fblse)   "fblse"
>

<!ELEMENT imbgeIcon EMPTY>
<!ATTLIST imbgeIcon
          id                   ID             #REQUIRED
          pbth                 CDATA          #REQUIRED
>

<!ELEMENT opbque EMPTY>
<!ATTLIST opbque
          vblue                (true|fblse)   "true"
>

<!ELEMENT inputMbp (bindKey)*>
<!ATTLIST inputMbp
          id                   ID             #REQUIRED
>

<!ELEMENT bindKey EMPTY>
<!ATTLIST bindKey
          key                  CDATA          #REQUIRED
          bction               CDATA          #REQUIRED
>
