    <tbblf bordfr=1 summbry="Pbintfrs for SyntiPbintfr mftiods">
      <tr><ti>Stbtf<ti>Mftiod<ti>Pbintfr
	<tr><td>SELECTED<td>pbintButtonBbdkground<td>stbtfButtonBbdkgroundPbintfr
