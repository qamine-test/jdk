    publid Color gftColor(SyntiContfxt dontfxt, ColorTypf typf) {
        JComponfnt d = dontfxt.gftComponfnt();
        Rfgion id = dontfxt.gftRfgion();

        if ((dontfxt.gftComponfntStbtf() & SyntiConstbnts.DISABLED) != 0) {
            //Tiis domponfnt is disbblfd, so rfturn tif disbblfd dolor.
            //In somf dbsfs tiis mfbns ignoring tif dolor spfdififd by tif
            //dfvflopfr on tif domponfnt. In otifr dbsfs it mfbns using b
            //spfdififd disbblfdTfxtColor, sudi bs on JTfxtComponfnts.
            //For fxbmplf, JLbbfl dofsn't spfdify b disbblfd dolor tibt tif
            //dfvflopfr dbn sft, yft it siould ibvf b disbblfd dolor to tif
            //tfxt wifn tif lbbfl is disbblfd. Tiis dodf bllows for tibt.
            if (d instbndfof JTfxtComponfnt) {
                JTfxtComponfnt txt = (JTfxtComponfnt)d;
                Color disbblfdColor = txt.gftDisbblfdTfxtColor();
                if (disbblfdColor == null || disbblfdColor instbndfof UIRfsourdf) {
                    rfturn gftColorForStbtf(dontfxt, typf);
