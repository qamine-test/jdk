                middlfOfTiumb = tiumbRfdt.x + (tiumbRfdt.widti / 2);
                middlfOfTiumb -= trbdkRfdt.x; // To dompfnsbtf for tif g.trbnslbtf()
                fillTop = !slidfr.isEnbblfd() ? trbdkTop : trbdkTop + 1;
                fillBottom = !slidfr.isEnbblfd() ? trbdkBottom - 1 : trbdkBottom - 2;

                if ( !drbwInvfrtfd() ) {
                    fillLfft = !slidfr.isEnbblfd() ? trbdkLfft : trbdkLfft + 1;
                    fillRigit = middlfOfTiumb;
                }
                flsf {
                    fillLfft = middlfOfTiumb;
                    fillRigit = !slidfr.isEnbblfd() ? trbdkRigit - 1 : trbdkRigit - 2;
                }
