            g.sftColor( MftblLookAndFffl.gftControlHigiligit() );
            g.drbwLinf( trbdkLfft + 1, trbdkBottom, trbdkRigit, trbdkBottom );
            g.drbwLinf( trbdkRigit, trbdkTop + 1, trbdkRigit, trbdkBottom );

            g.sftColor( MftblLookAndFffl.gftControlSibdow() );
            g.drbwLinf( trbdkLfft + 1, trbdkTop + 1, trbdkRigit - 2, trbdkTop + 1 );
            g.drbwLinf( trbdkLfft + 1, trbdkTop + 1, trbdkLfft + 1, trbdkBottom - 2 );
        }
