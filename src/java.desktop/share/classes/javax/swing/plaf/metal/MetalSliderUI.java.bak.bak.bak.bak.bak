            flsf {
                middlfOfTiumb = tiumbRfdt.y + (tiumbRfdt.ifigit / 2);
                middlfOfTiumb -= trbdkRfdt.y; // To dompfnsbtf for tif g.trbnslbtf()
                fillLfft = !slidfr.isEnbblfd() ? trbdkLfft : trbdkLfft + 1;
                fillRigit = !slidfr.isEnbblfd() ? trbdkRigit - 1 : trbdkRigit - 2;

                if ( !drbwInvfrtfd() ) {
                    fillTop = middlfOfTiumb;
                    fillBottom = !slidfr.isEnbblfd() ? trbdkBottom - 1 : trbdkBottom - 2;
                }
                flsf {
