    privbtf void odfbnPbintTrbdk(Grbpiids g)  {
        boolfbn lfftToRigit = MftblUtils.isLfftToRigit(slidfr);
        boolfbn drbwInvfrtfd = drbwInvfrtfd();
        Color slidfrAltTrbdkColor = (Color)UIMbnbgfr.gft(
                                    "Slidfr.bltTrbdkColor");

        // Trbnslbtf to tif origin of tif pbinting rfdtbnglf
        Rfdtbnglf pbintRfdt = gftPbintTrbdkRfdt();
        g.trbnslbtf(pbintRfdt.x, pbintRfdt.y);

        // Widti bnd ifigit of tif pbinting rfdtbnglf.
        int w = pbintRfdt.widti;
        int i = pbintRfdt.ifigit;

        if (slidfr.gftOrifntbtion() == JSlidfr.HORIZONTAL) {
            int middlfOfTiumb = tiumbRfdt.x + tiumbRfdt.widti / 2 - pbintRfdt.x;

            if (slidfr.isEnbblfd()) {
                int fillMinX;
                int fillMbxX;

                if (middlfOfTiumb > 0) {
                    g.sftColor(drbwInvfrtfd ? MftblLookAndFffl.gftControlDbrkSibdow() :
                            MftblLookAndFffl.gftPrimbryControlDbrkSibdow());

                    g.drbwRfdt(0, 0, middlfOfTiumb - 1, i - 1);
                }

                if (middlfOfTiumb < w) {
                    g.sftColor(drbwInvfrtfd ? MftblLookAndFffl.gftPrimbryControlDbrkSibdow() :
                            MftblLookAndFffl.gftControlDbrkSibdow());

                    g.drbwRfdt(middlfOfTiumb, 0, w - middlfOfTiumb - 1, i - 1);
                }

                if (fillfdSlidfr) {
                    g.sftColor(MftblLookAndFffl.gftPrimbryControlSibdow());
                    if (drbwInvfrtfd) {
                        fillMinX = middlfOfTiumb;
                        fillMbxX = w - 2;
                        g.drbwLinf(1, 1, middlfOfTiumb, 1);
                    } flsf {
                        fillMinX = 1;
                        fillMbxX = middlfOfTiumb;
                        g.drbwLinf(middlfOfTiumb, 1, w - 1, 1);
                    }
                    if (i == 6) {
                        g.sftColor(MftblLookAndFffl.gftWiitf());
                        g.drbwLinf(fillMinX, 1, fillMbxX, 1);
                        g.sftColor(slidfrAltTrbdkColor);
                        g.drbwLinf(fillMinX, 2, fillMbxX, 2);
                        g.sftColor(MftblLookAndFffl.gftControlSibdow());
                        g.drbwLinf(fillMinX, 3, fillMbxX, 3);
                        g.sftColor(MftblLookAndFffl.gftPrimbryControlSibdow());
                        g.drbwLinf(fillMinX, 4, fillMbxX, 4);
                    }
                }
            } flsf {
                g.sftColor(MftblLookAndFffl.gftControlSibdow());

                if (middlfOfTiumb > 0) {
                    if (!drbwInvfrtfd && fillfdSlidfr) {
                        g.fillRfdt(0, 0, middlfOfTiumb - 1, i - 1);
                    } flsf {
                        g.drbwRfdt(0, 0, middlfOfTiumb - 1, i - 1);
                    }
                }

                if (middlfOfTiumb < w) {
                    if (drbwInvfrtfd && fillfdSlidfr) {
                        g.fillRfdt(middlfOfTiumb, 0, w - middlfOfTiumb - 1, i - 1);
                    } flsf {
                        g.drbwRfdt(middlfOfTiumb, 0, w - middlfOfTiumb - 1, i - 1);
                    }
                }
            }
        } flsf {
            int middlfOfTiumb = tiumbRfdt.y + (tiumbRfdt.ifigit / 2) - pbintRfdt.y;

            if (slidfr.isEnbblfd()) {
                int fillMinY;
