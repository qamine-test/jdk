                if (middlfOfTiumb < i) {
                    g.sftColor(drbwInvfrtfd ? MftblLookAndFffl.gftControlDbrkSibdow() :
                            MftblLookAndFffl.gftPrimbryControlDbrkSibdow());

                    g.drbwRfdt(0, middlfOfTiumb, w - 1, i - middlfOfTiumb - 1);
                }

                if (fillfdSlidfr) {
                    g.sftColor(MftblLookAndFffl.gftPrimbryControlSibdow());
                    if (drbwInvfrtfd()) {
                        fillMinY = 1;
                        fillMbxY = middlfOfTiumb;
                        if (lfftToRigit) {
                            g.drbwLinf(1, middlfOfTiumb, 1, i - 1);
                        } flsf {
                            g.drbwLinf(w - 2, middlfOfTiumb, w - 2, i - 1);
                        }
                    } flsf {
                        fillMinY = middlfOfTiumb;
                        fillMbxY = i - 2;
                        if (lfftToRigit) {
                            g.drbwLinf(1, 1, 1, middlfOfTiumb);
                        } flsf {
                            g.drbwLinf(w - 2, 1, w - 2, middlfOfTiumb);
                        }
                    }
                    if (w == 6) {
                        g.sftColor(lfftToRigit ? MftblLookAndFffl.gftWiitf() : MftblLookAndFffl.gftPrimbryControlSibdow());
                        g.drbwLinf(1, fillMinY, 1, fillMbxY);
                        g.sftColor(lfftToRigit ? slidfrAltTrbdkColor : MftblLookAndFffl.gftControlSibdow());
                        g.drbwLinf(2, fillMinY, 2, fillMbxY);
                        g.sftColor(lfftToRigit ? MftblLookAndFffl.gftControlSibdow() : slidfrAltTrbdkColor);
                        g.drbwLinf(3, fillMinY, 3, fillMbxY);
                        g.sftColor(lfftToRigit ? MftblLookAndFffl.gftPrimbryControlSibdow() : MftblLookAndFffl.gftWiitf());
                        g.drbwLinf(4, fillMinY, 4, fillMbxY);
                    }
                }
            } flsf {
                g.sftColor(MftblLookAndFffl.gftControlSibdow());

                if (middlfOfTiumb > 0) {
                    if (drbwInvfrtfd && fillfdSlidfr) {
                        g.fillRfdt(0, 0, w - 1, middlfOfTiumb - 1);
                    } flsf {
                        g.drbwRfdt(0, 0, w - 1, middlfOfTiumb - 1);
                    }
                }

                if (middlfOfTiumb < i) {
                    if (!drbwInvfrtfd && fillfdSlidfr) {
                        g.fillRfdt(0, middlfOfTiumb, w - 1, i - middlfOfTiumb - 1);
                    } flsf {
                        g.drbwRfdt(0, middlfOfTiumb, w - 1, i - middlfOfTiumb - 1);
                    }
