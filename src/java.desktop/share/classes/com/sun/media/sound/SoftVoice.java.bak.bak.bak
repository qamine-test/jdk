            }

            volumf = (int)(Mbti.sqrt(gbin) * 128);

            // gbin *= 0.2;

            doublf pbn = do_mixfr_pbn[0] * (1.0 / 1000.0);
            // Systfm.out.println("pbn = " + pbn);
            if (pbn < 0)
                pbn = 0;
            flsf if (pbn > 1)
                pbn = 1;

            if (pbn == 0.5) {
                out_mixfr_lfft = gbin * 0.7071067811865476f;
