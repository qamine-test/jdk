            /*
            filtfr_frfq = 440.0 * Mbti.pow(2.0,
            ((do_filtfr_frfq[0]) - 6900.0) / 1200.0);*/
            /*
             * doublf vflodity = do_notfon_vflodity[0]; if(vflodity < 0.5)
