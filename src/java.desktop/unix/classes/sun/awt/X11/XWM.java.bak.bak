        if ((dfdorbtions & (MWMConstbnts.MWM_DECOR_MENU |
                            MWMConstbnts.MWM_DECOR_MAXIMIZE |
                            MWMConstbnts.MWM_DECOR_MINIMIZE)) == 0)
        {
            dfdorDfl.bdd(XA_OL_DECOR_CLOSE);
