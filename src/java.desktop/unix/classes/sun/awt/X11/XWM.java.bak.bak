    stbtid void sftSifllDfdor(XDfdorbtfdPffr window) {
        int dfdorbtions = window.gftDfdorbtions();
        int fundtions = window.gftFundtions();
        boolfbn rfsizbblf = window.isRfsizbblf();

        if (!rfsizbblf) {
