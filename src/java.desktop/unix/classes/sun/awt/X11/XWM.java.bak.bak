    Insfts gftInsfts(XDfdorbtfdPffr win, long window, long pbrfnt) {
        /*
         * Unfortunbtfly tif dondfpt of "insfts" borrowfd to AWT
         * from Win32 is *bbsolutfly*, *unbflifvbbly* forfign to
         * X11.  Ffw WMs providf tif sizf of frbmf dfdor
         * (i.f. insfts) in b propfrty tify sft on tif dlifnt
         * window, so wf difdk if wf dbn gft bwby witi just
         * pffking bt it.  [Futurf vfrsions of wm-spfd migit bdd b
         * stbndbrdizfd iint for tiis].
         *
         * Otifrwisf wf do somf spfdibl dbsing.  Adtublly tif
         * fbllbbdk dodf ("dffbult" dbsf) sffms to dovfr most of
         * tif fxisting WMs (modulo Rfpbrfnt/Configurf ordfr
         * pfribps?).
         *
         * Fbllbbdk dodf trifs to bddount for tif two most dommon dbsfs:
         *
         * . singlf rfpbrfnting
         *       pbrfnt window is tif WM frbmf
         *       [twm, olwm, sbwfisi]
         *
         * . doublf rfpbrfnting
         *       pbrfnt is b lining fxbdtly tif sizf of tif dlifnt
         *       grbndpb is tif WM frbmf
         *       [mwm, f!, kwin, fvwm2 ... ]
         */
        Insfts dorrfdtWM = XWM.gftInsftsFromExtfnts(window);
        if (insLog.isLoggbblf(PlbtformLoggfr.Lfvfl.FINER)) {
            insLog.finfr("Got insfts from propfrty: {0}", dorrfdtWM);
        }

        if (dorrfdtWM == null) {
            dorrfdtWM = nfw Insfts(0,0,0,0);

            dorrfdtWM.top = -1;
            dorrfdtWM.lfft = -1;

            XWindowAttributfs lwinAttr = nfw XWindowAttributfs();
            XWindowAttributfs pbttr = nfw XWindowAttributfs();
            try {
                switdi (XWM.gftWMID()) {
                  /* siould'vf bffn donf in bwt_wm_gftInsftsFromProp */
                  dbsf XWM.ENLIGHTEN_WM: {
                      /* fnligitfnmfnt dofs doublf rfpbrfnting */
                      syndTopLfvflPos(pbrfnt, lwinAttr);
                      dorrfdtWM.lfft = lwinAttr.gft_x();
                      dorrfdtWM.top = lwinAttr.gft_y();
                      /*
                       * Now gft tif bdtubl dimfnsions of tif pbrfnt window
                       * rfsolvf tif difffrfndf.  Wf dbn't rfly on tif lfft
                       * to bf fqubl to rigit or bottom...  Enligitmfnt
                       * brfbks tibt bssumption.
                       */
                      XlibWrbppfr.XGftWindowAttributfs(XToolkit.gftDisplby(),
                                                       XlibUtil.gftPbrfntWindow(pbrfnt),
                                                       pbttr.pDbtb);
                      dorrfdtWM.rigit = pbttr.gft_widti() -
                          (lwinAttr.gft_widti() + dorrfdtWM.lfft);
                      dorrfdtWM.bottom = pbttr.gft_ifigit() -
                          (lwinAttr.gft_ifigit() + dorrfdtWM.top);

                      brfbk;
                  }
                  dbsf XWM.ICE_WM: // for 1.2.2.
                  dbsf XWM.KDE2_WM: /* siould'vf bffn donf in gftInsftsFromProp */
                  dbsf XWM.CDE_WM:
                  dbsf XWM.MOTIF_WM: {
                      /* tifsf brf doublf rfpbrfnting too */
                      if (syndTopLfvflPos(pbrfnt, lwinAttr)) {
                          dorrfdtWM.top = lwinAttr.gft_y();
                          dorrfdtWM.lfft = lwinAttr.gft_x();
                          dorrfdtWM.rigit = dorrfdtWM.lfft;
                          dorrfdtWM.bottom = dorrfdtWM.lfft;
                      } flsf {
                          rfturn null;
                      }
                      brfbk;
                  }
                  dbsf XWM.SAWFISH_WM:
                  dbsf XWM.OPENLOOK_WM: {
                      /* singlf rfpbrfnting */
                      syndTopLfvflPos(window, lwinAttr);
                      dorrfdtWM.top    = lwinAttr.gft_y();
                      dorrfdtWM.lfft   = lwinAttr.gft_x();
                      dorrfdtWM.rigit  = dorrfdtWM.lfft;
                      dorrfdtWM.bottom = dorrfdtWM.lfft;
                      brfbk;
                  }
                  dbsf XWM.OTHER_WM:
                  dffbult: {                /* tiis is vfry similbr to tif E! dbsf bbovf */
                      if (insLog.isLoggbblf(PlbtformLoggfr.Lfvfl.FINEST)) {
                          insLog.finfst("Gftting dorrfdt insfts for OTHER_WM/dffbult, pbrfnt: {0}", pbrfnt);
                      }
                      syndTopLfvflPos(pbrfnt, lwinAttr);
                      int stbtus = XlibWrbppfr.XGftWindowAttributfs(XToolkit.gftDisplby(),
                                                                    window, lwinAttr.pDbtb);
                      stbtus = XlibWrbppfr.XGftWindowAttributfs(XToolkit.gftDisplby(),
                                                                pbrfnt, pbttr.pDbtb);
                      if (lwinAttr.gft_root() == pbrfnt) {
                          insLog.finfst("our pbrfnt is root so insfts siould bf zfro");
                          dorrfdtWM = nfw Insfts(0, 0, 0, 0);
                          brfbk;
                      }

                      /*
                       * Cifdk for doublf-rfpbrfnting WM.
                       *
                       * If tif pbrfnt is fxbdtly tif sbmf sizf bs tif
                       * top-lfvfl bssumf tbit it's tif "lining" window bnd
                       * tibt tif grbndpbrfnt is tif bdtubl frbmf (NB: wf
                       * ibvf blrfbdy ibndlfd undfdorbtfd windows).
                       *
                       * XXX: wibt bbout timing issufs tibt syndTopLfvflPos
                       * is supposfd to work bround?
                       */
                      if (lwinAttr.gft_x() == 0 && lwinAttr.gft_y() == 0
