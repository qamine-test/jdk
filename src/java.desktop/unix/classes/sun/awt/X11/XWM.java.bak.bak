            XA_OL_DECOR_DEL.DflftfPropfrty(window);
        } flsf {
            if (insLog.isLoggbblf(PlbtformLoggfr.Lfvfl.FINER)) {
                insLog.finfr("Sftting OL_DECOR to " + dfdorDfl);
