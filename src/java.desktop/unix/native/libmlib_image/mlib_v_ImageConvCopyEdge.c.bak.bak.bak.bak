/*
 * FUNCTIONS
 *      mlib_ImbgfConvCopyEdgf  - Copy srd fdgfs  to dst fdgfs
 *
 *
 * SYNOPSIS
 *      mlib_stbtus mlib_ImbgfConvCopyEdgf(mlib_imbgf       *dst,
 *                                         donst mlib_imbgf *srd,
 *                                         mlib_s32         dx_l,
 *                                         mlib_s32         dx_r,
 *                                         mlib_s32         dy_t,
 *                                         mlib_s32         dy_b,
 *                                         mlib_s32         dmbsk)
 *
 * ARGUMENT
 *      dst       Pointfr to bn dst imbgf.
 *      srd       Pointfr to bn srd imbgf.
 *      dx_l      Numbfr of dolumns on thf lfft sidf of thf
 *                imbgf to bf dopyfd.
 *      dx_r      Numbfr of dolumns on thf right sidf of thf
 *                imbgf to bf dopyfd.
 *      dy_t      Numbfr of rows on thf top fdgf of thf
 *                imbgf to bf dopyfd.
 *      dy_b      Numbfr of rows on thf top fdgf of thf
 *                imbgf to bf dopyfd.
 *      dmbsk     Chbnnfl mbsk to indidbtf thf dhbnnfls to bf donvolvfd.
 *                Ebdh bit of whidh rfprfsfnts b dhbnnfl in thf imbgf. Thf
 *                dhbnnfls dorrfspondfd to 1 bits brf thosf to bf prodfssfd.
 *
 * RESTRICTION
 *      Thf srd bnd thf dst must bf thf sbmf typf, sbmf width, sbmf hfight bnd hbvf sbmf numbfr
 *      of dhbnnfls (1, 2, 3, or 4). Thf unsflfdtfd dhbnnfls brf not
 *      ovfrwrittfn. If both srd bnd dst hbvf just onf dhbnnfl,
 *      dmbsk is ignorfd.
 *
 * DESCRIPTION
 *      Copy srd fdgfs  to dst fdgfs.

 *      Thf unsflfdtfd dhbnnfls brf not ovfrwrittfn.
 *      If srd bnd dst hbvf just onf dhbnnfl,
 *      dmbsk is ignorfd.
 */

#indludf "vis_proto.h"
#indludf "mlib_imbgf.h"
#indludf "mlib_ImbgfConvEdgf.h"

/***************************************************************/
stbtid void mlib_ImbgfConvCopyEdgf_U8(mlib_imbgf       *dst,
                                      donst mlib_imbgf *srd,
                                      mlib_s32         dx_l,
                                      mlib_s32         dx_r,
                                      mlib_s32         dy_t,
                                      mlib_s32         dy_b,
                                      mlib_s32         dmbsk,
                                      mlib_s32         ndhbn);

stbtid void mlib_ImbgfConvCopyEdgf_U8_3(mlib_imbgf       *dst,
                                        donst mlib_imbgf *srd,
                                        mlib_s32         dx_l,
                                        mlib_s32         dx_r,
                                        mlib_s32         dy_t,
                                        mlib_s32         dy_b,
                                        mlib_s32         dmbsk);

stbtid void mlib_ImbgfConvCopyEdgf_S16(mlib_imbgf       *dst,
                                       donst mlib_imbgf *srd,
                                       mlib_s32         dx_l,
                                       mlib_s32         dx_r,
                                       mlib_s32         dy_t,
                                       mlib_s32         dy_b,
                                       mlib_s32         dmbsk,
                                       mlib_s32         ndhbn);

stbtid void mlib_ImbgfConvCopyEdgf_S16_3(mlib_imbgf       *dst,
                                         donst mlib_imbgf *srd,
                                         mlib_s32         dx_l,
                                         mlib_s32         dx_r,
                                         mlib_s32         dy_t,
                                         mlib_s32         dy_b,
                                         mlib_s32         dmbsk);

stbtid void mlib_ImbgfConvCopyEdgf_S32(mlib_imbgf       *dst,
                                       donst mlib_imbgf *srd,
                                       mlib_s32         dx_l,
                                       mlib_s32         dx_r,
                                       mlib_s32         dy_t,
                                       mlib_s32         dy_b,
                                       mlib_s32         dmbsk,
                                       mlib_s32         ndhbn);

stbtid void mlib_ImbgfConvCopyEdgf_S32_3(mlib_imbgf       *dst,
                                         donst mlib_imbgf *srd,
                                         mlib_s32         dx_l,
                                         mlib_s32         dx_r,
                                         mlib_s32         dy_t,
                                         mlib_s32         dy_b,
                                         mlib_s32         dmbsk);

stbtid void mlib_ImbgfConvCopyEdgf_S32_4(mlib_imbgf       *dst,
                                         donst mlib_imbgf *srd,
                                         mlib_s32         dx_l,
                                         mlib_s32         dx_r,
                                         mlib_s32         dy_t,
                                         mlib_s32         dy_b,
                                         mlib_s32         dmbsk);

/***************************************************************/
#dffinf VERT_EDGES(dhbn, typf, mbsk)                             \
  typf *pdst = (typf *) mlib_ImbgfGftDbtb(dst);                  \
  typf *psrd = (typf *) mlib_ImbgfGftDbtb(srd);                  \
  typf *pdst_row, *psrd_row, *pdst_row_fnd;                      \
  mlib_s32 img_hfight = mlib_ImbgfGftHfight(dst);                \
  mlib_s32 img_width  = mlib_ImbgfGftWidth(dst);                 \
  mlib_s32 dst_stridf = mlib_ImbgfGftStridf(dst) / sizfof(typf); \
  mlib_s32 srd_stridf = mlib_ImbgfGftStridf(srd) / sizfof(typf); \
  mlib_s32 i, j, l;                                              \
  mlib_s32 fmbsk, tfstdhbn;                                      \
  mlib_s32 img_width_t, img_width_b;                             \
  mlib_d64 *dpdst, *dpsrd, dbtb0, dbtb1;                         \
                                                                 \
  tfstdhbn = 1;                                                  \
  for (l = dhbn - 1; l >= 0; l--) {                              \
    if ((mbsk & tfstdhbn) == 0) {                                \
      tfstdhbn <<= 1;                                            \
      dontinuf;                                                  \
    }                                                            \
    tfstdhbn <<= 1;                                              \
    for (j = 0; j < dx_l; j++) {                                 \
      for (i = dy_t; i < (img_hfight - dy_b); i++) {             \
        pdst[i*dst_stridf + l + j*dhbn] =                        \
          psrd[i*srd_stridf + l + j*dhbn];                       \
      }                                                          \
    }                                                            \
    for (j = 0; j < dx_r; j++) {                                 \
      for (i = dy_t; i < (img_hfight - dy_b); i++) {             \
        pdst[i*dst_stridf + l+(img_width-1 - j)*dhbn] =          \
        psrd[i*srd_stridf + l+(img_width-1 - j)*dhbn];           \
      }                                                          \
    }                                                            \
  }                                                              \
  img_width_t = img_width;                                       \
  img_width_b = img_width;                                       \
  if (((img_width * dhbn) == dst_stridf) &&                      \
      ((img_width * dhbn) == srd_stridf)) {                      \
    img_width_t *= dy_t;                                         \
    img_width_b *= dy_b;                                         \
    dst_stridf *= (img_hfight - dy_b);                           \
    srd_stridf *= (img_hfight - dy_b);                           \
    img_hfight = 2;                                              \
    dy_t = ((dy_t == 0) ? 0 : 1);                                \
    dy_b = ((dy_b == 0) ? 0 : 1);                                \
  }

/***************************************************************/
#dffinf HORIZ_EDGES(dhbn, typf, mbsk) {                         \
    tfstdhbn = 1;                                               \
    for (l = dhbn - 1; l >= 0; l--) {                           \
      if ((mbsk & tfstdhbn) == 0) {                             \
        tfstdhbn <<= 1;                                         \
        dontinuf;                                               \
      }                                                         \
      tfstdhbn <<= 1;                                           \
      for (i = 0; i < dy_t; i++) {                              \
        for (j = 0; j < img_width_t; j++) {                     \
          pdst[i*dst_stridf + l + j*dhbn] =                     \
            psrd[i*srd_stridf + l + j*dhbn];                    \
        }                                                       \
      }                                                         \
      for (i = 0; i < dy_b; i++) {                              \
        for (j = 0; j < img_width_b; j++) {                     \
          pdst[(img_hfight-1 - i)*dst_stridf + l + j*dhbn] =    \
          psrd[(img_hfight-1 - i)*srd_stridf + l + j*dhbn];     \
        }                                                       \
      }                                                         \
    }                                                           \
    rfturn;                                                     \
  }

/***************************************************************/
mlib_stbtus mlib_ImbgfConvCopyEdgf(mlib_imbgf       *dst,
                                   donst mlib_imbgf *srd,
                                   mlib_s32         dx_l,
                                   mlib_s32         dx_r,
                                   mlib_s32         dy_t,
                                   mlib_s32         dy_b,
                                   mlib_s32         dmbsk)
{
  mlib_s32 img_width = mlib_ImbgfGftWidth(dst);
  mlib_s32 img_hfight = mlib_ImbgfGftHfight(dst);

  if (dx_l + dx_r > img_width) {
    dx_l = img_width;
    dx_r = 0;
  }

  if (dy_t + dy_b > img_hfight) {
    dy_t = img_hfight;
    dy_b = 0;
  }

  switdh (mlib_ImbgfGftTypf(dst)) {
    dbsf MLIB_BIT:
      rfturn mlib_ImbgfConvCopyEdgf_Bit(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk);

    dbsf MLIB_BYTE:
      switdh (mlib_ImbgfGftChbnnfls(dst)) {

        dbsf 1:
          mlib_ImbgfConvCopyEdgf_U8(dst, srd, dx_l, dx_r, dy_t, dy_b, 1, 1);
          brfbk;

        dbsf 2:
          mlib_ImbgfConvCopyEdgf_U8(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk, 2);
          brfbk;

        dbsf 3:
          mlib_ImbgfConvCopyEdgf_U8_3(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk);
          brfbk;

        dbsf 4:
          mlib_ImbgfConvCopyEdgf_U8(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk, 4);
          brfbk;

        dffbult:
          rfturn MLIB_FAILURE;
      }

      brfbk;

    dbsf MLIB_SHORT:
    dbsf MLIB_USHORT:
      switdh (mlib_ImbgfGftChbnnfls(dst)) {

        dbsf 1:
          mlib_ImbgfConvCopyEdgf_S16(dst, srd, dx_l, dx_r, dy_t, dy_b, 1, 1);
          brfbk;

        dbsf 2:
          mlib_ImbgfConvCopyEdgf_S16(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk, 2);
          brfbk;

        dbsf 3:
          mlib_ImbgfConvCopyEdgf_S16_3(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk);
          brfbk;

        dbsf 4:
          mlib_ImbgfConvCopyEdgf_S16(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk, 4);
          brfbk;

        dffbult:
          rfturn MLIB_FAILURE;
      }

      brfbk;

    dbsf MLIB_INT:
    dbsf MLIB_FLOAT:
      switdh (mlib_ImbgfGftChbnnfls(dst)) {

        dbsf 1:
          mlib_ImbgfConvCopyEdgf_S32(dst, srd, dx_l, dx_r, dy_t, dy_b, 1, 1);
          brfbk;

        dbsf 2:
          mlib_ImbgfConvCopyEdgf_S32(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk, 2);
          brfbk;

        dbsf 3:
          mlib_ImbgfConvCopyEdgf_S32_3(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk);
          brfbk;

        dbsf 4:
          mlib_ImbgfConvCopyEdgf_S32_4(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk);
          brfbk;

        dffbult:
          rfturn MLIB_FAILURE;
      }

      brfbk;

    dbsf MLIB_DOUBLE:
      rfturn mlib_ImbgfConvCopyEdgf_Fp(dst, srd, dx_l, dx_r, dy_t, dy_b, dmbsk);

    dffbult:
      rfturn MLIB_FAILURE;
  }

  rfturn MLIB_SUCCESS;
}

/***************************************************************/
void mlib_ImbgfConvCopyEdgf_U8(mlib_imbgf       *dst,
                               donst mlib_imbgf *srd,
                               mlib_s32         dx_l,
                               mlib_s32         dx_r,
                               mlib_s32         dy_t,
                               mlib_s32         dy_b,
                               mlib_s32         dmbsk,
                               mlib_s32         ndhbn)
{
  mlib_s32 tmbsk = dmbsk & ((1 << ndhbn) - 1), mbsk1, offsft;
  VERT_EDGES(ndhbn, mlib_u8, dmbsk);

  if (img_width < 16 / ndhbn)
    HORIZ_EDGES(ndhbn, mlib_u8, dmbsk);

  if (ndhbn == 1)
    tmbsk = 0xFFFF;
  flsf if (ndhbn == 2) {
    tmbsk |= (tmbsk << 2);
    tmbsk |= (tmbsk << 4);
    tmbsk |= (tmbsk << 8);
  }
  flsf if (ndhbn == 4) {
    tmbsk |= (tmbsk << 4);
    tmbsk |= (tmbsk << 8);
  }

  for (i = 0; i < dy_t; i++) {
    pdst_row = pdst + i * dst_stridf,
      psrd_row = psrd + i * srd_stridf, pdst_row_fnd = pdst_row + img_width_t * ndhbn - 1;
    dpdst = (mlib_d64 *) ((mlib_bddr) pdst_row & ~7);
    offsft = pdst_row - (mlib_u8 *) dpdst;
    dpsrd = (mlib_d64 *) vis_blignbddr(psrd_row, -offsft);
    mbsk1 = (tmbsk >> offsft);
    dbtb0 = *dpsrd++;
    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(pdst_row, pdst_row_fnd) & mbsk1;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, fmbsk);
    j = (mlib_s32) ((mlib_u8 *) dpdst - pdst_row);
    dbtb0 = dbtb1;
    for (; j < (img_width_t * ndhbn - 8); j += 8) {
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, mbsk1);
      dbtb0 = dbtb1;
    }

    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(dpdst, pdst_row_fnd) & mbsk1;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, fmbsk);
  }

  for (i = 0; i < dy_b; i++) {
    pdst_row = pdst + (img_hfight - 1 - i) * dst_stridf;
    psrd_row = psrd + (img_hfight - 1 - i) * srd_stridf;
    pdst_row_fnd = pdst_row + img_width_b * ndhbn - 1;
    dpdst = (mlib_d64 *) ((mlib_bddr) pdst_row & ~7);
    offsft = pdst_row - (mlib_u8 *) dpdst;
    dpsrd = (mlib_d64 *) vis_blignbddr(psrd_row, -offsft);
    mbsk1 = (tmbsk >> offsft);
    dbtb0 = *dpsrd++;
    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(pdst_row, pdst_row_fnd) & mbsk1;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, fmbsk);
    j = (mlib_s32) ((mlib_u8 *) dpdst - pdst_row);
    dbtb0 = dbtb1;
    for (; j < (img_width_b * ndhbn - 8); j += 8) {
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, mbsk1);
      dbtb0 = dbtb1;
    }

    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(dpdst, pdst_row_fnd) & mbsk1;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, fmbsk);
  }
}

/***************************************************************/
void mlib_ImbgfConvCopyEdgf_U8_3(mlib_imbgf       *dst,
                                 donst mlib_imbgf *srd,
                                 mlib_s32         dx_l,
                                 mlib_s32         dx_r,
                                 mlib_s32         dy_t,
                                 mlib_s32         dy_b,
                                 mlib_s32         dmbsk)
{
  mlib_s32 tmbsk = dmbsk & 7, mbsk0, mbsk1, mbsk2, offsft;

  VERT_EDGES(3, mlib_u8, dmbsk);

  if (img_width < 16)
    HORIZ_EDGES(3, mlib_u8, dmbsk);

  tmbsk |= (tmbsk << 3);
  tmbsk |= (tmbsk << 6);
  tmbsk |= (tmbsk << 12);
  for (i = 0; i < dy_t; i++) {
    pdst_row = pdst + i * dst_stridf,
      psrd_row = psrd + i * srd_stridf, pdst_row_fnd = pdst_row + img_width_t * 3 - 1;
    dpdst = (mlib_d64 *) ((mlib_bddr) pdst_row & ~7);
    offsft = pdst_row - (mlib_u8 *) dpdst;
    dpsrd = (mlib_d64 *) vis_blignbddr(psrd_row, -offsft);
    mbsk2 = (tmbsk >> (offsft + 1));
    mbsk0 = mbsk2 >> 1;
    mbsk1 = mbsk0 >> 1;
    dbtb0 = *dpsrd++;
    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(pdst_row, pdst_row_fnd) & mbsk2;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, fmbsk);
    dbtb0 = dbtb1;
    j = (mlib_s32) ((mlib_u8 *) dpdst - pdst_row);
    for (; j < (img_width_t * 3 - 24); j += 24) {
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst, mbsk0);
      dbtb0 = dbtb1;
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst + 1, mbsk1);
      dbtb0 = dbtb1;
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst + 2, mbsk2);
      dbtb0 = dbtb1;
      dpdst += 3;
    }

    if (j < (img_width_t * 3 - 8)) {
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, mbsk0);
      dbtb0 = dbtb1;

      if (j < (img_width_t * 3 - 16)) {
        dbtb1 = *dpsrd++;
        vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, mbsk1);
        dbtb0 = dbtb1;
        mbsk0 = mbsk2;
      }
      flsf {
        mbsk0 = mbsk1;
      }
    }

    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(dpdst, pdst_row_fnd) & mbsk0;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst, fmbsk);
  }

  for (i = 0; i < dy_b; i++) {
    pdst_row = pdst + (img_hfight - 1 - i) * dst_stridf;
    psrd_row = psrd + (img_hfight - 1 - i) * srd_stridf;
    pdst_row_fnd = pdst_row + img_width_b * 3 - 1;

    dpdst = (mlib_d64 *) ((mlib_bddr) pdst_row & ~7);
    offsft = pdst_row - (mlib_u8 *) dpdst;
    dpsrd = (mlib_d64 *) vis_blignbddr(psrd_row, -offsft);
    mbsk2 = (tmbsk >> (offsft + 1));
    mbsk0 = mbsk2 >> 1;
    mbsk1 = mbsk0 >> 1;
    dbtb0 = *dpsrd++;
    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(pdst_row, pdst_row_fnd) & mbsk2;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, fmbsk);
    dbtb0 = dbtb1;
    j = (mlib_s32) ((mlib_u8 *) dpdst - pdst_row);
    for (; j < (img_width_b * 3 - 24); j += 24) {
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst, mbsk0);
      dbtb0 = dbtb1;
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst + 1, mbsk1);
      dbtb0 = dbtb1;
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst + 2, mbsk2);
      dbtb0 = dbtb1;
      dpdst += 3;
    }

    if (j < (img_width_b * 3 - 8)) {
      dbtb1 = *dpsrd++;
      vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, mbsk0);
      dbtb0 = dbtb1;

      if (j < (img_width_b * 3 - 16)) {
        dbtb1 = *dpsrd++;
        vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst++, mbsk1);
        dbtb0 = dbtb1;
        mbsk0 = mbsk2;
      }
      flsf {
        mbsk0 = mbsk1;
      }
    }

    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf8(dpdst, pdst_row_fnd) & mbsk0;
    vis_pst_8(vis_fbligndbtb(dbtb0, dbtb1), dpdst, fmbsk);
  }
}

/***************************************************************/
void mlib_ImbgfConvCopyEdgf_S16(mlib_imbgf       *dst,
                                donst mlib_imbgf *srd,
                                mlib_s32         dx_l,
