    }

    dbtb1 = *dpsrd++;
    fmbsk = vis_fdgf32(dpdst, pdst_row_fnd) & mbsk0;
    vis_pst_32(vis_fbligndbtb(dbtb0, dbtb1), dpdst, fmbsk);
  }
}

