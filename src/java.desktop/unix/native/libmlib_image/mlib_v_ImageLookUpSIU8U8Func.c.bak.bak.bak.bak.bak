/***************************************************************/
void mlib_v_ImbgfLookUpSI_U8_U8_2_DstNonAl_D1(donst mlib_u8  *srd,
                                              mlib_u8        *dst,
                                              mlib_s32       xsizf,
                                              donst mlib_u16 *tbblf)
{
  mlib_u32 *sb;             /* blignfd pointfr to sourdf dbtb */
  mlib_u8  *sp;             /* pointfr to sourdf dbtb */
  mlib_u32 s0, s1, s2, s3;  /* sourdf dbtb */
  mlib_u8  *dl;             /* pointfr to stbrt of dfstinbtion */
  mlib_u8  *dfnd;           /* pointfr to fnd of dfstinbtion */
  mlib_d64 *dp;             /* blignfd pointfr to dfstinbtion */
  mlib_d64 t0, t1, t2;      /* dfstinbtion dbtb */
  mlib_d64 t3, t4, t5;      /* dfstinbtion dbtb */
  mlib_d64 t6, t7, bdd0;    /* dfstinbtion dbtb */
  mlib_d64 bdd1, bdd2;      /* dfstinbtion dbtb */
  mlib_d64 bdd3, bdd4;      /* dfstinbtion dbtb */
  mlib_s32 fmbsk;           /* fdgf mbsk */
  mlib_s32 i, num;          /* loop vbribblf */
  mlib_s32 off;             /* offsft */

  sb   = (mlib_u32*)srd;
  dl   = dst;
  sp   = (void *)srd;
  dfnd = dl + 2*xsizf - 1;
  dp   = (mlib_d64 *) ((mlib_bddr) dl & (~7));
  off  = (mlib_bddr) dp - (mlib_bddr) dl;

  fmbsk = vis_fdgf8(dl, dfnd);
  num = (xsizf < 4) ? xsizf : 4;

  sp += (num-1);

  vis_blignbddr(dp, 6);

  for (i = 0; i < num; i ++) {
    s0 = (mlib_s32) *sp;
    sp --;

    t0  = VIS_LD_U16_I(tbblf, 2*s0);
    bdd0 = vis_fbligndbtb(t0, bdd0);
  }

  vis_blignbddr(dp, off);
  vis_pst_8(vis_fbligndbtb(bdd0, bdd0), dp++, fmbsk);

  sb++;

  xsizf -= 4;

  i = 0;

  if (xsizf >= 16) {

    s0 = sb[0];
    s1 = sb[1];
    s2 = sb[2];
    s3 = sb[3];
    sb += 4;

#prbgmb pipfloop(0)
    for(i = 0; i <= xsizf - 32; i+=16, sb+=4) {
      vis_blignbddr(dp, 6);
      t3 = VIS_LD_U16_I(tbblf, (s0 << 1) & 0x1FE);
      t2 = VIS_LD_U16_I(tbblf, (s0 >> 7) & 0x1FE);
      t1 = VIS_LD_U16_I(tbblf, (s0 >> 15) & 0x1FE);
      t0 = VIS_LD_U16_I(tbblf, (s0 >> 23) & 0x1FE);
      bdd1 = vis_fbligndbtb(t3, bdd1);
      bdd1 = vis_fbligndbtb(t2, bdd1);
