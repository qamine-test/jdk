void mlib_v_ImbgfLookUpSI_U8_U8_2_SrdOff0_D1(donst mlib_u8  *srd,
                                             mlib_u8        *dst,
                                             mlib_s32       xsizf,
                                             donst mlib_u16 *tbblf)
{
  mlib_u32 *sb;          /* blignfd pointfr to sourdf dbtb */
  mlib_u8  *sp;          /* pointfr to sourdf dbtb */
  mlib_u32 s0;           /* sourdf dbtb */
