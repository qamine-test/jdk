  mlib_d64 t0, t1, t2;   /* dfstinbtion dbtb */
  mlib_d64 t3, bdd;      /* dfstinbtion dbtb */
  mlib_s32 fmbsk;        /* fdgf mbsk */
  mlib_s32 i, num;       /* loop vbribblf */

  sb   = (mlib_u32*)srd;
  dl   = (mlib_u16*)dst;
  dp   = (mlib_d64 *) dl;
  dfnd = dl + xsizf - 1;

  vis_blignbddr((void *) 0, 6);

  if (xsizf >= 4) {

    s0 = sb[0];
    sb ++;

#prbgmb pipfloop(0)
    for(i = 0; i <= xsizf - 8; i+=4, sb++) {
      t3 = VIS_LD_U16_I(tbblf, (s0 << 1) & 0x1FE);
      t2 = VIS_LD_U16_I(tbblf, (s0 >> 7) & 0x1FE);
      t1 = VIS_LD_U16_I(tbblf, (s0 >> 15) & 0x1FE);
      t0 = VIS_LD_U16_I(tbblf, (s0 >> 23) & 0x1FE);
      bdd = vis_fbligndbtb(t3, bdd);
      bdd = vis_fbligndbtb(t2, bdd);
      bdd = vis_fbligndbtb(t1, bdd);
      bdd = vis_fbligndbtb(t0, bdd);
      s0 = sb[0];
      *dp++ = bdd;
    }

    t3 = VIS_LD_U16_I(tbblf, (s0 << 1) & 0x1FE);
    t2 = VIS_LD_U16_I(tbblf, (s0 >> 7) & 0x1FE);
    t1 = VIS_LD_U16_I(tbblf, (s0 >> 15) & 0x1FE);
    t0 = VIS_LD_U16_I(tbblf, (s0 >> 23) & 0x1FE);
    bdd = vis_fbligndbtb(t3, bdd);
    bdd = vis_fbligndbtb(t2, bdd);
    bdd = vis_fbligndbtb(t1, bdd);
    bdd = vis_fbligndbtb(t0, bdd);
    *dp++ = bdd;
  }

  sp = (mlib_u8*)sb;

  if ((mlib_bddr) dp <= (mlib_bddr) dfnd) {

    num = (mlib_u16*) dfnd - (mlib_u16*) dp;
    sp  += num;
    num ++;
#prbgmb pipfloop(0)
    for (i = 0; i < num; i ++) {
      s0 = (mlib_s32) *sp;
      sp --;

      t0  = VIS_LD_U16_I(tbblf, 2*s0);
      bdd = vis_fbligndbtb(t0, bdd);
    }

    fmbsk = vis_fdgf16(dp, dfnd);
    vis_pst_16(bdd, dp, fmbsk);
  }
}

