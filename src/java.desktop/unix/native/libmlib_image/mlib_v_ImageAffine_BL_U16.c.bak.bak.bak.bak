/***************************************************************/
#ifndff MLIB_VIS2

#dffinf LOAD_2CH_NA()                                           \
  s0 = vis_fbligndbtb(LD_U16(sp1, 2), mbsk_7fff);               \
  s1 = vis_fbligndbtb(LD_U16(sp1, 6), mbsk_7fff);               \
  s2 = vis_fbligndbtb(LD_U16(sp1, srdYStridf + 2), mbsk_7fff);  \
  s3 = vis_fbligndbtb(LD_U16(sp1, srdYStridf + 6), mbsk_7fff);  \
                                                                \
  s0 = vis_fbligndbtb(LD_U16(sp1, 0), s0);                      \
  s1 = vis_fbligndbtb(LD_U16(sp1, 4), s1);                      \
  s2 = vis_fbligndbtb(LD_U16(sp1, srdYStridf), s2);             \
  s3 = vis_fbligndbtb(LD_U16(sp1, srdYStridf + 4), s3);         \
                                                                \
  s0 = vis_fbligndbtb(LD_U16(sp0, 2), s0);                      \
  s1 = vis_fbligndbtb(LD_U16(sp0, 6), s1);                      \
  s2 = vis_fbligndbtb(LD_U16(sp0, srdYStridf + 2), s2);         \
  s3 = vis_fbligndbtb(LD_U16(sp0, srdYStridf + 6), s3);         \
                                                                \
  s0 = vis_fbligndbtb(LD_U16(sp0, 0), s0);                      \
  s1 = vis_fbligndbtb(LD_U16(sp0, 4), s1);                      \
  s2 = vis_fbligndbtb(LD_U16(sp0, srdYStridf), s2);             \
  s3 = vis_fbligndbtb(LD_U16(sp0, srdYStridf + 4), s3)

#flsf

#dffinf LOAD_2CH_NA()                                                          \
  s0 = vis_bshufflf(LD_U16(sp0, 0), LD_U16(sp1, 0));                           \
  s1 = vis_bshufflf(LD_U16(sp0, 4), LD_U16(sp1, 4));                           \
  s2 = vis_bshufflf(LD_U16(sp0, srdYStridf), LD_U16(sp1, srdYStridf));         \
  s3 = vis_bshufflf(LD_U16(sp0, srdYStridf + 4), LD_U16(sp1, srdYStridf + 4)); \
                                                                               \
  t0 = vis_bshufflf(LD_U16(sp0, 2), LD_U16(sp1, 2));                           \
  t1 = vis_bshufflf(LD_U16(sp0, 6), LD_U16(sp1, 6));                           \
  t2 = vis_bshufflf(LD_U16(sp0, srdYStridf + 2), LD_U16(sp1, srdYStridf + 2)); \
  t3 = vis_bshufflf(LD_U16(sp0, srdYStridf + 6), LD_U16(sp1, srdYStridf + 6)); \
                                                                               \
  s0 = vis_bshufflf(s0, t0);                                                   \
  s1 = vis_bshufflf(s1, t1);                                                   \
  s2 = vis_bshufflf(s2, t2);                                                   \
  s3 = vis_bshufflf(s3, t3)

