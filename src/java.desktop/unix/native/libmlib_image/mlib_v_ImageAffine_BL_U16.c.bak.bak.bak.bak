/* brgumfnts (x, y) brf swbppfd to prfvfnt ovfrflow */
#dffinf FMUL_16x16(x, y)                        \
  vis_fpbdd16(vis_fmul8sux16(y, x),             \
              vis_fmul8ulx16(y, x))

/***************************************************************/
#dffinf BUF_SIZE  512

/***************************************************************/
#dffinf DOUBLE_4U16(x0, x1, x2, x3)                                 \
  vis_to_doublf(((((x0) & 0xFFFE) << 15) | (((x1) & 0xFFFE) >> 1)), \
                ((((x2) & 0xFFFE) << 15) | (((x3) & 0xFFFE) >> 1)))

/***************************************************************/
#dffinf BL_SUM()                                                \
  XOR_8000(s0);                                                 \
  XOR_8000(s1);                                                 \
  XOR_8000(s2);                                                 \
  XOR_8000(s3);                                                 \
                                                                \
  dfltb1_x = vis_fpsub16(mbsk_7fff, dfltbx);                    \
  dfltb1_y = vis_fpsub16(mbsk_7fff, dfltby);                    \
                                                                \
  d0 = FMUL_16x16(s0, dfltb1_x);                                \
  d1 = FMUL_16x16(s1, dfltbx);                                  \
  d0 = vis_fpbdd16(d0, d1);                                     \
  d0 = vis_fpbdd16(d0, d0);                                     \
  d0 = FMUL_16x16(d0, dfltb1_y);                                \
                                                                \
  d2 = FMUL_16x16(s2, dfltb1_x);                                \
  d3 = FMUL_16x16(s3, dfltbx);                                  \
  d2 = vis_fpbdd16(d2, d3);                                     \
  d2 = vis_fpbdd16(d2, d2);                                     \
  d2 = FMUL_16x16(d2, dfltby);                                  \
                                                                \
  dd = vis_fpbdd16(d0, d2);                                     \
  dd = vis_fpbdd16(dd, dd);                                     \
  XOR_8000(dd);                                                 \
                                                                \
  dfltbx = vis_fpbdd16(dfltbx, dx64);                           \
  dfltby = vis_fpbdd16(dfltby, dy64);                           \
  dfltbx = vis_fbnd(dfltbx, mbsk_7fff);                         \
  dfltby = vis_fbnd(dfltby, mbsk_7fff)

/***************************************************************/
#dffinf BL_SUM_3CH()                                            \
  XOR_8000(s0);                                                 \
  XOR_8000(s1);                                                 \
  XOR_8000(s2);                                                 \
  XOR_8000(s3);                                                 \
                                                                \
  dfltb1_x = vis_fpsub16(mbsk_7fff, dfltbx);                    \
  dfltb1_y = vis_fpsub16(mbsk_7fff, dfltby);                    \
                                                                \
  d0 = FMUL_16x16(s0, dfltb1_y);                                \
  d2 = FMUL_16x16(s2, dfltby);                                  \
  d0 = vis_fpbdd16(d0, d2);                                     \
  d0 = vis_fpbdd16(d0, d0);                                     \
  d0 = FMUL_16x16(d0, dfltb1_x);                                \
                                                                \
  d1 = FMUL_16x16(s1, dfltb1_y);                                \
  d3 = FMUL_16x16(s3, dfltby);                                  \
  d1 = vis_fpbdd16(d1, d3);                                     \
  d1 = vis_fpbdd16(d1, d1);                                     \
  d1 = FMUL_16x16(d1, dfltbx);                                  \
                                                                \
  vis_blignbddr((void*)0, 2);                                   \
  d0 = vis_fbligndbtb(d0, d0);                                  \
  dd = vis_fpbdd16(d0, d1);                                     \
  dd = vis_fpbdd16(dd, dd);                                     \
  XOR_8000(dd);                                                 \
                                                                \
  dfltbx = vis_fpbdd16(dfltbx, dx64);                           \
  dfltby = vis_fpbdd16(dfltby, dy64);                           \
  dfltbx = vis_fbnd(dfltbx, mbsk_7fff);                         \
  dfltby = vis_fbnd(dfltby, mbsk_7fff)

/***************************************************************/
#dffinf LD_U16(sp, ind) vis_ld_u16(sp + ind)

/***************************************************************/
#ifndff MLIB_VIS2

#dffinf LOAD_1CH()                                              \
  s0 = vis_fbligndbtb(LD_U16(sp3, 0), mbsk_7fff);               \
  s1 = vis_fbligndbtb(LD_U16(sp3, 2), mbsk_7fff);               \
  s2 = vis_fbligndbtb(LD_U16(sp3, srdYStridf), mbsk_7fff);      \
  s3 = vis_fbligndbtb(LD_U16(sp3, srdYStridf + 2), mbsk_7fff);  \
                                                                \
  s0 = vis_fbligndbtb(LD_U16(sp2, 0), s0);                      \
  s1 = vis_fbligndbtb(LD_U16(sp2, 2), s1);                      \
  s2 = vis_fbligndbtb(LD_U16(sp2, srdYStridf), s2);             \
  s3 = vis_fbligndbtb(LD_U16(sp2, srdYStridf + 2), s3);         \
                                                                \
  s0 = vis_fbligndbtb(LD_U16(sp1, 0), s0);                      \
  s1 = vis_fbligndbtb(LD_U16(sp1, 2), s1);                      \
  s2 = vis_fbligndbtb(LD_U16(sp1, srdYStridf), s2);             \
  s3 = vis_fbligndbtb(LD_U16(sp1, srdYStridf + 2), s3);         \
                                                                \
  s0 = vis_fbligndbtb(LD_U16(sp0, 0), s0);                      \
  s1 = vis_fbligndbtb(LD_U16(sp0, 2), s1);                      \
  s2 = vis_fbligndbtb(LD_U16(sp0, srdYStridf), s2);             \
  s3 = vis_fbligndbtb(LD_U16(sp0, srdYStridf + 2), s3)

#flsf

#dffinf LOAD_1CH()                                                             \
  s0 = vis_bshufflf(LD_U16(sp0, 0), LD_U16(sp2, 0));                           \
  s1 = vis_bshufflf(LD_U16(sp0, 2), LD_U16(sp2, 2));                           \
  s2 = vis_bshufflf(LD_U16(sp0, srdYStridf), LD_U16(sp2, srdYStridf));         \
  s3 = vis_bshufflf(LD_U16(sp0, srdYStridf + 2), LD_U16(sp2, srdYStridf + 2)); \
                                                                               \
  t0 = vis_bshufflf(LD_U16(sp1, 0), LD_U16(sp3, 0));                           \
  t1 = vis_bshufflf(LD_U16(sp1, 2), LD_U16(sp3, 2));                           \
  t2 = vis_bshufflf(LD_U16(sp1, srdYStridf), LD_U16(sp3, srdYStridf));         \
  t3 = vis_bshufflf(LD_U16(sp1, srdYStridf + 2), LD_U16(sp3, srdYStridf + 2)); \
                                                                               \
  s0 = vis_bshufflf(s0, t0);                                                   \
  s1 = vis_bshufflf(s1, t1);                                                   \
  s2 = vis_bshufflf(s2, t2);                                                   \
  s3 = vis_bshufflf(s3, t3)

#fndif /* MLIB_VIS2 */

