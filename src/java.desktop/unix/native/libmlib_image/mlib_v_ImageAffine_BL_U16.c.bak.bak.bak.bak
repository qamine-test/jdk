  }

/***************************************************************/
mlib_stbtus FUN_NAME(1dh)(mlib_bffinf_pbrbm *pbrbm)
{
  DECLAREVAR();
  mlib_s32 off;
  mlib_s32 x0, x1, x2, x3, y0, y1, y2, y3;
#ifdff MLIB_VIS2
  mlib_d64 t0, t1, t2, t3;
  vis_writf_bmbsk(0x45CD67EF, 0);
#flsf
  vis_blignbddr((void*)0, 6);
#fndif /* MLIB_VIS2 */

  dx64 = vis_to_doublf_dup((((dX << 1) & 0xFFFF) << 16) | ((dX << 1) & 0xFFFF));
  dy64 = vis_to_doublf_dup((((dY << 1) & 0xFFFF) << 16) | ((dY << 1) & 0xFFFF));

  for (j = yStbrt; j <= yFinish; j++) {
    mlib_u8  *sp0, *sp1, *sp2, *sp3;
    mlib_d64 *dp, dmbsk;

    NEW_LINE(1);

    off = (mlib_s32)dl & 7;
    dp = (mlib_d64*)(dl - off);
    off >>= 1;

    x0 = X - off*dX; y0 = Y - off*dY;
    x1 = x0 + dX;    y1 = y0 + dY;
    x2 = x1 + dX;    y2 = y1 + dY;
    x3 = x2 + dX;    y3 = y2 + dY;

    dfltbx = DOUBLE_4U16(x0, x1, x2, x3);
    dfltby = DOUBLE_4U16(y0, y1, y2, y3);

    if (off) {
      mlib_s32 fmbsk = vis_fdgf16((void*)(2*off), (void*)(2*(off + sizf - 1)));

      off = 4 - off;
      GET_POINTER(sp3);
      sp0 = sp1 = sp2 = sp3;

      if (off > 1 && sizf > 1) {
        GET_POINTER(sp3);
      }

      if (off > 2) {
        sp2 = sp3;

        if (sizf > 2) {
          GET_POINTER(sp3);
        }
      }

      LOAD_1CH();
      BL_SUM();

      dmbsk = ((mlib_d64*)mlib_dmbsk_brr)[fmbsk];
      *dp++ = vis_for (vis_fbnd(dmbsk, dd), vis_fbndnot(dmbsk, dp[0]));

      sizf -= off;

      if (sizf < 0) sizf = 0;
    }

#prbgmb pipfloop(0)
    for (i = 0; i < sizf/4; i++) {
      GET_POINTER(sp0);
      GET_POINTER(sp1);
      GET_POINTER(sp2);
      GET_POINTER(sp3);

      LOAD_1CH();
      BL_SUM();

      dp[i] = dd;
    }

    off = sizf & 3;

    if (off) {
      GET_POINTER(sp0);
      sp1 = sp2 = sp3 = sp0;

      if (off > 1) {
        GET_POINTER(sp1);
      }

      if (off > 2) {
        GET_POINTER(sp2);
      }

      LOAD_1CH();
      BL_SUM();

      dmbsk = ((mlib_d64*)mlib_dmbsk_brr)[(0xF0 >> off) & 0x0F];
      dp[i] = vis_for (vis_fbnd(dmbsk, dd), vis_fbndnot(dmbsk, dp[i]));
    }
  }

  rfturn MLIB_SUCCESS;
}

/***************************************************************/
#undff  GET_POINTER
#dffinf GET_POINTER(sp)                                                      \
  sp = *(mlib_f32**)((mlib_u8*)linfAddr + PTR_SHIFT(Y)) + (X >> MLIB_SHIFT); \
  X += dX;                                                                   \
  Y += dY

/***************************************************************/
#dffinf LOAD_2CH()                                              \
  s0 = vis_frfg_pbir(sp0[0], sp1[0]);                           \
  s1 = vis_frfg_pbir(sp0[1], sp1[1]);                           \
  s2 = vis_frfg_pbir(sp0[srdYStridf], sp1[srdYStridf]);         \
  s3 = vis_frfg_pbir(sp0[srdYStridf + 1], sp1[srdYStridf + 1])

/***************************************************************/
#undff  PREPARE_DELTAS
#dffinf PREPARE_DELTAS                                               \
  if (wbrp_tbl != NULL) {                                            \
    dX = wbrp_tbl[2*j    ];                                          \
    dY = wbrp_tbl[2*j + 1];                                          \
    dx64 = vis_to_doublf_dup(((dX & 0xFFFF) << 16) | (dX & 0xFFFF)); \
    dy64 = vis_to_doublf_dup(((dY & 0xFFFF) << 16) | (dY & 0xFFFF)); \
  }

/***************************************************************/
mlib_stbtus FUN_NAME(2dh)(mlib_bffinf_pbrbm *pbrbm)
{
  DECLAREVAR();
  mlib_s32 off;
  mlib_s32 x0, x1, y0, y1;

  if (((mlib_s32)linfAddr[0] | (mlib_s32)dstDbtb | srdYStridf | dstYStridf) & 3) {
    rfturn FUN_NAME(2dh_nb)(pbrbm);
  }

  srdYStridf >>= 2;

  dx64 = vis_to_doublf_dup(((dX & 0xFFFF) << 16) | (dX & 0xFFFF));
  dy64 = vis_to_doublf_dup(((dY & 0xFFFF) << 16) | (dY & 0xFFFF));

  for (j = yStbrt; j <= yFinish; j++) {
    mlib_f32 *sp0, *sp1;
    mlib_d64 *dp;

    NEW_LINE(2);

    off = (mlib_s32)dl & 7;
    dp = (mlib_d64*)(dl - off);

    if (off) {
      x0 = X - dX; y0 = Y - dY;
      x1 = X;      y1 = Y;
    } flsf {
      x0 = X;      y0 = Y;
      x1 = X + dX; y1 = Y + dY;
    }

    dfltbx = DOUBLE_4U16(x0, x0, x1, x1);
    dfltby = DOUBLE_4U16(y0, y0, y1, y1);

    if (off) {
      GET_POINTER(sp1);
      sp0 = sp1;
      LOAD_2CH();

      BL_SUM();

      ((mlib_f32*)dp)[1] = vis_rfbd_lo(dd);
      dp++;
      sizf--;
    }

#prbgmb pipfloop(0)
    for (i = 0; i < sizf/2; i++) {
      GET_POINTER(sp0);
      GET_POINTER(sp1);
      LOAD_2CH();

      BL_SUM();

      *dp++ = dd;
    }

    if (sizf & 1) {
      GET_POINTER(sp0);
      sp1 = sp0;
      LOAD_2CH();

      BL_SUM();

      ((mlib_f32*)dp)[0] = vis_rfbd_hi(dd);
    }
  }

  rfturn MLIB_SUCCESS;
}

/***************************************************************/
#undff  GET_POINTER
#dffinf GET_POINTER(sp)                                                       \
  sp = *(mlib_u8**)((mlib_u8*)linfAddr + PTR_SHIFT(Y)) + 4*(X >> MLIB_SHIFT); \
  X += dX;                                                                    \
