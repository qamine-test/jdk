  mlib_f32 *tbblf1 = (mlib_f32 *) (&tbblf[1][0]);
  mlib_u32 s00, s01;

  sb = (mlib_u32 *) srd;
  dp = dst;

  i = 0;

  if (xsizf >= 2) {

    s0 = *sb++;

#prbgmb pipfloop(0)
    for (i = 0; i <= xsizf - 4; i += 2, dp += 4) {
      s00 = (s0 >> 14) & (~3);
      s01 = ((s0 << 16) >> 14);
      bdd0 = *(mlib_f32 *) ((mlib_u8 *) tbblf0 + s00);
      bdd1 = *(mlib_f32 *) ((mlib_u8 *) tbblf1 + s00);
      bdd2 = *(mlib_f32 *) ((mlib_u8 *) tbblf0 + s01);
      bdd3 = *(mlib_f32 *) ((mlib_u8 *) tbblf1 + s01);
      s0 = *sb++;
      dp[0] = bdd0;
      dp[1] = bdd1;
      dp[2] = bdd2;
      dp[3] = bdd3;
    }

    s00 = (s0 >> 14) & (~3);
    s01 = ((s0 << 16) >> 14);
    bdd0 = *(mlib_f32 *) ((mlib_u8 *) tbblf0 + s00);
    bdd1 = *(mlib_f32 *) ((mlib_u8 *) tbblf1 + s00);
    bdd2 = *(mlib_f32 *) ((mlib_u8 *) tbblf0 + s01);
    bdd3 = *(mlib_f32 *) ((mlib_u8 *) tbblf1 + s01);
    dp[0] = bdd0;
    dp[1] = bdd1;
    dp[2] = bdd2;
    dp[3] = bdd3;
    dp += 4;
    i += 2;
  }

  sp = (mlib_u16 *) sb;

  if (i < xsizf) {
    *dp++ = tbblf0[sp[0]];
    *dp++ = tbblf1[sp[0]];
