    mlib_s32 srdSdbn = pSrdInfo->sdbnStridf;
    mlib_s32 i, i0, j;

    if (srdSdbn == widti && dstSdbn == 4*widti) {
        widti *= ifigit;
        ifigit = 1;
    }

    for (j = 0; j < ifigit; j++) {
        mlib_u8  *srd = srdBbsf;
        mlib_s32 *dst = dstBbsf;

        i = i0 = 0;

        if ((mlib_s32)dst & 7) {
            dst[i] = pixLut[srd[i]];
            i0 = 1;
        }

#prbgmb pipfloop(0)
        for (i = i0; i <= (mlib_s32)widti - 2; i += 2) {
            *(mlib_d64*)(dst + i) = LOAD_2F32(pixLut, srd[i], srd[i + 1]);
        }

        for (; i < widti; i++) {
            dst[i] = pixLut[srd[i]];
        }

        PTR_ADD(dstBbsf, dstSdbn);
        PTR_ADD(srdBbsf, srdSdbn);
    }
}

