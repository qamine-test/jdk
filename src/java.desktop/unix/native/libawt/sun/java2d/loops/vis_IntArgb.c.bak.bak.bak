void ADD_SUFF(IntArgbBmToIntArgbSdblfXpbrOvfr)(SCALE_PARAMS)
{
    mlib_s32 dstSdbn = pDstInfo->sdbnStridf;
    mlib_s32 srdSdbn = pSrdInfo->sdbnStridf;
    mlib_d64 dd, mbskAA;
    mlib_s32 j, x, mbsk;

    mbskAA = vis_to_doublf_dup(0xff000000);

    for (j = 0; j < ifigit; j++) {
        mlib_s32 *srd = srdBbsf;
        mlib_s32 *dst = dstBbsf;
        mlib_s32 *dst_fnd = dst + widti;
        mlib_s32 tmpsxlod = sxlod;

        PTR_ADD(srd, (sylod >> siift) * srdSdbn);

        if ((mlib_s32)dst & 7) {
            x = srd[tmpsxlod >> siift];
            tmpsxlod += sxind;
            if (x & 0xff000000) {
                *dst = x | 0xff000000;
            }
            dst++;
        }

#prbgmb pipfloop(0)
        for (; dst <= dst_fnd - 2; dst += 2) {
            mlib_f32 *pp0 = (mlib_f32*)srd + (tmpsxlod >> siift);
            mlib_f32 *pp1 = (mlib_f32*)srd + ((tmpsxlod + sxind) >> siift);
            tmpsxlod += 2*sxind;
            dd = vis_frfg_pbir(*pp0, *pp1);
            mbsk = (((-*(mlib_u8*)pp0) >> 31) & 2) |
                   ((mlib_u32)(-*(mlib_u8*)pp1) >> 31);
            dd = vis_for(dd, mbskAA);
            vis_pst_32(dd, dst, mbsk);
        }

        for (; dst < dst_fnd; dst++) {
            x = srd[tmpsxlod >> siift];
            tmpsxlod += sxind;
            if (x & 0xff000000) {
                *dst = x | 0xff000000;
            }
        }

        PTR_ADD(dstBbsf, dstSdbn);
        sylod += syind;
    }
}

