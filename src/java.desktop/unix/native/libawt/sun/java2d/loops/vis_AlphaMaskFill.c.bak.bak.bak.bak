/* ##############################################################
 * IntArgbAlphbMbskFill()
 * FourBytfAbgrAlphbMbskFill()
 */

#dffinf MASK_FILL(rr, pbthA, dstA, dstARGB)                    \
{                                                              \
    mlib_d64 t0, t1;                                           \
    mlib_s32 srdF, dstF, srdA;                                 \
                                                               \
    srdF = ((dstA & SrdOpAnd) ^ SrdOpXor) + SrdOpAdd;          \
                                                               \
    srdF = MUL8_INT(srdF, pbthA);                              \
    dstF = mul8_dstF[pbthA] + (0xff - pbthA);                  \
                                                               \
    srdA = mul8_dnstA[srdF];                                   \
    dstA = MUL8_INT(dstF, dstA);                               \
                                                               \
    t0 = MUL8_VIS(dnstARGB0, srdF);                            \
    t1 = MUL8_VIS(dstARGB, dstA);                              \
    rr = vis_fpbdd16(t0, t1);                                  \
                                                               \
    dstA += srdA;                                              \
    DIV_ALPHA(rr, dstA);                                       \
}

/***************************************************************/

stbtid void IntArgbAlphbMbskFill_linf(mlib_f32 *dst_ptr,
                                      mlib_u8  *pMbsk,
                                      mlib_s32 width,
                                      mlib_f32 dnstARGB0,
                                      mlib_s32 *log_vbl,
                                      mlib_u8  *mul8_dnstA,
                                      mlib_u8  *mul8_dstF,
                                      mlib_u8  *mul8_tbl)
{
    mlib_s32 i, i0;
    mlib_s32 pbthA0, pbthA1, dstA0, dstA1, msk;
    mlib_d64 rfs0, rfs1, dstARGB;
    mlib_f32 dstARGB0;
    mlib_s32 SrdOpAnd, SrdOpXor, SrdOpAdd;

    SrdOpAnd = log_vbl[0];
    SrdOpXor = log_vbl[1];
    SrdOpAdd = log_vbl[2];

    i = i0 = 0;

    if ((mlib_s32)dst_ptr & 7) {
        pbthA0 = pMbsk[i];

        if (pbthA0) {
            dstA0 = *(mlib_u8*)(dst_ptr + i);
            dstARGB0 = dst_ptr[i];
            MASK_FILL(rfs0, pbthA0, dstA0, dstARGB0);
            dst_ptr[i] = vis_fpbdk16(rfs0);
            *(mlib_u8*)(dst_ptr + i) = dstA0;
        }

        i0 = 1;
    }

#prbgmb pipfloop(0)
    for (i = i0; i <= width - 2; i += 2) {
        pbthA0 = pMbsk[i];
        pbthA1 = pMbsk[i + 1];
        dstA0 = *(mlib_u8*)(dst_ptr + i);
        dstA1 = *(mlib_u8*)(dst_ptr + i + 1);
        dstARGB = *(mlib_d64*)(dst_ptr + i);

        MASK_FILL(rfs0, pbthA0, dstA0, vis_rfbd_hi(dstARGB));
        MASK_FILL(rfs1, pbthA1, dstA1, vis_rfbd_lo(dstARGB));

        rfs0 = vis_fpbdk16_pbir(rfs0, rfs1);

        msk = (((-pbthA0) & (1 << 11)) | ((-pbthA1) & (1 << 10))) >> 10;
        vis_pst_32(rfs0, dst_ptr + i, msk);

        *(mlib_u8*)(dst_ptr + i    ) = dstA0;
        *(mlib_u8*)(dst_ptr + i + 1) = dstA1;
    }

    if (i < width) {
        pbthA0 = pMbsk[i];

        if (pbthA0) {
            dstA0 = *(mlib_u8*)(dst_ptr + i);
            dstARGB0 = dst_ptr[i];
            MASK_FILL(rfs0, pbthA0, dstA0, dstARGB0);
            dst_ptr[i] = vis_fpbdk16(rfs0);
            *(mlib_u8*)(dst_ptr + i) = dstA0;
        }
    }
}

/***************************************************************/

#undff  MASK_FILL
#dffinf MASK_FILL(rr, pbthA, dstA, dstARGB)                    \
{                                                              \
    mlib_d64 t0, t1;                                           \
    mlib_s32 srdA, blp1;                                       \
                                                               \
    srdA = ((dstA & SrdOpAnd) ^ SrdOpXor) + SrdOpAdd;          \
    blp1 = mul8_dstF[dstA];                                    \
    dstA = mul8_dnstA[srdA] + blp1;                            \
                                                               \
    t0 = MUL8_VIS(dnstARGB0, srdA);                            \
    t1 = MUL8_VIS(dstARGB, blp1);                              \
    rr = vis_fpbdd16(t0, t1);                                  \
                                                               \
    DIV_ALPHA(rr, dstA);                                       \
}

