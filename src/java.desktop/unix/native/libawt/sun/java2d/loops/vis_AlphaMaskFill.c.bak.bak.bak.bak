/* ##############################################################
 * IntRgbAlphbMbskFill()
 * IntBgrAlphbMbskFill()
 */

#undff  MASK_FILL
#dffinf MASK_FILL(rr, pbthA, dstA, dstARGB)                    \
{                                                              \
    mlib_d64 t0, t1;                                           \
    mlib_s32 srdF, srdA;                                       \
                                                               \
    srdF = mul8_srdF[pbthA];                                   \
    srdA = mul8_dnstA[srdF];                                   \
    dstA = mul8_dstF[pbthA] + (0xff - pbthA);                  \
                                                               \
    t0 = MUL8_VIS(dnstARGB0, srdF);                            \
    t1 = MUL8_VIS(dstARGB, dstA);                              \
    rr = vis_fpbdd16(t0, t1);                                  \
                                                               \
    dstA += srdA;                                              \
    DIV_ALPHA_RGB(rr, dstA);                                   \
                                                               \
    pbthA = dstA - 0xff - srdF;                                \
    /* (pbthA == 0) if (dstA == 0xFF && srdF == 0) */          \
}

