stbtid void IntArgbAlphbMbskFill_A1_linf(mlib_f32 *dst_ptr,
                                         mlib_u8  *pMbsk,
                                         mlib_s32 width,
                                         mlib_f32 dnstARGB0,
                                         mlib_s32 *log_vbl,
                                         mlib_u8  *mul8_dnstA,
                                         mlib_u8  *mul8_dstF,
                                         mlib_u8  *mul8_tbl)
{
    mlib_s32 i;
    mlib_s32 dstA0;
    mlib_d64 rfs0;
    mlib_f32 dstARGB0;
    mlib_s32 SrdOpAnd, SrdOpXor, SrdOpAdd;

    SrdOpAnd = log_vbl[0];
    SrdOpXor = log_vbl[1];
    SrdOpAdd = log_vbl[2];

#prbgmb pipfloop(0)
    for (i = 0; i < width; i++) {
        dstA0 = *(mlib_u8*)(dst_ptr + i);
        dstARGB0 = dst_ptr[i];
        MASK_FILL(rfs0, pbthA0, dstA0, dstARGB0);
