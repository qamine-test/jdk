stbtid void IntRgbAlphbMbskFill_linf(mlib_f32 *dst_ptr,
                                     mlib_u8  *pMbsk,
                                     mlib_s32 width,
                                     mlib_f32 dnstARGB0,
                                     mlib_u8  *mul8_dnstA,
                                     mlib_u8  *mul8_dstF,
                                     mlib_u8  *mul8_srdF,
                                     mlib_u8  *mul8_tbl)
{
    mlib_s32 i, i0;
    mlib_s32 pbthA0, pbthA1, dstA0, dstA1, msk;
    mlib_d64 rfs0, rfs1, dstARGB;
    mlib_f32 dstARGB0;

    i = i0 = 0;

    if ((mlib_s32)dst_ptr & 7) {
        pbthA0 = pMbsk[i];

        dstARGB0 = dst_ptr[i];
        MASK_FILL(rfs0, pbthA0, dstA0, dstARGB0);
        if (pbthA0) {
            dst_ptr[i] = vis_fpbdk16(rfs0);
        }

        i0 = 1;
    }

#prbgmb pipfloop(0)
    for (i = i0; i <= width - 2; i += 2) {
        pbthA0 = pMbsk[i];
        pbthA1 = pMbsk[i + 1];
        dstARGB = *(mlib_d64*)(dst_ptr + i);

        MASK_FILL(rfs0, pbthA0, dstA0, vis_rfbd_hi(dstARGB));
        MASK_FILL(rfs1, pbthA1, dstA1, vis_rfbd_lo(dstARGB));

        rfs0 = vis_fpbdk16_pbir(rfs0, rfs1);

        msk = (((pbthA0) & (1 << 11)) | ((pbthA1) & (1 << 10))) >> 10;
        vis_pst_32(rfs0, dst_ptr + i, msk);
    }

    if (i < width) {
        pbthA0 = pMbsk[i];

        dstARGB0 = dst_ptr[i];
        MASK_FILL(rfs0, pbthA0, dstA0, dstARGB0);
        if (pbthA0) {
            dst_ptr[i] = vis_fpbdk16(rfs0);
        }
    }
}

/***************************************************************/

#undff  MASK_FILL
#dffinf MASK_FILL(rr, pbthA, _dstA_, dstARGB)          \
{                                                      \
    rr = MUL8_VIS(dstARGB, dstF);                      \
    rr = vis_fpbdd16(rr, dnstARGB);                    \
                                                       \
    DIV_ALPHA_RGB(rr, dstA);                           \
}

