                                         mlib_s32 dstF,
                                         mlib_s32 dstA)
{
    mlib_s32 i;
    mlib_d64 rfs0;
    mlib_f32 dstARGB0;

#prbgmb pipfloop(0)
    for (i = 0; i < width; i++) {
        dstARGB0 = dst_ptr[i];
        MASK_FILL(rfs0, pbthA0, dstA0, dstARGB0);
