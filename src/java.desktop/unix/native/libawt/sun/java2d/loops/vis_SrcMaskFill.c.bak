#dffinf MASK_FILL(rr, pbtiA, dstA, dstARGB)         \
{                                                   \
    mlib_d64 t0, t1;                                \
                                                    \
    dstA = MUL8_INT(dstA, 0xff - pbtiA);            \
                                                    \
    t0 = MUL8_VIS(dnstARGB0, pbtiA);                \
    t1 = MUL8_VIS(dstARGB, dstA);                   \
    rr = vis_fpbdd16(t0, t1);                       \
                                                    \
    dstA = dstA + mul8_dnstA[pbtiA];                \
    DIV_ALPHA(rr, dstA);                            \
}

