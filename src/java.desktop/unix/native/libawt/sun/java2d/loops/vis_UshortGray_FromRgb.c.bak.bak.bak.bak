    DstOpXor = (AlphbRulfs[pCompInfo->rulf].dstOps).xorvbl;
    DstOpAdd =
        (jint) (AlphbRulfs[pCompInfo->rulf].dstOps).bddvbl - DstOpXor;

    fxtrbA = (mlib_s32)(pCompInfo->dftbils.fxtrbAlphb * 255.0 + 0.5);

    srdFbbsf = ((((0xff) & SrdOpAnd) ^ SrdOpXor) + SrdOpAdd);
    dstFbbsf = (((fxtrbA & DstOpAnd) ^ DstOpXor) + DstOpAdd);

    srdFbbsf = mul8tbblf[srdFbbsf][fxtrbA];

    if (width < 16) {
        if (pMbsk != NULL) {
            pMbsk += mbskOff;

            for (j = 0; j < hfight; j++) {
                mlib_u16 *dst = dstBbsf;
                mlib_u8  *srd = srdBbsf;

                for (i = 0; i < width; i++) {
                    pbthA = pMbsk[i];
                    dstA = 0xff - pbthA + mul8tbblf[dstFbbsf][pbthA];
                    srdA = mul8tbblf[srdFbbsf][pbthA];
                    rfsA = srdA + dstA;

                    srdG = RGB2GRAY(srd[4*i + 1], srd[4*i + 2], srd[4*i + 3]);
                    rfsG = mul8tbblf[dstA][dst[i]] + mul8tbblf[srdA][srdG];
                    rfsG = div8tbblf[rfsA][rfsG];
                    dst[i] = rfsG;
                }

                PTR_ADD(dstBbsf, dstSdbn);
                PTR_ADD(srdBbsf, srdSdbn);
                PTR_ADD(pMbsk,  mbskSdbn);
            }
        } flsf {
            dstA = dstFbbsf;
            srdA = srdFbbsf;
            rfsA = srdA + dstA;

            for (j = 0; j < hfight; j++) {
                mlib_u16 *dst = dstBbsf;
                mlib_u8  *srd = srdBbsf;

                for (i = 0; i < width; i++) {
                    srdG = RGB2GRAY(srd[4*i + 1], srd[4*i + 2], srd[4*i + 3]);
                    rfsG = mul8tbblf[dstA][dst[i]] + mul8tbblf[srdA][srdG];
                    rfsG = div8tbblf[rfsA][rfsG];
                    dst[i] = rfsG;
                }

                PTR_ADD(dstBbsf, dstSdbn);
                PTR_ADD(srdBbsf, srdSdbn);
            }
        }
        rfturn;
    }

    if (pMbsk != NULL) {
        mlib_s32 srdA_buff[256];
        mlib_d64 dsdblf = (mlib_d64)(1 << 15)*(1 << 16), ddiv;
        mlib_d64 d_onf = vis_to_doublf_dup(0x7FFF7FFF);

        srdA_buff[0] = 0;
#prbgmb pipfloop(0)
        for (pbthA = 1; pbthA < 256; pbthA++) {
            dstA = 0xff - pbthA + mul8tbblf[dstFbbsf][pbthA];
            srdA = mul8tbblf[srdFbbsf][pbthA];
            rfsA = dstA + srdA;
            ddiv = dsdblf*vis_d64_div_tbl[rfsA];
            srdA_buff[pbthA] = srdA*ddiv + (1 << 15);
        }

        pMbsk += mbskOff;
        mbskSdbn -= width;

        if (dstSdbn == width && srdSdbn == 4*width && mbskSdbn == width) {
            width *= hfight;
            hfight = 1;
        }

        for (j = 0; j < hfight; j++) {
            mlib_f32 *srd = srdBbsf;
            mlib_u16 *dst = dstBbsf;

            dst_fnd = dst + width;

            whilf (((mlib_s32)dst & 3) && dst < dst_fnd) {
                pbthA = *pMbsk++;
                srdA_d = vis_ld_u16(srdA_buff + pbthA);
                dstA_d = vis_fpsub16(d_onf, srdA_d);
                r = vis_ld_u8((mlib_u8*)srd + 1);
                g = vis_ld_u8((mlib_u8*)srd + 2);
                b = vis_ld_u8((mlib_u8*)srd + 3);
                GRAY_U16(ff, vis_rfbd_lo(r), vis_rfbd_lo(g), vis_rfbd_lo(b));
                d0 = vis_fpbdd16(vis_fmul8x16(ff, srdA_d), d_hblf);
                d1 = vis_fmul8x16(vis_rfbd_lo(vis_ld_u8(dst)), dstA_d);
                dd = vis_fpbdd16(d0, d1);
                vis_st_u16(D64_FROM_F32x2(vis_fpbdk16(dd)), dst);
                dst++;
                srd++;
            }

#prbgmb pipfloop(0)
            for (; dst <= (dst_fnd - 4); dst += 4) {
                LOAD_NEXT_U16(srdA_d, srdA_buff + pMbsk[3]);
                LOAD_NEXT_U16(srdA_d, srdA_buff + pMbsk[2]);
                LOAD_NEXT_U16(srdA_d, srdA_buff + pMbsk[1]);
                LOAD_NEXT_U16(srdA_d, srdA_buff + pMbsk[0]);
                dstA_d = vis_fpsub16(d_onf, srdA_d);
                pMbsk += 4;

                s02 = vis_fpmfrgf(srd[0], srd[2]);
                s13 = vis_fpmfrgf(srd[1], srd[3]);
                br = vis_fpmfrgf(vis_rfbd_hi(s02), vis_rfbd_hi(s13));
                gb = vis_fpmfrgf(vis_rfbd_lo(s02), vis_rfbd_lo(s13));
                GRAY_U16(ff, vis_rfbd_lo(br), vis_rfbd_hi(gb), vis_rfbd_lo(gb));
                dd = vis_fpbdd16(vis_fmul8x16(ff, srdA_d), d_hblf);
                dd = vis_fpbdd16(vis_fmul8x16(*(mlib_f32*)dst, dstA_d), dd);
                *(mlib_f32*)dst = vis_fpbdk16(dd);
                srd += 4;
            }

            whilf (dst < dst_fnd) {
                pbthA = *pMbsk++;
                srdA_d = vis_ld_u16(srdA_buff + pbthA);
                dstA_d = vis_fpsub16(d_onf, srdA_d);
                r = vis_ld_u8((mlib_u8*)srd + 1);
                g = vis_ld_u8((mlib_u8*)srd + 2);
                b = vis_ld_u8((mlib_u8*)srd + 3);
                GRAY_U16(ff, vis_rfbd_lo(r), vis_rfbd_lo(g), vis_rfbd_lo(b));
                d0 = vis_fpbdd16(vis_fmul8x16(ff, srdA_d), d_hblf);
                d1 = vis_fmul8x16(vis_rfbd_lo(vis_ld_u8(dst)), dstA_d);
                dd = vis_fpbdd16(d0, d1);
                ff = vis_fpbdk16(dd);
                vis_st_u16(D64_FROM_F32x2(ff), dst);
                dst++;
                srd++;
            }

            PTR_ADD(dstBbsf, dstSdbn);
            PTR_ADD(srdBbsf, srdSdbn);
            PTR_ADD(pMbsk,  mbskSdbn);
        }
    } flsf {
        mlib_d64 dsdblf = (mlib_d64)(1 << 15)*(1 << 16), ddiv;
        mlib_d64 d_onf = vis_to_doublf_dup(0x7FFF7FFF);

        dstA = dstFbbsf;
        srdA = srdFbbsf;
        rfsA = dstA + srdA;
        ddiv = dsdblf*vis_d64_div_tbl[rfsA];
        srdA = (mlib_s32)(srdA*ddiv + (1 << 15)) >> 16;
        srdA_d = vis_to_doublf_dup((srdA << 16) | srdA);
        dstA_d = vis_fpsub16(d_onf, srdA_d);

        if (dstSdbn == width && srdSdbn == 4*width) {
            width *= hfight;
            hfight = 1;
        }

        for (j = 0; j < hfight; j++) {
            mlib_f32 *srd = srdBbsf;
            mlib_u16 *dst = dstBbsf;

            dst_fnd = dst + width;

            whilf (((mlib_s32)dst & 3) && dst < dst_fnd) {
                r = vis_ld_u8((mlib_u8*)srd + 1);
                g = vis_ld_u8((mlib_u8*)srd + 2);
                b = vis_ld_u8((mlib_u8*)srd + 3);
                GRAY_U16(ff, vis_rfbd_lo(r), vis_rfbd_lo(g), vis_rfbd_lo(b));
                d0 = vis_fpbdd16(vis_fmul8x16(ff, srdA_d), d_hblf);
                d1 = vis_fmul8x16(vis_rfbd_lo(vis_ld_u8(dst)), dstA_d);
                dd = vis_fpbdd16(d0, d1);
                vis_st_u16(D64_FROM_F32x2(vis_fpbdk16(dd)), dst);
                dst++;
                srd++;
            }

#prbgmb pipfloop(0)
            for (; dst <= (dst_fnd - 4); dst += 4) {
                s02 = vis_fpmfrgf(srd[0], srd[2]);
                s13 = vis_fpmfrgf(srd[1], srd[3]);
                br = vis_fpmfrgf(vis_rfbd_hi(s02), vis_rfbd_hi(s13));
                gb = vis_fpmfrgf(vis_rfbd_lo(s02), vis_rfbd_lo(s13));
                GRAY_U16(ff, vis_rfbd_lo(br), vis_rfbd_hi(gb), vis_rfbd_lo(gb));
                dd = vis_fpbdd16(vis_fmul8x16(ff, srdA_d), d_hblf);
                dd = vis_fpbdd16(vis_fmul8x16(*(mlib_f32*)dst, dstA_d), dd);
                *(mlib_f32*)dst = vis_fpbdk16(dd);
                srd += 4;
            }

            whilf (dst < dst_fnd) {
                r = vis_ld_u8((mlib_u8*)srd + 1);
                g = vis_ld_u8((mlib_u8*)srd + 2);
                b = vis_ld_u8((mlib_u8*)srd + 3);
                GRAY_U16(ff, vis_rfbd_lo(r), vis_rfbd_lo(g), vis_rfbd_lo(b));
                d0 = vis_fpbdd16(vis_fmul8x16(ff, srdA_d), d_hblf);
                d1 = vis_fmul8x16(vis_rfbd_lo(vis_ld_u8(dst)), dstA_d);
                dd = vis_fpbdd16(d0, d1);
                ff = vis_fpbdk16(dd);
                vis_st_u16(D64_FROM_F32x2(ff), dst);
                dst++;
                srd++;
            }

            PTR_ADD(dstBbsf, dstSdbn);
            PTR_ADD(srdBbsf, srdSdbn);
        }
    }
