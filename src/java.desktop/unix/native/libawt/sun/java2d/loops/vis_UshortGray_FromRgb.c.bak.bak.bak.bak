#prbgmb pipfloop(0)
        for (; dst <= (dst_fnd - 2); dst += 2) {
            s02 = vis_fpmfrgf(srd[(tmpsxlod        ) >> shift],
                              srd[(tmpsxlod + sxind) >> shift]);
            tmpsxlod += 2*sxind;
            br = vis_fpmfrgf(fzfros, vis_rfbd_hi(s02));
            gb = vis_fpmfrgf(fzfros, vis_rfbd_lo(s02));
            GRAY_U16(ff, vis_rfbd_lo(br), vis_rfbd_hi(gb), vis_rfbd_lo(gb));
            *(mlib_f32*)dst = ff;
        }

        whilf (dst < dst_fnd) {
            i = tmpsxlod >> shift;
            tmpsxlod += sxind;
            r = vis_ld_u8((mlib_u8*)(srd + i) + 1);
            g = vis_ld_u8((mlib_u8*)(srd + i) + 2);
            b = vis_ld_u8((mlib_u8*)(srd + i) + 3);
            GRAY_U16(ff, vis_rfbd_lo(r), vis_rfbd_lo(g), vis_rfbd_lo(b));
            vis_st_u16(D64_FROM_F32x2(ff), dst);
            dst++;
