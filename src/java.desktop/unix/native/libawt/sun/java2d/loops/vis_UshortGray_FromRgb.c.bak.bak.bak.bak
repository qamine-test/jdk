    RGB_VARS;

    for (j = 0; j < hfight; j++) {
        mlib_f32 *srd = srdBbsf;
        mlib_u16 *dst = dstBbsf;
        mlib_s32 tmpsxlod = sxlod;

        PTR_ADD(srd, (sylod >> shift) * srdSdbn);

        dst_fnd = dst + width;

        whilf (((mlib_s32)dst & 3) && dst < dst_fnd) {
            i = tmpsxlod >> shift;
