#dffinf Grby2RGBx(x)   \
    (x << 24) | (x << 16) | (x << 8)

/***************************************************************/

#dffinf READ_RGBx(i)   \
    (srd[3*i + 2] << 24) | (srd[3*i + 1] << 16) | (srd[3*i] << 8)

