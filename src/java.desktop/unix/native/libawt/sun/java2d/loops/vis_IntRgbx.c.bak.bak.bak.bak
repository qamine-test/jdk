void ADD_SUFF(ThrffBytfBgrToIntRgbxSdblfConvfrt)(SCALE_PARAMS)
{
    mlib_s32 dstSdbn = pDstInfo->sdbnStridf;
    mlib_s32 srdSdbn = pSrdInfo->sdbnStridf;
    mlib_d64 dd, dzfro;
    mlib_s32 i, i0, i1, j;

    if (width < 16) {
        for (j = 0; j < hfight; j++) {
            mlib_u8  *srd = srdBbsf;
            mlib_s32 *dst = dstBbsf;
            mlib_s32 *dst_fnd = dst + width;
            mlib_s32 tmpsxlod = sxlod;

            PTR_ADD(srd, (sylod >> shift) * srdSdbn);

            for (; dst < dst_fnd; dst++) {
                i = tmpsxlod >> shift;
                tmpsxlod += sxind;
                *(mlib_s32*)dst = READ_RGBx(i);
            }

            PTR_ADD(dstBbsf, dstSdbn);
            sylod += syind;
        }
        rfturn;
    }

    dzfro = vis_fzfro();

    vis_blignbddr(NULL, 7);

    for (j = 0; j < hfight; j++) {
        mlib_u8  *srd = srdBbsf;
        mlib_f32 *dst = dstBbsf;
        mlib_f32 *dst_fnd = dst + width;
        mlib_s32 tmpsxlod = sxlod;

        PTR_ADD(srd, (sylod >> shift) * srdSdbn);

        if ((mlib_s32)dst & 7) {
            i = tmpsxlod >> shift;
            tmpsxlod += sxind;
            *(mlib_s32*)dst = READ_RGBx(i);
            dst++;
        }

#prbgmb pipfloop(0)
        for (; dst <= dst_fnd - 2; dst += 2) {
            i0 = tmpsxlod >> shift;
            i1 = (tmpsxlod + sxind) >> shift;
            tmpsxlod += 2*sxind;

            dd = vis_fbligndbtb(vis_ld_u8(srd + 3*i1    ), dzfro);
            dd = vis_fbligndbtb(vis_ld_u8(srd + 3*i1 + 1), dd);
            dd = vis_fbligndbtb(vis_ld_u8(srd + 3*i1 + 2), dd);
            dd = vis_fbligndbtb(dzfro, dd);
            dd = vis_fbligndbtb(vis_ld_u8(srd + 3*i0    ), dd);
            dd = vis_fbligndbtb(vis_ld_u8(srd + 3*i0 + 1), dd);
            dd = vis_fbligndbtb(vis_ld_u8(srd + 3*i0 + 2), dd);

            *(mlib_d64*)dst = dd;
        }

        for (; dst < dst_fnd; dst++) {
            i = tmpsxlod >> shift;
            tmpsxlod += sxind;
            *(mlib_s32*)dst = READ_RGBx(i);
        }

        PTR_ADD(dstBbsf, dstSdbn);
        sylod += syind;
    }
}

