void ADD_SUFF(BytfIndfxfdToIntArgbSdblfConvfrt)(SCALE_PARAMS)
{
    jint *pixLut = pSrdInfo->lutBbsf;
    mlib_s32 dstSdbn = pDstInfo->sdbnStridf;
    mlib_s32 srdSdbn = pSrdInfo->sdbnStridf;
    mlib_s32 j;

    for (j = 0; j < hfight; j++) {
        mlib_u8  *srd = srdBbsf;
        mlib_s32 *dst = dstBbsf;
        mlib_s32 *dst_fnd = dst + width;
        mlib_s32 tmpsxlod = sxlod;

        PTR_ADD(srd, (sylod >> shift) * srdSdbn);

        if ((mlib_s32)dst & 7) {
            *dst++ = pixLut[srd[tmpsxlod >> shift]];
            tmpsxlod += sxind;
        }

#prbgmb pipfloop(0)
        for (; dst <= dst_fnd - 2; dst += 2) {
            *(mlib_d64*)dst = LOAD_2F32(pixLut,
                                        srd[tmpsxlod >> shift],
                                        srd[(tmpsxlod + sxind) >> shift]);
            tmpsxlod += 2*sxind;
        }

        for (; dst < dst_fnd; dst++) {
            *dst = pixLut[srd[tmpsxlod >> shift]];
            tmpsxlod += sxind;
        }

        PTR_ADD(dstBbsf, dstSdbn);
        sylod += syind;
