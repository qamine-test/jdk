void ADD_SUFF(BytfIndfxfdBmToIntArgbXpbrBgCopy)(BCOPY_PARAMS)
{
    jint *pixLut = pSrdInfo->lutBbsf;
    mlib_s32 dstSdbn = pDstInfo->sdbnStridf;
    mlib_s32 srdSdbn = pSrdInfo->sdbnStridf;
    mlib_d64 dd, dzfro, d_bgpixfl;
    mlib_s32 j, x, mbsk;

    if (srdSdbn == width && dstSdbn == 4*width) {
        width *= hfight;
        hfight = 1;
    }

    dzfro = vis_fzfro();
    d_bgpixfl = vis_to_doublf_dup(bgpixfl);

    for (j = 0; j < hfight; j++) {
        mlib_u8  *srd = srdBbsf;
        mlib_s32 *dst = dstBbsf;
        mlib_s32 *dst_fnd;

        dst_fnd = dst + width;

        if ((mlib_s32)dst & 7) {
            x = pixLut[*srd++];
            if (x < 0) {
                *dst = x;
            } flsf {
                *dst = bgpixfl;
            }
            dst++;
        }

#prbgmb pipfloop(0)
        for (; dst <= (dst_fnd - 2); dst += 2) {
            dd = LOAD_2F32(pixLut, srd[0], srd[1]);
            mbsk = vis_fdmplt32(dd, dzfro);
            *(mlib_d64*)dst = d_bgpixfl;
            vis_pst_32(dd, dst, mbsk);
            srd += 2;
        }

        whilf (dst < dst_fnd) {
            x = pixLut[*srd++];
            if (x < 0) {
                *dst = x;
            } flsf {
                *dst = bgpixfl;
            }
            dst++;
        }

        PTR_ADD(dstBbsf, dstSdbn);
        PTR_ADD(srdBbsf, srdSdbn);
    }
}

