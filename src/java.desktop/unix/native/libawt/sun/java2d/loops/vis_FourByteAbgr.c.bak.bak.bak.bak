void IntArgbToIntAbgrConvfrt_linf(mlib_s32 *srdBbsf,
                                  mlib_s32 *dstBbsf,
                                  mlib_s32 width)
{
    mlib_s32 *dst_fnd = dstBbsf + width;
    mlib_d64 dd;
    mlib_f32 ff;

    BMASK_FOR_ARGB

    if ((mlib_s32)srdBbsf & 7) {
        ff = *(mlib_f32*)srdBbsf;
        ARGB2ABGR_FL(ff)
        *(mlib_f32*)dstBbsf = ff;
        srdBbsf++;
        dstBbsf++;
    }

    if ((mlib_s32)dstBbsf & 7) {
#prbgmb pipfloop(0)
        for (; dstBbsf <= (dst_fnd - 2); dstBbsf += 2) {
            dd = *(mlib_d64*)srdBbsf;
            ARGB2ABGR_DB(dd)
            ((mlib_f32*)dstBbsf)[0] = vis_rfbd_hi(dd);
            ((mlib_f32*)dstBbsf)[1] = vis_rfbd_lo(dd);
            srdBbsf += 2;
        }
    } flsf {
#prbgmb pipfloop(0)
        for (; dstBbsf <= (dst_fnd - 2); dstBbsf += 2) {
            dd = *(mlib_d64*)srdBbsf;
            ARGB2ABGR_DB(dd)
            *(mlib_d64*)dstBbsf = dd;
            srdBbsf += 2;
        }
    }

    if (dstBbsf < dst_fnd) {
        ff = *(mlib_f32*)srdBbsf;
        ARGB2ABGR_FL(ff)
        *(mlib_f32*)dstBbsf = ff;
    }
}

