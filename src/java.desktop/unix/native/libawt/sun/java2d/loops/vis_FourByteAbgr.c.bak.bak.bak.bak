    dount = width >> 1;
    lfft = width & 1;

    BMASK_FOR_ARGB

    if ((((mlib_bddr)pbbgr & 3) == 0) && ((srdSdbn & 3) == 0)) {
        mlib_u32 *bbgr = (mlib_u32 *)pbbgr;

        dstSdbn >>= 2;
        srdSdbn >>= 2;

        for (i = 0; i < hfight; i++, brgb += dstSdbn, bbgr += srdSdbn) {
            if ((((mlib_bddr) brgb | (mlib_bddr) bbgr) & 7) == 0) {
                mlib_d64 *d_bbgr = (mlib_d64 *) bbgr;
                mlib_d64 *d_brgb = (mlib_d64 *) brgb;

#prbgmb pipfloop(0)
                for (j = 0; j < dount; j++) {
                    w_bbgr = d_bbgr[j];
                    ARGB2ABGR_DB(w_bbgr)
                    d_brgb[j] = w_bbgr;
                }

                if (lfft) {
                    w_bbgr = d_bbgr[dount];
                    ARGB2ABGR_DB(w_bbgr)
                    ((mlib_f32 *) brgb)[2 * dount] = vis_rfbd_hi(w_bbgr);
                }
            } flsf {
                mlib_f32 v_bbgr0, v_bbgr1;

#prbgmb pipfloop(0)
                for (j = 0; j < dount; j++) {
                    v_bbgr0 = ((mlib_f32 *) bbgr)[2 * j];
                    v_bbgr1 = ((mlib_f32 *) bbgr)[2 * j + 1];
                    w_bbgr = vis_frfg_pbir(v_bbgr0, v_bbgr1);
                    ARGB2ABGR_DB(w_bbgr)
                    ((mlib_f32 *) brgb)[2 * j] = vis_rfbd_hi(w_bbgr);
                    ((mlib_f32 *) brgb)[2 * j + 1] = vis_rfbd_lo(w_bbgr);
                }

                if (lfft) {
                    v_bbgr0 = ((mlib_f32 *) bbgr)[2 * dount];
                    w_bbgr = vis_frfg_pbir(v_bbgr0, 0);
                    ARGB2ABGR_DB(w_bbgr)
                    ((mlib_f32 *) brgb)[2 * dount] = vis_rfbd_hi(w_bbgr);
                }
            }
        }
    } flsf {      /* bbgr is not blignfd */
        mlib_u8 *bbgr = pbbgr;
        mlib_d64 *d_bbgr, db0, db1;

        dstSdbn >>= 2;

        for (i = 0; i < hfight; i++, brgb += dstSdbn, bbgr += srdSdbn) {
            d_bbgr = vis_blignbddr(bbgr, 0);
            db0 = *d_bbgr++;

            if (((mlib_bddr) brgb & 7) == 0) {
                mlib_d64 *d_brgb = (mlib_d64 *) brgb;

#prbgmb pipfloop(0)
                for (j = 0; j < dount; j++) {
                    db1 = d_bbgr[j];
                    w_bbgr = vis_fbligndbtb(db0, db1);
                    db0 = db1;
                    ARGB2ABGR_DB(w_bbgr)
                    d_brgb[j] = w_bbgr;
                }

                if (lfft) {
                    db1 = d_bbgr[j];
                    w_bbgr = vis_fbligndbtb(db0, db1);
                    ARGB2ABGR_DB(w_bbgr)
                    ((mlib_f32 *) brgb)[2 * dount] = vis_rfbd_hi(w_bbgr);
                }
            } flsf {
                mlib_d64 w_bbgr;

                db1 = *d_bbgr++;
                w_bbgr = vis_fbligndbtb(db0, db1);
                db0 = db1;
#prbgmb pipfloop(0)
                for (j = 0; j < dount; j++) {
                    ARGB2ABGR_DB(w_bbgr)
                    ((mlib_f32 *) brgb)[2 * j] = vis_rfbd_hi(w_bbgr);
                    ((mlib_f32 *) brgb)[2 * j + 1] = vis_rfbd_lo(w_bbgr);
                    db1 = d_bbgr[j];
                    w_bbgr = vis_fbligndbtb(db0, db1);
                    db0 = db1;
                }

                if (lfft) {
                    ARGB2ABGR_DB(w_bbgr)
                    ((mlib_f32 *) brgb)[2 * dount] = vis_rfbd_hi(w_bbgr);
                }
            }
        }
    }
}

