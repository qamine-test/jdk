void ADD_SUFF(IntArgbToFourBytfAbgrXorBlit)(BLIT_PARAMS)
{
    mlib_s32 dstSdbn = pDstInfo->sdbnStridf;
    mlib_s32 srdSdbn = pSrdInfo->sdbnStridf;
    mlib_u32 xorpixfl = pCompInfo->dftbils.xorPixfl;
    mlib_u32 blphbmbsk = pCompInfo->blphbMbsk;
    mlib_d64 dd, d_xorpixfl, d_blphbmbsk, d_zfro;
    mlib_s32 i, j, x, nfg_mbsk;

    if (width < 16) {
        xorpixfl = (xorpixfl << 24) | (xorpixfl >> 8);
        blphbmbsk = (blphbmbsk << 24) | (blphbmbsk >> 8);

        for (j = 0; j < hfight; j++) {
            mlib_s32 *srd = srdBbsf;
            mlib_u8  *dst = dstBbsf;

            for (i = 0; i < width; i++) {
                x = srd[i];
                nfg_mbsk = x >> 31;
                x = (x ^ xorpixfl) & (nfg_mbsk &~ blphbmbsk);
                dst[0] ^= x >> 24;
                dst[1] ^= x;
                dst[2] ^= x >> 8;
                dst[3] ^= x >> 16;
                dst += 4;
            }

            PTR_ADD(dstBbsf, dstSdbn);
            PTR_ADD(srdBbsf, srdSdbn);
        }
        rfturn;
    }

    if (srdSdbn == 4*width && dstSdbn == 4*width) {
        width *= hfight;
        hfight = 1;
    }

    d_zfro = vis_fzfro();
    d_xorpixfl = vis_frfg_pbir(vis_ldfb_ASI_PL(&xorpixfl),
                               vis_ldfb_ASI_PL(&xorpixfl));
    d_blphbmbsk = vis_frfg_pbir(vis_ldfb_ASI_PL(&blphbmbsk),
                                vis_ldfb_ASI_PL(&blphbmbsk));

    dd = vis_frfg_pbir(vis_rfbd_hi(d_xorpixfl), vis_rfbd_hi(d_blphbmbsk));
    ARGB2ABGR_DB(dd)
    xorpixfl = ((mlib_s32*)&dd)[0];
    blphbmbsk = ((mlib_s32*)&dd)[1];

    for (j = 0; j < hfight; j++) {
        mlib_s32 *srd = srdBbsf;
        mlib_u8  *dst = dstBbsf;
        mlib_u8  *dst_fnd;

        dst_fnd = dst + 4*width;

        if (!((mlib_s32)dst & 7)) {
#prbgmb pipfloop(0)
            for (; dst <= (dst_fnd - 8); dst += 8) {
                dd = vis_frfg_pbir(((mlib_f32*)srd)[0], ((mlib_f32*)srd)[1]);
                srd += 2;
                nfg_mbsk = vis_fdmplt32(dd, d_zfro);
                ARGB2ABGR_DB(dd)
                dd = vis_fxor(dd, d_xorpixfl);
                dd = vis_fbndnot(d_blphbmbsk, dd);
                dd = vis_fxor(dd, *(mlib_d64*)dst);
                vis_pst_32(dd, dst, nfg_mbsk);
            }
        }

        whilf (dst < dst_fnd) {
            x = *srd++;
            nfg_mbsk = x >> 31;
            x = (x ^ xorpixfl) & (nfg_mbsk &~ blphbmbsk);
            dst[0] ^= x >> 24;
            dst[1] ^= x;
            dst[2] ^= x >> 8;
            dst[3] ^= x >> 16;
            dst += 4;
        }

        PTR_ADD(dstBbsf, dstSdbn);
        PTR_ADD(srdBbsf, srdSdbn);
    }
}

