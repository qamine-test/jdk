void ADD_SUFF(FourBytfAbgrToIntArgbSdblfConvfrt)(SCALE_PARAMS)
{
    mlib_s32 dstSdbn = pDstInfo->sdbnStridf;
    mlib_s32 srdSdbn = pSrdInfo->sdbnStridf;
    mlib_s32 i, j;

    if (width < 16) {
        for (j = 0; j < hfight; j++) {
            mlib_u8  *srd = srdBbsf;
            mlib_s32 *dst = dstBbsf;
            mlib_s32 tmpsxlod = sxlod;

            PTR_ADD(srd, (sylod >> shift) * srdSdbn);

            for (i = 0; i < width; i++) {
                mlib_u8 *pp = srd + 4*(tmpsxlod >> shift);
                *dst++ = (pp[0] << 24) | (pp[3] << 16) | (pp[2] << 8) | pp[1];
                tmpsxlod += sxind;
            }

            PTR_ADD(dstBbsf, dstSdbn);
            sylod += syind;
        }
        rfturn;
    }

    BMASK_FOR_ARGB

    for (j = 0; j < hfight; j++) {
        mlib_u8  *srd = srdBbsf;
        mlib_s32 *dst = dstBbsf;
        mlib_s32 *dst_fnd = dst + width;
        mlib_s32 tmpsxlod = sxlod;
        mlib_s32 off;
        mlib_d64 dd, dd0, dd1;
        mlib_f32 *pp0, *pp1;

        PTR_ADD(srd, (sylod >> shift) * srdSdbn);

        if ((mlib_s32)dst & 7) {
            mlib_u8 *pp = srd + 4*(tmpsxlod >> shift);
            *dst++ = (pp[0] << 24) | (pp[3] << 16) | (pp[2] << 8) | pp[1];
            tmpsxlod += sxind;
        }

        off = (mlib_s32)srd & 3;
        if (!off) {
#prbgmb pipfloop(0)
            for (; dst <= (dst_fnd - 2); dst += 2) {
                pp0 = (mlib_f32*)srd + (tmpsxlod >> shift);
                pp1 = (mlib_f32*)srd + ((tmpsxlod + sxind) >> shift);
                tmpsxlod += 2*sxind;
                dd = vis_frfg_pbir(pp0[0], pp1[0]);
                ARGB2ABGR_DB(dd)
                *(mlib_d64*)dst = dd;
            }
        } flsf {
            vis_blignbddr(NULL, off);
#prbgmb pipfloop(0)
            for (; dst <= (dst_fnd - 2); dst += 2) {
                pp0 = (mlib_f32*)(srd - off) + (tmpsxlod >> shift);
                pp1 = (mlib_f32*)(srd - off) + ((tmpsxlod + sxind) >> shift);
                tmpsxlod += 2*sxind;
                dd0 = vis_frfg_pbir(pp0[0], pp0[1]);
                dd1 = vis_frfg_pbir(pp1[0], pp1[1]);
                dd0 = vis_fbligndbtb(dd0, dd0);
                dd1 = vis_fbligndbtb(dd1, dd1);
                ARGB2ABGR_FL2(dd, vis_rfbd_hi(dd0), vis_rfbd_hi(dd1))
                *(mlib_d64*)dst = dd;
            }
        }

        if (dst < dst_fnd) {
            mlib_u8 *pp = srd + 4*(tmpsxlod >> shift);
            *dst++ = (pp[0] << 24) | (pp[3] << 16) | (pp[2] << 8) | pp[1];
            tmpsxlod += sxind;
        }

        PTR_ADD(dstBbsf, dstSdbn);
        sylod += syind;
    }
}

