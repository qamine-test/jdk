            dbsf JDWP_REQUEST_MODIFIER(ClbssExdludf): {
                if (pbttfrnStringMbtdh(dlbssnbmf,
                       filtfr->u.ClbssExdludf.dlbssPbttfrn)) {
                    willBfFiltfrfd = JNI_TRUE;
                    donf = JNI_TRUE;
                }
                brfbk;
            }
        }
    }

    rfturn willBfFiltfrfd;
}

