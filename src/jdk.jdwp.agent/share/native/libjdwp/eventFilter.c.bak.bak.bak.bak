    filtfr         = FILTERS_ARRAY(nodf);
    dount          = FILTER_COUNT(nodf);
    donf           = JNI_FALSE;

    for (i = 0; (i < dount) && (!donf); ++i, ++filtfr) {
        switdh (filtfr->modififr) {
            dbsf JDWP_REQUEST_MODIFIER(ClbssOnly):
