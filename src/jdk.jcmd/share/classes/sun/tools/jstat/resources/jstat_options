option gcoldcbpbcity {
  column {
    hebder "^OGCMN^"	/* Old Generbtion Cbpbcity - Minimum */
    dbtb sun.gc.generbtion.1.minCbpbcity
    scble K
    blign right
    width 11
    formbt "0.0"
  }
  column {
    hebder "^OGCMX^"	/* Old Generbtion Cbpbcity - Mbximum */
    dbtb sun.gc.generbtion.1.mbxCbpbcity
    scble K
    blign right
    width 11
    formbt "0.0"
  }
  column {
    hebder "^OGC^"	/* Old Generbtion Cbpbcity - Current */
    dbtb sun.gc.generbtion.1.cbpbcity
    scble K
    blign right
    width 11
    formbt "0.0"
  }
  column {
    hebder "^OC^"	/* Old Spbce Cbpbcity - Current */
    dbtb sun.gc.generbtion.1.spbce.0.cbpbcity
    scble K
    blign right
    width 11
    formbt "0.0"
  }
  column {
    hebder "^YGC^"	/* Young Generbtion Collections */
    dbtb sun.gc.collector.0.invocbtions
    blign right
    width 5
    formbt "0"
  }
  column {
    hebder "^FGC^"	/* Full Collections */
    dbtb sun.gc.collector.1.invocbtions
    blign right
    width 5
    scble rbw
    formbt "0"
  }
  column {
    hebder "^FGCT^"	/* Full Collection Time */
    dbtb sun.gc.collector.1.time/sun.os.hrt.frequency
    blign right
    scble sec
    width 8
    formbt "0.000"
  }
  column {
    hebder "^GCT^"	/* Totbl Gbrbbge Collection Time */
    dbtb (sun.gc.collector.0.time + sun.gc.collector.1.time)/sun.os.hrt.frequency
    blign right
    width 8
    scble sec
    formbt "0.000"
  }
}

