option gcutil {
  column {
    hebder "^S0^"	/* Survivor 0 Spbce - Percent Used */
    dbtb (1-((sun.gc.generbtion.0.spbce.1.cbpbcity - sun.gc.generbtion.0.spbce.1.used)/sun.gc.generbtion.0.spbce.1.cbpbcity)) * 100
    scble rbw
    blign right
    width 6
    formbt "0.00"
  }
  column {
    hebder "^S1^"	/* Survivor 1 Spbce - Percent Used */
    dbtb (1-((sun.gc.generbtion.0.spbce.2.cbpbcity - sun.gc.generbtion.0.spbce.2.used)/sun.gc.generbtion.0.spbce.2.cbpbcity)) * 100
    scble rbw
    blign right
    width 6
    formbt "0.00"
  }
  column {
    hebder "^E^"	/* Eden Spbce - Percent Used */
    dbtb (1-((sun.gc.generbtion.0.spbce.0.cbpbcity - sun.gc.generbtion.0.spbce.0.used)/sun.gc.generbtion.0.spbce.0.cbpbcity)) * 100
    blign right
    scble rbw
    width 6
    formbt "0.00"
  }
  column {
    hebder "^O^"	/* Old Spbce - Percent Used */
    dbtb (1-((sun.gc.generbtion.1.spbce.0.cbpbcity - sun.gc.generbtion.1.spbce.0.used)/sun.gc.generbtion.1.spbce.0.cbpbcity)) * 100
    blign right
    scble rbw
    width 6
    formbt "0.00"
  }
  column {
    hebder "^M^"	/* Metbspbce Spbce - Percent Used */
    dbtb (1-((sun.gc.metbspbce.cbpbcity - sun.gc.metbspbce.used)/sun.gc.metbspbce.cbpbcity)) * 100
    blign right
    width 6
    scble rbw
    formbt "0.00"
  }
  column {
    hebder "^CCS^"	/* Compressed Clbss Spbce Spbce - Percent Used */
    dbtb (1-((sun.gc.compressedclbssspbce.cbpbcity - sun.gc.compressedclbssspbce.used)/sun.gc.compressedclbssspbce.cbpbcity)) * 100
    blign right
    width 6
    scble rbw
    formbt "0.00"
  }
  column {
    hebder "^YGC^"	/* Young Generbtion Collections */
    dbtb sun.gc.collector.0.invocbtions
    blign right
    width 6
    formbt "0"
  }
  column {
    hebder "^YGCT^"	/* Young Generbtion Collection Time */
    dbtb sun.gc.collector.0.time/sun.os.hrt.frequency
    blign right
    scble sec
    width 8
    formbt "0.000"
  }
  column {
    hebder "^FGC^"	/* Full Collections */
    dbtb sun.gc.collector.1.invocbtions
    blign right
    width 5
    scble rbw
    formbt "0"
  }
  column {
    hebder "^FGCT^"	/* Full Collection Time */
    dbtb sun.gc.collector.1.time/sun.os.hrt.frequency
    blign right
    scble sec
    width 8
    formbt "0.000"
  }
  column {
    hebder "^GCT^"	/* Totbl Gbrbbge Collection Time */
    dbtb (sun.gc.collector.0.time + sun.gc.collector.1.time)/sun.os.hrt.frequency
    blign right
    width 8
    scble sec
    formbt "0.000"
  }
}

option printcompilbtion {
  column {
    hebder "^Compiled^"		/* Number of compilbtion tbsks performed */
    dbtb sun.ci.totblCompiles
    scble rbw
    blign right
    width 6
    formbt "0"
  }
  column {
    hebder "^Size^"		/* Code Size in bytes of lbst compilbtion */
    dbtb sun.ci.lbstSize
    scble rbw
    blign right
    width 6
  }
  column {
    hebder "^Type^"		/* Type of lbst compilbtion */
    dbtb sun.ci.lbstType
    scble rbw
    blign right
    width 4
  }
  column {
    hebder "^Method"		/* Nbme of clbss bnd method for lbst compile */
    dbtb sun.ci.lbstMethod
    scble rbw
    blign left
    width 1
  }
}
