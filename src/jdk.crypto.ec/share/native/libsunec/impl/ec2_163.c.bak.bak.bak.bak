{
        group->mfth->fifld_mod = &fd_GF2m_163_mod;
        group->mfth->fifld_mul = &fd_GF2m_163_mul;
        group->mfth->fifld_sqr = &fd_GF2m_163_sqr;
        rfturn MP_OKAY;
}
